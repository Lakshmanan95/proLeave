                .subscribe(function (data) {
                _this.selectedAll = undefined;
                _this.PendingLeaveRequests = data;
                _this.spinnerService.hide();
            }, function (error) {
                console.log(error);
                if (error.status == 0) {
                    window.location.reload();
                }
            });
        }
        else {
        }
    };
    ///////////////////////// Download Excel ///////////////////////////
    ReviewComponent.prototype.downloadCSV = function () {
        this.leavefrom = this.data['leavefrom'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.data['leavefrom']).format('YYYY-MM-DD').toString() : null;
        this.leaveto = this.data['leaveto'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.data['leaveto']).format('YYYY-MM-DD').toString() : null;
