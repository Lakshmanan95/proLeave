        this.modalRef = this.modalService.show(template, { class: 'modal-sm' });
    };
    HomeComponent.prototype.confirm = function (template, success, error) {
        var _this = this;
        this.modalRef.hide();
        var urlSave = __WEBPACK_IMPORTED_MODULE_1__utility_constant__["a" /* CONSTANT */].LEAVE_REQUEST_PROCESS;
