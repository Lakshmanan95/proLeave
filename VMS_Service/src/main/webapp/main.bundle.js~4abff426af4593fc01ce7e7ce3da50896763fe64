    ////////////////////////////////////////// For Filter Search ///////////////////////////////////
    ReviewComponent.prototype.onEmployeeChanged = function (selectedName) {
        var model = selectedName.toLowerCase();
        var temp = [];
        this.reporteeLeaveListRequest.map(function (obj) {
            obj.employeeName = obj.employeeName.toLowerCase();
            if (obj.employeeName.includes(model) || obj.employeeCode.includes(model)) {
                temp.push(obj);
            }
        });
        this.reporteeListPendingRequest = temp;
    };
    ReviewComponent.prototype.onLeaveListSelect = function (selectedLeaveList) {
        this.selectionColor = true;
        return this.PendingLeaveRequests.find(function (userleave) { return userleave.leaveTypeName === selectedLeaveList; });
    };
    ////////////////////////////////////// Leave Approve Reject //////////////////////////////////
    ReviewComponent.prototype.processLeaveRequests = function (toProcess, action, reason) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_3__utility_constant__["a" /* CONSTANT */].APPROVE_LEAVE_REQUEST_PROCESS;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "employeeId": this.service.employeeId,
            "leaveRequestId": toProcess,
            "leaveStatus": action,
            "leaveReason": reason,
            "contactNumber": this.service.contactNumber,
            "numberOfDays": "",
            "leaveTypeId": "",
            "fromDate": null,
            "toDate": null
        };
        this.spinnerService.show();
        this.click = true;
        this.service.postService(url, data)
            .subscribe(function (data) {
            if (data[0].status == 'Success') {
                _this.toastr.success(data[0].successMessage);
                _this.itemsSelected = 0;
                _this.reason = '';
                _this.selectedAll = false;
                _this.click = true;
                _this.getPendingLeaveRequests();
            }
            else if (data[0].status == 'Error') {
                _this.toastr.error(data[0].errorMessage);
            }
            _this.click = true;
            _this.spinnerService.hide();
            _this.searchReset();
        }, function (error) {
            console.log(error);
