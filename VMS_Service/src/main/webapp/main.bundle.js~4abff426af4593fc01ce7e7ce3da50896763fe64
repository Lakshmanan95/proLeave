    };
    ReviewComponent.prototype.getPendingLeaveRequests = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_3__utility_constant__["a" /* CONSTANT */].PENDING_REQUESTS;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "reporteeEmployeeNumber": "",
            "reporteeName": "",
            "fromDate": "",
            "toDate": "",
            "appliedDate": "",
            "leaveType": ""
