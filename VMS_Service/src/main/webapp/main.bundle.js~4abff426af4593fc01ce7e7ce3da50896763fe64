    ReviewComponent.prototype.Reset = function () {
        this.reportData['searchName'] = undefined;
        this.selectedEmployeeId = undefined;
        this.reportData['leavefrom'] = undefined;
        this.reportData['leaveto'] = undefined;
        this.reportData["appliedDate"] = undefined;
