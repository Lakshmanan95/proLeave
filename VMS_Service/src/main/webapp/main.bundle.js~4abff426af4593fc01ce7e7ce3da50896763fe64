        this.reviewedOn = this.reportData['reviewedDate'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.reportData['reviewedDate']).format('YYYY-MM-DD').toString() : null;
        this.repleaveType = (this.leaveTypeId == undefined ? null : this.leaveTypeId);
        this.selectedEmployeeId = this.selectedEmployeeId == undefined && this.reportData['searchName'] != undefined && this.reportData['searchName'] != '' ? '0' : this.selectedEmployeeId;
        var data = {
            "flag": this.toggle ? 'DR' : 'IH',
            "loginEmployeeCode": this.service.employeeNumber,
            "leaveFromDate": this.repleavefrom,
            "leaveToDate": this.repleaveto,
            "leaveAppliedOn": this.repappliedDate,
            "leaveReviewedOn": this.reviewedOn,
            "employeeCode": this.selectedEmployeeId,
            "leaveTypeId": this.repleaveType,
            "leaveStatusId": this.repapprove
        };
        var url = __WEBPACK_IMPORTED_MODULE_3__utility_constant__["a" /* CONSTANT */].SEARCH_HIERACHY;
        this.spinnerService.show();
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.ReportLeaveLoad = data.searchList;
            _this.employeeDetail = {};
            _this.modifiedHierarchy(data.searchList, '');
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    ReviewComponent.prototype.modifiedHierarchy = function (data, str) {
        var _this = this;
        if (str = !'' && str == 'onload') {
            this.reportData['leavefrom'] = new Date(__WEBPACK_IMPORTED_MODULE_1_moment__().utc().startOf('month').format('YYYY-MM-DD'));
            this.reportData['leaveto'] = new Date(__WEBPACK_IMPORTED_MODULE_1_moment__().utc().endOf('month').format('YYYY-MM-DD'));
        }
        var empName, rmpEmpCode, empCode;
        this.employeeDetail = {};
        var levelOneArr, levelTwoArr = [];
        this.empCodeList = [];
        /**Separating Level1 start*/
        var levelOneList = data.filter(function (value, key) {
            if (_this.empCodeList.length == 0) {
                _this.empCodeList.push(value.employeeCode);
            }
            if (_this.empCodeList.length != 0) {
                if (_this.empCodeList.indexOf(value.employeeCode) == -1) {
                    _this.empCodeList.push(value.employeeCode);
                }
            }
            if (value.levelId == 1) {
                return value;
            }
        });
        /**Separating Level1 end*/
        /**Separating Level2 start*/
        var levelTwoList = data.filter(function (value, key) {
            if (value.levelId == 2) {
                return value;
            }
        });
        /**Separating Level2 end*/
        /**Making Hierarchy for Level1 and Level2*/
        this.empCodeList.forEach(function (code, index) {
            _this.leveloneEmployeeFilterArr = levelOneList.filter(function (elem, key) {
                if (code == elem.employeeCode) {
                    empName = elem.employeeName;
                    empCode = elem.employeeCode;
                    return elem;
                }
            });
            var obj = {};
            levelOneArr = [];
            obj['empCode'] = empCode;
            obj['empName'] = empName;
            obj['arrList'] = _this.leveloneEmployeeFilterArr;
            if (_this.leveloneEmployeeFilterArr.length != 0) {
                levelOneArr.push(obj);
                _this.employeeDetail[code] = levelOneArr;
            }
            //leveltwostructure
            _this.leveltwoEmployeeFilterArr = levelTwoList.filter(function (elem1, key1) {
                if (code == elem1.employeeCode) {
                    empName = elem1.employeeName;
                    empCode = elem1.employeeCode;
                    rmpEmpCode = elem1.rmEmpCode;
                    return elem1;
                }
            });
            var obj1 = {}; // levelTwoArr=[];
            obj1['empCode'] = empCode;
            obj1['empName'] = empName;
            obj1['rmpEmpCode'] = rmpEmpCode;
            obj1['arrList'] = _this.leveltwoEmployeeFilterArr;
            if (_this.leveltwoEmployeeFilterArr.length != 0) {
                levelTwoArr.push(obj1);
            }
        });
        /**Checking Level2 Resource is mapping in level1 */
        levelTwoArr.forEach(function (val, index1) {
            var arr = [];
            if (_this.employeeDetail[val.rmpEmpCode] != undefined) {
            }
            else if (_this.employeeDetail[val.rmpEmpCode] == undefined) {
                arr.push(val);
                _this.employeeDetail[val.empCode] = arr;
            }
        }); /**filtering reportees employeeId if user as sub reportee */
        var reportee = [];
        var subList = levelTwoArr.filter(function (elem1, key1) {
            if (_this.employeeDetail[elem1.rmpEmpCode] != undefined) {
                if (reportee.length == 0) {
                    reportee.push(elem1.rmpEmpCode);
                }
                if (reportee.length != 0 && reportee.indexOf(elem1.rmpEmpCode) == -1) {
                    reportee.push(elem1.rmpEmpCode);
                }
                return elem1;
            }
        });
        /***making subreportee hierarchy */
        var temp = [], empcodeArr = [];
        var filteredList = reportee.filter(function (val, key1) {
            temp = [];
            levelTwoArr.forEach(function (data, index) {
                if (val == data.rmpEmpCode) {
                    data['records'] = data['arrList'];
                    temp.push(data);
                    return data;
                }
            });
            if (temp.length != 0) {
                _this.employeeDetail[val][0]['isReportee'] = true;
                _this.employeeDetail[val][0]['subReportees'] = temp;
            }
        });
        /**Making Hierarchy for Level1 end*/
        this.isObjectempty = Object.keys(this.employeeDetail).length == 0 ? true : false;
    };
    ReviewComponent.prototype.flagHide = function (val) {
