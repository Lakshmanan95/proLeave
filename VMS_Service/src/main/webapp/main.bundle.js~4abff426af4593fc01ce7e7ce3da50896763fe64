    ReviewComponent.prototype.checkAll = function (ev) {
        if ((ev.target.checked == false)) {
            this.itemsSelected = 0;
        }
        var interateVAl = this.p * 15 - 15;
        interateVAl = interateVAl > 0 ? interateVAl : interateVAl;
        for (var i = interateVAl; i < this.p * 15; i++) {
            if (this.PendingLeaveRequests[i] != undefined) {
                this.PendingLeaveRequests[i].selected = this.selectedAll;
                if (this.PendingLeaveRequests[i].selected == true) {
                    this.itemsSelected++;
                }
                else {
                    if (this.itemsSelected > 0)
                        this.itemsSelected--;
                }
            }
        }
    };
    ReviewComponent.prototype.itemselected = function (ev) {
        if (ev.target.checked == true) {
            this.itemsSelected++;
        }
        else {
            this.itemsSelected--;
            this.selectedAll = false;
        }
        this.selectedAll = this.PendingLeaveRequests.every(function (item) {
            return item.selected == true;
        });
    };
    ReviewComponent.prototype.rowItemSelected = function (leave) {
        if (leave.selected == true) {
            leave.selected = false;
            this.itemsSelected--;
            this.selectedAll = false;
        }
        else if (leave.selected == undefined) {
            leave.selected = true;
            this.itemsSelected++;
        }
        else {
            leave.selected = true;
            this.itemsSelected++;
        }
        this.selectedAll = this.PendingLeaveRequests.every(function (item) {
            return item.selected == true;
        });
    };
    ReviewComponent.prototype.isAllChecked = function () {
        return this.PendingLeaveRequests && this.PendingLeaveRequests.every(function (_) { return _.selected; });
    };
    ReviewComponent.prototype.openModal = function (template) {
        this.modalRef = this.modalService.show(template, { class: 'modal-sm' });
    };
