        }
        this.data['fincemployeeName'] = obj.employeeName;
        this.selectedEmployeeId = obj.employeeId;
        this.isvalidCmp = false;
    };
    AdminComponent.prototype.onFinanceEmployeeChange = function (selectedName, empId) {
        $('#fin-name-search').show();
        this.isvalidCmp = false;
        this.cancelFincCredit = false;
    };
