    HomeComponent.prototype.submitLeaveRequest = function (template, success, error, Infotemplate) {
        var _this = this;
        if ((this.leavetype == "OD" && (this.onDutyOptions != "OTH" && this.onDutyOptions != "undefined") && !this.leaveTypeChangeEvent()) ||
            (this.onDutyOptions == "OTH" && !this.ReasonChangeEvent() && !this.leaveTypeChangeEvent()) ||
            (this.leavetype != "OD" && this.leavetype != "undefined" && !this.ReasonChangeEvent())) {
            this.data.leaveTypeId = this.fetchid('', '');
            this.data.contactNumber = this.service.contactNumber;
            this.data.employeeNumber = Number(this.service.employeeNumber);
            this.data.fromDate = this.newdateFrom();
            this.data.toDate = this.newdateTo();
            this.data.leaveRequestId = this.withdrawFlag == true ? this.currentLeave : 0; // this.currentLeave
            this.data.leaveStatus = this.withdrawFlag == true ? 'Cancelled' : "Pending"; // Cancelled
            this.data.numberOfDays = this.noOfDays;
            if (this.onDutyOptions == "OTH") {
                this.data.leaveReason = this.odComments;
            }
            else {
                this.data.leaveReason = this.reason;
            }
            this.data.employeeId = Number(this.service.employeeId);
            this.data.odOptionId = (this.leavetype == 'OD') ? this.fetchid(this.leavetype, this.onDutyOptions) : null;
            if (this.withdrawFlag == false) {
                this.openModal(template);
            }
            else {
                this.spinnerService.show();
                var urlSave = __WEBPACK_IMPORTED_MODULE_1__utility_constant__["a" /* CONSTANT */].LEAVE_REQUEST_PROCESS;
                this.service.postService(urlSave, this.data).subscribe(function (data) {
                    if (data[0].status === 'Success') {
                        _this.openModal(success);
                        _this.successmessage = "Leave has been withdrawn successfully. You can now modify the details and re-submit or reset the leave request";
                        _this.getloggeduserinfo();
                        _this.getuserhistoryinfo();
                        _this.withdrawReset = false;
                        _this.reset('');
                    }
                    else {
                        _this.openModal(error);
                        _this.errormessage = data[0].errorMessage;
                    }
                    _this.spinnerService.hide();
                }, function (error) {
                    console.log(error);
                    if (error.status == 0) {
                        window.location.reload();
                    }
                });
            }
        }
        else {
            this.isvalid = true;
        }
    };
    HomeComponent.prototype.infoPopup = function (template, success, error, Infotemplate) {
        this.modalRef2.hide();
        this.submitLeaveRequest(template, success, error, Infotemplate);
    };
