            this.checkedFinanceFlag = false;
            this.data['fincemployeeName'] = '';
            this.reporttype = 'Select Report';
        }
        else if (this.checkedFinance == 'inactive') {
            this.reporttype = 'Select Report';
        }
        this.isvalidCmp = false;
        this.isvalidReport = false;
        this.isvalidEmp = false;
    };
    AdminComponent.prototype.getFinanceEmployeeReport = function () {
        var _this = this;
