);\n  }\n\n  ngOnInit() {\n    this.getloggeduserinfo();\n    this.FilterForm = new FormGroup({\n      days: new FormControl()\n    });\n    const btn = document.getElementById(\"LB\");\n    const ele = document.getElementsByClassName('container-fluid')[0];\n    btn.style.display = 'none';\n    ele.addEventListener('scroll', function () {\n      if (ele.scrollTop > 20) {\n        btn.style.display = \"block\";\n      } else {\n        btn.style.display = \"none\";\n      }\n    });\n  }\n\n  /////////////////// User logged informatioan ///////////////////////\n\n  getloggeduserinfo() {\n    const url = CONSTANT.LOGGED_USER_INFO;\n    let data = {\n      // \"employeeNumber\": \"102861\"\n      \"employeeNumber\": \"\"\n    }\n    this.LeaveTypeNewDropDown = [];\n    this.CopyLeaveTypeNewDropDown = [];\n    this.onDutyOptionsArr = [];\n    this.legendcolor = [];\n    this.leaveBalanceWidget = [];\n    this.spinnerService.show();\n    this.service.postService(url, data)\n      .subscribe(data => {\n        let setColor;\n        var checkFlag = false;\n        data.LeaveTypeList.forEach(element => {\n          this.leaveTypeLists.push(element.leaveCode);\n          if (element.isDefault == \"Y\") {\n            this.defaultLeaveTypeLists.push(element.leaveCode);\n          }\n          if (element.isContinuous == \"Y\") {\n            this.continuousLeaveTypeLists.push(element.leaveCode);\n          }\n          checkFlag = true;\n        });\n        if (checkFlag) {\n          this.LeaveTypeNewDropDown.push({ \"type\": \"Loss Of Pay\", \"leaveCode\": \"LOP\", \"typeid\": 5 }, { \"type\": \"On Duty\", \"typeid\": 6, \"leaveCode\": \"OD\" });\n        }\n        data.LeaveBalanceInfo.forEach((element, index) => {\n          if (this.leaveTypeLists.indexOf(element.leaveCode) > -1) {\n            if (this.continuousLeaveTypeLists.indexOf(element.leaveCode) > -1) {\n              if (parseFloat(element.approvedAndPendingBalance) == 0 && element.balanceLeave > 0) {\n                this.LeaveTypeNewDropDown.push({ \"type\": element.leaveTypeName, \"leaveCode\": element.leaveCode, \"typeid\": element.leaveTypeId, \"balanceLeave\": element.balanceLeave, \"location\": element.locationCode });\n              }\n            } else if (element.balanceLeave > 0 || element.leaveCode == \"OD\" || element.leaveCode == \"LOP\") {\n              this.LeaveTypeNewDropDown.push({ \"type\": element.leaveTypeName, \"leaveCode\": element.leaveCode, \"typeid\": element.leaveTypeId, \"balanceLeave\": element.balanceLeave, \"location\": element.locationCode });\n            }\n            if (this.defaultLeaveTypeLists.indexOf(element.leaveCode) > -1) {\n              this.leaveBalanceWidget.push(element);\n              this.defaultLeaveWithoutBalance.push(element.leaveCode);\n            }\n          }\n          if (this.defaultLeaveTypeLists.indexOf(element.leaveCode) > -1 && this.legendsLeaveData.indexOf(element.leaveCode) < 0) {\n            this.legendsLeaveData.push(element.leaveCode);\n          }\n          let perDay = 100 / (element.openLeave + Number(element.creditedLeave));\n          // Setting color in legends\n          if (element.leaveCode !== 'PTL' && element.leaveCode !== 'BL' && element.leaveCode !== 'ML') {\n            element.percentage = perDay * Number(element.usedLeave);\n          }\n          if (element.leaveCode == 'ML') {\n            this.currentLeaveBalance = element.balanceLeave;\n          }\n        });\n        this.defaultLeaveTypeLists.forEach(element => {\n          if (this.defaultLeaveWithoutBalance.indexOf(element) < 0) {\n            this.leaveBalanceWidget.push({ \"leaveCode\": element, \"leaveTypeName\": element, \"openLeave\": \"0\", \"creditedLeave\": \"0.00\", \"usedLeave\": \"0.00\", \"balanceLeave\": \"0.00\", \"pendingLeave\": \"0.00\" });\n          }\n        });\n        if (data.LeaveBalanceInfo.length == 0) {\n          data.LeaveTypeList.forEach(element => {\n            if (element.isDefault == \"Y\") {\n              this.leaveBalanceWidget.push({ \"leaveCode\": element.leaveCode, \"leaveTypeName\": element.leaveType, \"openLeave\": \"0\", \"creditedLeave\": \"0.00\", \"usedLeave\": \"0.00\", \"balanceLeave\": \"0.00\", \"pendingLeave\": \"0.00\" });\n            }\n          });\n        }\n        this.legendsLeaveData.forEach(element => {\n          setColor = data.Legends[0].hasOwnProperty(element) != undefined ? data.Legends[0][element] : '';\n          this.legendcolor.push({ \"leaveCode\": element, \"color\": setColor });\n        });\n        data.OutOfOfficeOptions.forEach((element, index) => {\n          this.onDutyOptionsArr.push({ \"type\": element.odOptionName, \"typeid\": element.odOptionId, \"code\": element.odOptionCode, \"reason\": element.leaveReason });\n\n        });\n        data.AdHocWorkingDays.forEach((element, index) => {\n          if (this.workingdays != undefined && this.workingdays.indexOf(element.leaveDate) == -1) {\n            this.workingdays.push(element.leaveDate);\n          }\n        });\n        this.userData = this.leaveBalanceWidget;\n        this.percentage = 100;\n        this.loggedUsserInfo = data;\n        this.holidays = data.HolidayList;\n        this.LeaveList = data.LeaveTypeList;\n        this.service.employeeNumber = data.EmpReportManagerInfo[0].employeeNumber;\n        this.service.employeeType = data.EmpReportManagerInfo[0].employeeType;\n        if (this.service.employeeType == 'C') {\n          this.hideIcon = false;\n        }\n        this.service.sendReporteeInfo({ employeeName: data.EmpReportManagerInfo[0].employeeName, locationName: data.EmpReportManagerInfo[0].locationName, role: data.EmpReportManagerInfo[0].role, leftPanelMenu: \"Home\", hide: this.hideIcon });\n        this.service.employeeId = data.EmpReportManagerInfo[0].employeeId;\n        this.service.contactNumber = data.EmpReportManagerInfo[0].contactNumber;\n        this.service.locationCode = data.EmpReportManagerInfo[0].locationCode;\n        this.reportingManagerCount = data.EmpReportManagerInfo[0].reportingManagerName.split(',');\n        this.location = data.EmpReportManagerInfo[0].locationName;\n        this.name = data.EmpReportManagerInfo[0].employeeName;\n        this.reportingManagerCountLength = this.reportingManagerCount.length;\n        if (this.reportingManagerCount.length > 1) {\n          this.reportingManagerCountFlag = true;\n        }\n        this.reportingManagerName = data.EmpReportManagerInfo[0].reportingManagerName.replace(/,/g, '|');\n        if (this.service.employeeType == 'C') {\n          this.router.navigate(['/consultant']);\n        }\n        //Holiday List\n        for (let i = 0; i < data.HolidayList.length; i++) {\n          this.holidaysArray.push(data.HolidayList[i].holidayDate);\n        }\n        this.disabledDaysArr();\n        this.CopyLeaveTypeNewDropDown = this.CopyLeaveTypeNewDropDown.concat(this.LeaveTypeNewDropDown);\n        this.spinnerService.hide();\n        this.getuserhistoryinfo();\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n        }\n      })\n  }\n\n  /////////////////// Showing leave Information //////////////////\n\n  getleaverequestinfo(post, rejectInfoTemplate, Infotemplate) {\n    const url = CONSTANT.LEAVE_REQUEST_FETCH;\n    this.submitBtnTxt = true;\n    this.withdrawFlag = true;\n    this.currentLeave = post.leaveRequestId;\n    (post.leaveStatus == 'Pending') ? this.WithdrawHide = true : this.WithdrawHide = false;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"leaveRequestId\": post.leaveRequestId\n    }\n    this.spinnerService.show();\n    this.service.postService(url, data)\n      .subscribe(data => {\n        if (post.leaveStatus == \"Rejected\") {\n          this.fetchdateFrom = new Date(data[0].fromDate);\n          this.fetchdateTo = new Date(data[0].toDate);\n          this.rejectedreson = data[0].rejectedReason;\n          this.leavetypename = data[0].leaveTypeName;\n          this.noOfDays = data[0].numberOfDays;\n          this.modalRef3 = this.modalService.show(rejectInfoTemplate, { class: 'modal-sm' });\n          this.leavetype = data[0].leaveTypecode;\n          this.dateFrom = new Date(data[0].fromDate);\n          this.dateTo = new Date(data[0].toDate);\n          if (data[0].leaveReason == '') {\n            this.fetchreason = data[0].odOptionName;\n          } else {\n            this.fetchreason = data[0].leaveReason;\n          }\n          this.odComments = data[0].leaveReason;\n          this.reason = data[0].leaveReason;\n          (data[0].odOptionCode == '') ? this.onDutyFlag = false : this.onDutyFlag = true; this.onDutyOptions = data[0].odOptionCode;\n          this.submitBtnTxt = false;\n        } else {\n          if (this.LeaveTypeNewDropDown.find(x => x.leaveCode == data[0].leaveTypecode) == undefined) {\n            this.LeaveTypeNewDropDown.push({ \"type\": data[0].leaveTypeName, \"leaveCode\": data[0].leaveTypecode, \"typeid\": data[0].leaveTypeId });\n          }\n          this.leavetype = data[0].leaveTypecode;\n          this.leavetypename = data[0].leaveTypeName;\n          this.leaveTypeChangeEvent();\n          (data[0].odOptionCode == '') ? this.onDutyFlag = false : this.onDutyFlag = true; this.onDutyOptions = data[0].odOptionCode;\n          this.OndutyDropDown();\n          var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds\n          var fromDate = new Date(moment(data[0].fromDate).format('YYYY/MM/DD').toString());\n          var toDate = new Date(moment(data[0].toDate).format('YYYY/MM/DD').toString());\n          var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));\n          var numberOfDays = diffDays + 1;\n          this.dateFrom = fromDate;\n\n          this.dateTo = toDate;\n          if (data[0].odOptionCode == 'OTH') {\n            this.odComments = data[0].leaveReason;\n            this.reason = \"\";\n          } else {\n            this.reason = data[0].leaveReason;\n            this.odComments = \"\";\n          }\n          this.noOfDays = data[0].numberOfDays;\n          this.submitBtnTxt = false;\n        }\n        this.spinnerService.hide();\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n        }\n      })\n  }\n\n  rejecthide(post, rejectInfoTemplate) {\n    this.modalRef3.hide();\n  }\n  onRightClick() {\n    return false;\n  }\n  ////////////////// Getting User history information service ///////////////////// \n\n  getuserhistoryinfo() {\n    const url = CONSTANT.USER_HISTORY_INFO;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      // \"employeeNumber\": \"102861\",\n      \"appliedDate\": \"\",\n      \"fromDate\": \"\",\n      \"toDate\": \"\",\n      \"numberOfDays\": \"\",\n      \"leaveType\": \"\",\n      \"approvedDate\": \"\",\n      \"leaveStatus\": \"\"\n    }\n    this.fetchTimeOffHistory(url, data);\n  }\n\n  fetchTimeOffHistory(url, data) {\n    this.checkTimeOffHistory = [];\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.historyLeaveListRequest = data;\n        for (let i = 0; i < data.length; i++) {\n          if (data[i].leaveStatus.toLowerCase() == \"approved\") {\n            let noOfDates = []\n            var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds\n            var fromDate = new Date(moment(data[i].fromDate).format('YYYY/MM/DD').toString());\n            var toDate = new Date(moment(data[i].toDate).format('YYYY/MM/DD').toString());\n            var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));\n            var numberOfDays = diffDays + 1;\n            for (let j = 0; j < numberOfDays; j++) {\n              this.approvedDates.push(moment(data[i].fromDate).add(j, 'days').format('YYYY-MM-DD').toString());\n              let leaveData = {\n                leaveType: data[i].leaveTypeId,\n                leaveTypeCode: data[i].leaveCode,\n                leaveDate: moment(data[i].fromDate).add(j, 'days').format('YYYY-MM-DD').toString()\n              }\n              this.holidaysApproved.push(leaveData);\n            }\n            if (this.continuousLeaveTypeLists.indexOf(data[i].leaveCode) > -1) {\n              if (this.today.getFullYear() == new Date(data[i].toDate).getFullYear()) {\n                this.checkTimeOffHistory.push(data[i].leaveCode);\n              }\n            }\n          } else if (data[i].leaveStatus.toLowerCase().includes(\"pending\")) {\n            if (this.continuousLeaveTypeLists.indexOf(data[i].leaveCode) > -1) {\n              if (this.today.getFullYear() == new Date(data[i].toDate).getFullYear()) {\n                this.checkTimeOffHistory.push(data[i].leaveCode);\n              }\n            }\n          }\n\n          ////////// splitting the dual approval and dispalying single approval name in UI////////////\n          this.reportingManagerCoun = data[i].approvedStatus;\n          data[i].notificationApprovedStatus = data[i].approvedStatus;\n          this.reportingManagerCoun = data[i].approvedStatus.split(\",\", 1);\n          if (data[i].approvedStatus.split(\",\").length < 2) {\n            this.reportingManagerCoun = data[i].approvedStatus;\n            data[i].notificationShow = false;\n          } else {\n            if (data[i].notificationApprovedStatus.toLowerCase().includes(\"approved\")) {\n              data[i].notificationShow = false;\n            } else {\n              data[i].notificationShow = true;\n            }\n          }\n          data[i].approvedStatus = this.reportingManagerCoun;\n\n        }\n        this.userhistoryInfo = data.sort((a: any, b: any) =>\n          new Date(a.appliedOnDate).getTime() - new Date(b.appliedOnDate).getTime()\n        );\n        //////////////////// hover dual approval/////////////////////\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n\n      })\n  }\n\n  ////////////////////////// filter histroy ///////////////////////\n\n  searchHistroy(g) {\n    this.leavefrom = this.data['leavefrom'] != undefined ? moment(this.data['leavefrom']).format('YYYY-MM-DD').toString() : null;\n    this.leaveto = this.data['leaveto'] != undefined ? moment(this.data['leaveto']).format('YYYY-MM-DD').toString() : null;\n    this.approve = this.data['approve'] != undefined ? moment(this.data['approve']).format('YYYY-MM-DD').toString() : null;\n    this.appliedDate = this.data['appliedDate'] != undefined ? moment(this.data['appliedDate']).format('YYYY-MM-DD').toString() : null;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"appliedDate\": this.appliedDate,\n      \"fromDate\": this.leavefrom,\n      \"toDate\": this.leaveto,\n      \"numberOfDays\": this.data['days'],\n      \"leaveType\": this.data['employeeLeaveTypeList'],\n      \"approvedDate\": this.approve,\n      \"leaveStatus\": this.data['Searchstatus']\n    }\n    if ((this.leavefrom == \"\" && this.leaveto == \"\" && (this.approve != \"\" && this.appliedDate != \"\" && this.data['days'] != \"\" && this.data['Searchstatus'] != \"\" && this.data['employeeLeaveTypeList'] != \"\"))\n      || (this.data['employeeLeaveTypeList'] != 'undefined')) {\n      const url = CONSTANT.USER_HISTORY_INFO;\n      this.fetchTimeOffHistory(url, data);\n    }\n  }\n\n  searchReset() {\n    this.data['approve'] = undefined;\n    this.data['leavefrom'] = undefined;\n    this.data['leaveto'] = undefined;\n    this.data['days'] = \"\";\n    this.data['employeeLeaveTypeList'] = undefined;\n    this.data['appliedDate'] = undefined;\n    this.data['Searchstatus'] = undefined;\n    this.getuserhistoryinfo();\n  }\n\n  ////////////////// Displaying color in calander ////////////////////////\n\n  setAlignmentStyle(date) {\n    let month = date.month + 1;\n    let tempDate = date.year + '-' + ((month < 10) ? '0' + month : '' + month) + '-' + ((date.day < 10) ? '0' + date.day : '' + date.day);\n    var dateMoment = moment(date).format('YYYY-MM-DD').toString();\n    var test1 = moment(dateMoment).utc();\n    var newDate = new Date(test1.toString());\n    if (this.holidaysArray && this.holidaysArray.indexOf(tempDate) != -1) {\n      return \"#D07CED\";\n    }\n    for (let i = 0; i < this.holidaysApproved.length; i++) {\n      if (!((newDate.getDay() == 6 || newDate.getDay() == 0 || this.disableDatesForWeekend.indexOf(dateMoment) > -1) && this.enabledDatesForWeekend.indexOf(dateMoment) < 0) || this.continuousLeaveTypeListsWithWeekend.indexOf(this.holidaysApproved[i].leaveTypeCode) > -1) {\n        if (this.holidaysApproved[i].leaveDate == tempDate) {\n          if (this.holidaysApproved[i].leaveType == '1') { //CL\n            return \"#DB9696\";\n          }\n          if (this.holidaysApproved[i].leaveType == '2') { //PL\n            return \"#F5C66E\";\n          }\n          if (this.holidaysApproved[i].leaveType == '3') { //COFF\n            return \"#49D1E3\";\n          }\n          if (this.holidaysApproved[i].leaveType == '4') { //PTO\n            return \"#F5C66E\";\n          }\n          if (this.holidaysApproved[i].leaveType == '5') { //LOP\n            return \"#DF485B\";\n          }\n          if (this.holidaysApproved[i].leaveType == '6') { //OD\n            return \"#7ED321\";\n          }\n          if (this.holidaysApproved[i].leaveType == '14') { //MDL\n            return \"#DB9696\";\n          }\n          if (this.holidaysApproved[i].leaveType == '15') { //ANL\n            return \"#F5C66E\";\n          }\n          if (this.holidaysApproved[i].leaveType == '22') { //APTO\n            return \"#DB9696\";\n          }\n        }\n      }\n    }\n  }\n\n  //////////////////////////// Counting the days////////////////////////\n\n  doOnSelect(event, pickerName) {\n    if (pickerName == \"from\") {\n      this.dateTo = this.dateFrom;\n      if (this.leavetype == \"ML\") {\n        this.dateFilterUpdate(this.leavetype);\n        return true;\n      }\n    } else if (pickerName == \"fromSearch\") {\n      this.data[\"leaveto\"] = this.data[\"leavefrom\"];\n    }\n    this.dateFilterUpdate(this.leavetype);\n  }\n\n  //////////////////////////////// Year & Month Freeze in Date Picker///////////////////////////////////\n\n  doOnMonthChange(event, pickerName) {\n    if (event.month == 1) {\n      this.currentDate = new Date(event.year, 0, 1);\n    } else {\n      this.currentDate = new Date(event.year, event.month - 1, 1);\n    }\n    if (this.dateFrom.getMonth() == 1) {\n      this.toPrevDiable = new Date(this.dateFrom.getFullYear(), 0, 1);\n    } else {\n      this.toPrevDiable = new Date(this.dateFrom.getFullYear(), this.dateFrom.month - 1, 1);\n    }\n\n\n\n    if (this.currentDate.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {\n      $(\".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else if (this.currentDate.getTime() < this.dateFrom.getTime() && pickerName == 'to') {\n      $(\".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else {\n      $(\".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"all\");\n      $(\".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"all\");\n    }\n\n    if (this.currentDate.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'from') {\n      $(\".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else if (this.currentDate.getTime() < this.minDateYearEnd.getTime() && pickerName == 'from') {\n      $(\".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else {\n      $(\".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"all\");\n      $(\".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"all\");\n    }\n  }\n  ///////////////////////////////////// For Top Scroll From Leave History ////////////////////////////////////////\n\n  scroll(el: HTMLElement) {\n    el.scrollIntoView(true);\n    const scrolledY = window.scrollY;\n    if (scrolledY) {\n      window.scroll(0, scrolledY - 60);\n    }\n  }\n\n  scrollToBottom(id) {\n    document.getElementsByClassName('container-fluid')[0].scrollTo(0, document.getElementsByClassName('container-fluid')[0].scrollHeight)\n  }\n\n  scrollToTop(id) {\n    document.getElementsByClassName('container-fluid')[0].scrollTo(0, 0)\n  }\n\n  calculateNumberOfDays() {\n    var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds\n    this.noOfDays = 0;\n    var fromDate = new Date(moment(this.dateFrom).format('YYYY/MM/DD').toString());\n    var toDate = new Date(moment(this.dateTo).format('YYYY/MM/DD').toString());\n    var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));\n    var numberOfDays = diffDays + 1;\n    for (var i = 0; i < numberOfDays; i++) {\n      var inrDate = new Date(this.dateFrom);\n      inrDate.setDate(inrDate.getDate() + i);\n      var checkIncrementedDate = moment(inrDate).format('YYYY-MM-DD');\n      if (this.holidaysArray.indexOf(checkIncrementedDate) < 0 && (!(inrDate.getDay() == 0 || inrDate.getDay() == 6))) {\n        this.noOfDays++;\n      }\n      if (this.workingdays.indexOf(checkIncrementedDate) > -1) {\n        this.noOfDays++;\n      }\n    }\n  }\n\n  //////// disaling dates in datepicker based on working, holidays and approved dates/////////////\n\n  disabledDaysArr() {\n\n    this.workingdays.forEach((element) => {\n      this.enabledDates.push(new Date(moment(element).format('YYYY/MM/DD').toString()));\n      this.enabledDatesForWeekend.push(element);\n    });\n    this.holidaysArray.forEach(element => {\n      this.disableDates.push(new Date(moment(element).format('YYYY/MM/DD').toString()));\n      this.disableDatesForWeekend.push(element);\n    });\n    this.approvedDates.forEach(element => {\n    });\n  }\n\n  ////////////////////////// service call for submit /////////////////////////////\n\n  leaveTypeChangeEvent() {\n\n    if (this.leavetype == 'undefined') {\n      this.isvalidLeavetype = true;\n    }\n    else if (this.leavetype == 'OD') {\n      this.isvalidLeavetype = false;\n      this.isvalidonDutyOptions = false;\n      this.isvalidReason = false;\n      this.onDutyFlag = true;\n      this.isvalidLeavetype = false;\n      this.reason = \"\";\n    }\n    else {\n      this.isvalidonDutyOptions = false;\n      this.isvalidReason = false;\n      this.onDutyFlag = false;\n      this.isvalidLeavetype = false;\n    }\n    this.selectedColor = true;\n    this.dateFilterUpdate(this.leavetype);\n    return this.isvalidLeavetype;\n  }\n\n  ReasonChangeEvent() {\n    if (this.leavetype !== 'OD' && (this.reason.length == 0 || this.reason.length < 1)) {\n      this.isvalidReason = true;\n    } else if (this.leavetype == 'OD' && (this.odComments.length == 0 || this.odComments.length < 1)) {\n      this.isvalidReason = true;\n    }\n    else {\n      this.isvalidReason = false;\n    }\n    return this.isvalidReason;\n  }\n\n  OndutyDropDown() {\n    if (this.onDutyOptions == 'undefined') {\n      this.isvalidonDutyOptions = true;\n      this.odComments = \"\";\n    }\n    else if (this.onDutyOptions == \"OTH\") {\n      this.isvalidLeavetype = false;\n      this.isvalidReason = false;\n      this.isvalidonDutyOptions = false;\n    }\n    else {\n      this.odComments = \"\";\n      this.isvalidonDutyOptions = false;\n    }\n    this.selectedodcolor = true;\n    return this.isvalidonDutyOptions;\n  }\n\n  /////////////////////////// Maternity Leave validations ////////////////////////\n\n  dateFilterUpdate(leaveType) {\n\n    if ((leaveType == \"ML\" || leaveType == \"MML\" || leaveType == \"MCL\") && this.service.locationCode) {\n      this.toDateEditable = false;\n      this.dateTo = new Date((new Date(this.dateFrom)).getTime() + (60 * 60 * 24 * 1000 * (parseFloat(this.currentLeaveBalance) - 1)));\n      var fromDate = new Date(moment(this.dateFrom).format('YYYY/MM/DD').toString());\n      var toDate = new Date(moment(this.dateTo).format('YYYY/MM/DD').toString());\n      var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds\n      var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));\n      this.noOfDays = diffDays + 1;\n    } else if (this.continuousLeaveTypeListsWithWeekend.indexOf(leaveType) > -1) {\n      this.toDateEditable = true;\n      var fromDate = new Date(moment(this.dateFrom).format('YYYY/MM/DD').toString());\n      var toDate = new Date(moment(this.dateTo).format('YYYY/MM/DD').toString());\n      var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds\n      var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));\n      this.noOfDays = diffDays + 1;\n    } else {\n      this.toDateEditable = true;\n      this.calculateNumberOfDays();\n    }\n\n  }\n\n  ////////////////////////// fetching leave and dispalying in leave request form ///////////////////\n\n  fetchid(d, x) {\n    if (d == 'true') {\n      return this.LeaveTypeNewDropDown.find(x => x.leaveCode == this.leavetype).typeid;\n    }\n    else if (d == 'OD') {\n      return this.onDutyOptionsArr.find(x => x.code == this.onDutyOptions).typeid;\n    }\n    else {\n      return this.LeaveList.find(x => x.leaveCode == this.leavetype).leaveTypeId;\n    }\n  }\n\n  submitClicked(template, success, error, Infotemplate, LeaveHistoryTemplate) {\n\n    this.leaveTypeChangeEvent();\n    this.ReasonChangeEvent();\n    this.OndutyDropDown();\n\n    /////////////////////////////////Information MessagePopUp Scenario/////////////////////\n    if (this.withdrawFlag == false) {\n      var dateMoment = moment(this.dateFrom).format('YYYY-MM-DD').toString();\n\n      if ((this.dateFrom.getDay() == 6 || this.dateFrom.getDay() == 0 || this.disableDatesForWeekend.indexOf(dateMoment) > -1) && this.enabledDatesForWeekend.indexOf(dateMoment) < 0) {\n        this.errormessage = \"You are not allowed to submit leave on Saturday, Sunday or any other holidays!\"\n        this.modalRef1 = this.modalService.show(error, { class: 'modal-sm' });\n      } else {\n        switch (this.leavetype) {\n          case \"CL\": //Casual Leave\n            if (this.noOfDays >= 3) {\n              this.Infomessage = \"Kindly submit your medical certificate to facilitate your manager to approve this leave!\"\n              this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            } else {\n              this.submitLeaveRequest(template, success, error, Infotemplate);\n            }\n            break;\n          case \"MDL\": //Medical Leave\n            if (this.noOfDays >= 1) {\n              this.Infomessage = \"Kindly submit your medical certificate to facilitate your manager to approve this leave!\"\n              this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            } else {\n              this.submitLeaveRequest(template, success, error, Infotemplate);\n            }\n            break;\n          // case \"MML\": //Maternity Medical Leave\n          //   this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n          //   this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n          //   break;\n          // case \"MCL\"://Miscarriage Leave\n          //   this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n          //   this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n          //   break;\n          case \"TL\"://Tubectomy Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"PTL\"://Paternity Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"AL\"://Adoption Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"PRL\"://Parental Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"SL\"://Sick Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"FML\"://Family Medical Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"BL\"://Bereavement  Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"CNL\"://Condolence Leave\n            this.Infomessage = \"There should be no break in applying this leave. Should be used on a continuous basis.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          case \"LOP\"://Loss Of Pay\n            if (this.noOfDays > 3) {\n              this.Infomessage = \"You are applying more than 3 days of  LOP at a time.\"\n              this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            }\n            else {\n              this.submitLeaveRequest(template, success, error, Infotemplate);\n            }\n            break;\n          case \"APTO\"://Advance PTO\n            this.Infomessage = \"As per Swedish legislation, if employment is terminated within five (5) years from the date of appointment the same will be adjusted in the Final salary.\"\n            this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n            break;\n          default:\n            this.submitLeaveRequest(template, success, error, Infotemplate);\n            break;\n        }\n      }\n      this.clicked = false;\n    } else {\n      let leaveCount = this.checkTimeOffHistory.filter(f => f == this.leavetype).length;\n      if (leaveCount > 1) {\n        this.Infomessage = \"All the pending leaves of  \" + this.leavetypename + \" type should be withdrawn to resubmit the \" + this.leavetypename + \" request\";\n        this.modalRef2 = this.modalService.show(Infotemplate, { class: 'modal-sm' });\n      } else {\n        this.submitLeaveRequest(template, success, error, Infotemplate);\n      }\n    }\n  }\n\n  private submitLeaveRequest(template, success, error, Infotemplate) {\n    if ((this.leavetype == \"OD\" && (this.onDutyOptions != \"OTH\" && this.onDutyOptions != \"undefined\") && !this.leaveTypeChangeEvent()) || // onduty without comments(!others).\n      (this.onDutyOptions == \"OTH\" && !this.ReasonChangeEvent() && !this.leaveTypeChangeEvent()) || // onduty with comments(others).\n      (this.leavetype != \"OD\" && this.leavetype != \"undefined\" && !this.ReasonChangeEvent())) {\n      this.data.leaveTypeId = this.fetchid('', '');\n      this.data.contactNumber = this.service.contactNumber;\n      this.data.employeeNumber = Number(this.service.employeeNumber);\n      this.data.fromDate = this.newdateFrom();\n      this.data.toDate = this.newdateTo();\n      this.data.leaveRequestId = this.withdrawFlag == true ? this.currentLeave : 0; // this.currentLeave\n      this.data.leaveStatus = this.withdrawFlag == true ? 'Cancelled' : \"Pending\"; // Cancelled\n      this.data.numberOfDays = this.noOfDays;\n      if (this.onDutyOptions == \"OTH\") {\n        this.data.leaveReason = this.odComments;\n      } else {\n        this.data.leaveReason = this.reason;\n      }\n      this.data.employeeId = Number(this.service.employeeId);\n      this.data.odOptionId = (this.leavetype == 'OD') ? this.fetchid(this.leavetype, this.onDutyOptions) : null;\n      if (this.withdrawFlag == false) {\n        this.openModal(template);\n      }\n      else {\n        this.spinnerService.show();\n        const urlSave = CONSTANT.LEAVE_REQUEST_PROCESS;\n\n        this.service.postService(urlSave, this.data).subscribe(data => {\n          if (data[0].status === 'Success') {\n            this.openModal(success);\n            this.successmessage = \"Leave has been withdrawn successfully. You can now modify the details and re-submit or reset the leave request\";\n            this.getloggeduserinfo();\n            this.getuserhistoryinfo();\n            this.withdrawReset = false;\n            this.reset('');\n          } else {\n            this.openModal(error);\n            this.errormessage = data[0].errorMessage;\n          }\n\n          this.spinnerService.hide();\n        }, error => {\n          console.log(error);\n          if (error.status == 0) {\n            window.location.reload();\n          }\n        });\n      }\n    }\n    else {\n      this.isvalid = true;\n    }\n  }\n\n  infoPopup(template, success, error, Infotemplate) {\n    this.modalRef2.hide();\n    this.submitLeaveRequest(template, success, error, Infotemplate);\n  }\n  private newMethodLoad(): boolean {\n    return true;\n  }\n  private newdateFrom(): any {\n    return moment(this.dateFrom).format('YYYY-MM-DD');\n  }\n  private newdateTo(): any {\n    return moment(this.dateTo).format('YYYY-MM-DD');\n  }\n\n  ////////////////////////Reseting the form ///////////////////////////\n\n  reset(flag) {\n\n    this.WithdrawHide = false;\n    if (this.withdrawReset && this.withdrawFlag == true && this.submitBtnTxt == false && ((!this.submitBtnTxt && this.WithdrawHide) == false) || flag) {\n      this.dateFrom = new Date();\n      this.dateTo = new Date();\n      this.reason = \"\";\n      this.odComments = \"\";\n      this.noOfDays = 1;\n      this.leavetype = \"undefined\";\n      this.onDutyOptions = \"undefined\";\n    }\n    this.submitBtnTxt = true;\n    this.isvalidLeavetype = false;\n    this.isvalidonDutyOptions = false;\n    this.isvalidReason = false;\n    this.withdrawFlag = false;\n    this.currentLeave = 0;\n    this.toDateEditable = true;\n    this.selectedColor = false;\n    this.selectedodcolor = false;\n    this.LeaveTypeNewDropDown = [];\n    this.LeaveTypeNewDropDown = this.LeaveTypeNewDropDown.concat(this.CopyLeaveTypeNewDropDown);\n  }\n\n  ////////////// modal window ///////////////////\n\n  openModal(template: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(template, { class: 'modal-sm' });\n  }\n\n\n\n  confirm(template, success, error): void {\n    this.modalRef.hide();\n    const urlSave = CONSTANT.LEAVE_REQUEST_PROCESS;\n    this.spinnerService.show();\n    this.clicked = true;\n    this.service.postService(urlSave, this.data).subscribe(data => {\n      if (data[0].status === 'Success') {\n        this.modalRef.hide();\n        this.successmessage = data[0].successMessage;\n        this.modalRef1 = this.modalService.show(success, { class: 'modal-sm' });\n        this.getloggeduserinfo();\n        this.getuserhistoryinfo();\n        this.resetForm();\n        this.clicked = true;\n      }\n      else { // error\n        this.modalRef.hide();\n        this.errormessage = data[0].errorMessage;\n        this.modalRef1 = this.modalService.show(error, { class: 'modal-sm' });\n        this.clicked = true;\n      }\n      this.spinnerService.hide();\n    }, error => {\n      console.log(error);\n      if (error.status == 0) {\n        window.location.reload();\n      }\n    });\n\n  }\n\n  decline(): void {\n    this.spinnerService.hide();;\n    if (this.modalRef != undefined) {\n      this.modalRef.hide();\n    }\n    if (this.modalRef1 != undefined) {\n      this.modalRef1.hide();\n    }\n    this.clicked = false;\n  }\n\n  resetForm() {\n    this.WithdrawHide = false;\n    this.submitBtnTxt = true;\n    this.isvalidLeavetype = false;\n    this.isvalidonDutyOptions = false;\n    this.isvalidReason = false;\n    this.withdrawFlag = false;\n    this.currentLeave = 0;\n    this.dateFrom = new Date();\n    this.dateTo = new Date();\n    this.reason = \"\";\n    this.odComments = \"\";\n    this.noOfDays = 1;\n    this.leavetype = \"undefined\";\n    this.onDutyOptions = \"undefined\";\n    this.toDateEditable = true;\n    this.selectedColor = true;\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// C:/Users/selvaganesh_m/Downloads/VMS-5/src/app/home/home.component.ts","exports = module.exports = require(\"../../../node_modules/css-loader/lib/css-base.js\")(false);\n// imports\n\n\n// module\nexports.push([module.id, \"ul {\\n    padding: 0;\\n    background-image: linear-gradient(-180deg, #3691EA 0%, #38C8CA 100%);\\n    height: 100%;\\n}\\nul li {\\n    text-align: center;\\n    line-height: 38px;\\n    list-style-type: none;\\n}\\nul li a {\\n    display: block;\\n}\\nul .activeImg {\\n    background: #50abe9; \\n}\\n.selection{\\n    pointer-events: none;\\n}\\n\\n.badge {\\n    display: inline-block;\\n    padding: 2px 4px;\\n    font-size: 68%;\\n    font-weight: 600;\\n    line-height: 1;\\n    color: #3f6380;\\n    text-align: center;\\n    white-space: nowrap;\\n    vertical-align: baseline;\\n    background-color: #f7f967;\\n    border-radius: 16px;\\n    position: absolute;\\n    left: 56px;\\n    top: 61px;\\n\\n}\\n@media only screen and (max-width: 1330px) and (min-width: 992px){\\n    .badge {\\n        \\n        left: 37px;\\n        top: 68px;\\n    }\\n}\\n@media only screen and (max-width: 1443px) and (min-width: 1390px) {\\n    .badge {\\n      left: 46px;\\n    }\\n  }\\n\", \"\"]);\n\n// exports\n\n\n/*** EXPORTS FROM exports-loader ***/\nmodule.exports = module.exports.toString();\n\n\n//////////////////\n// WEBPACK FOOTER\n// C:/Users/selvaganesh_m/Downloads/VMS-5/src/app/leftpanel/leftpanel.component.css\n// module id 