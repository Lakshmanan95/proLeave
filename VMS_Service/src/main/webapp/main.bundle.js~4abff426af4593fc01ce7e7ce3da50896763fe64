        if (isNaN(this.data['searchText'])) {
            reporteeName = this.data['searchText'];
        }
        else {
            reporteeNumber = this.data['searchText'];
        }
        var employeeLeaveList = "";
        if (this.data['employeeLeaveTypeList']) {
            employeeLeaveList = this.data['employeeLeaveTypeList'];
        }
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "reporteeEmployeeNumber": reporteeNumber,
            "reporteeName": reporteeName,
            "fromDate": this.leavefrom,
            "toDate": this.leaveto,
            "appliedDate": this.appliedDate,
            "leaveType": employeeLeaveList
        };
