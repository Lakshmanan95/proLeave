        this.modalRef = this.modalService.show(approvetemplate, { class: 'modal-sm' });
    };
    ReviewComponent.prototype.openModalsingle = function (singleapprovetemplate) {
        this.modalRef = this.modalService.show(singleapprovetemplate, { class: 'modal-sm' });
    };
