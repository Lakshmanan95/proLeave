            var leaveCount = this.checkTimeOffHistory.filter(function (f) { return f == _this.leavetype; }).length;
            if (leaveCount > 1) {
                this.Infomessage = "All the pending leaves of  " + this.leavetypename + " type should be withdrawn to resubmit the " + this.leavetypename + " request";
