            "employeeNumber": empId
        };
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.employeeLeaveListsCopy = data.EmpLeaveBalaceInfo;
            _this.employeeLeaveLists = data.EmpLeaveBalaceInfo;
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.getuserhistoryinfo = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].USER_HISTORY_INFO;
        var data = {
            // "employeeNumber": this.service.employeeNumber,
            "employeeNumber": this.selectedEmployeeId,
            "fromDate": __WEBPACK_IMPORTED_MODULE_6_moment__().subtract('year', 1).format('YYYY') + '-' + '01' + '-' + '01',
            "toDate": __WEBPACK_IMPORTED_MODULE_6_moment__().format('YYYY') + '-' + '12' + '-' + '31',
            "appliedDate": null,
            "numberOfDays": null,
            "leaveType": this.templeaveCode,
            "approvedDate": null,
            "leaveStatus": null
        };
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.summaryHistory = data;
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.creditcancel = function (data, obj) {
        this.cancelCredit = false;
        this.dateJoining = false;
        this.revokeDisable = true;
        this.saveclick = false;
        this.templeaveCode = '';
        this.creditIds = [];
        this.creditObj = {};
        this.addedCompOff = [];
        this.tempSavedDates = [];
        if (data != '') {
            obj.employeeName = '';
            this.getloggeduserinfo();
            $('.creditleave').removeAttr('disabled'); //url(/assets/img/search.png) 97% / 6% no-repeat #ffff
            $('.creditleave').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
        }
        obj.isSubmit = false;
        this.getcompoff('');
    };
    AdminComponent.prototype.getleavecreditupdate = function (obj, error, template) {
        var _this = this;
        this.isSave = this.leavereason == '' ? true : false;
        if (!this.isSave) {
            var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].INSERT_UPDATE_CREDIT;
            var data = {
                "loginEmployeeCode": this.service.employeeNumber,
                "employeeNumber": this.selectedEmployeeId,
                "comments": this.leavereason,
                "leaveTypeId": Number(this.editedData.leaveTypeId),
                "leaveOpen": Number(this.editedData.openLeave),
                "leaveCredit": Number(this.editedData.creditedLeave),
                "leaveUsed": Number(this.editedData.usedLeave),
                "compoffDate": this.tempSavedDates.length != 0 ? this.tempSavedDates.join(',') : null,
                "revokeCompoffDate": this.tempArr.length != 0 ? this.tempArr.join(',') : null
            };
            this.spinnerService.show();
            this.service.postService(url, data)
                .subscribe(function (data) {
                if (data.status == 'Success') {
                    _this.modalRef.hide();
                    _this.templeaveCode = '';
                    _this.getcompoff('');
                    _this.toastr.success(data.successMessage);
                    _this.employeeLeaveBalance(_this.selectedEmployeeId);
                    _this.editdetail = false;
                    _this.saveclick = false;
                    _this.getLeaveHistory(_this.selectedEmployeeId);
                    _this.tempSavedDates = [];
                    _this.getuserhistoryinfo();
                    _this.tempArr = [];
                    _this.revokeDisable = true;
                }
                if (data.status === 'Error') {
                    _this.modalRef.hide();
                    _this.openModal(error);
                    _this.errormessage = data.errorMessage;
                }
                _this.spinnerService.hide();
            }, function (error) {
                console.log(error);
                if (error.status == 0) {
                    window.location.reload();
                }
            });
        }
    };
    AdminComponent.prototype.getLeaveHistory = function (empId) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].LEAVE_CREDIT_HISTORY;
        var data = {
            "employeeNumber": empId,
            "fromDate": null,
            "toDate": null,
            "flag": "HISRPT"
        };
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.leaveHistory = data.EmpLeaveHistoryInfo;
            _this.leaveHistory.forEach(function (history, index) {
                _this.creditId = history.leaveCreditId;
                if (_this.creditIds.length == 0) {
                    _this.creditIds.push(history.leaveCreditId);
                }
                else if (_this.creditIds.length != 0) {
                    if (_this.creditIds.indexOf(history.leaveCreditId) == -1) {
                        _this.creditIds.push(history.leaveCreditId);
                    }
                }
                var serial = history.sno;
            });
            var filteredData;
            _this.leaveHistorycopy = [];
            _this.creditIds.forEach(function (element) {
                var obj = {};
                obj['createdByDate'] = '';
                obj['createdByName'] = '';
                obj['sno'] = '';
                obj['leaveType'] = '';
                obj['comments'] = '';
                obj['leaveCategory'] = '';
                obj['leaveCommentsMerge'] = [];
                filteredData = _this.leaveHistory.filter(function (elem, key) {
                    if (elem.leaveCreditId == element) {
                        return elem;
                    }
                });
                var textMerge = '';
                if (filteredData.length > 1) {
                    obj['createdByDate'] = filteredData[0].createdByDate;
                    obj['createdByName'] = filteredData[0].createdByName;
                    obj['sno'] = filteredData[0].sno;
                    obj['leaveType'] = filteredData[0].leaveType;
                    obj['comments'] = filteredData[0].comments;
                    obj['leaveCategory'] = filteredData[0].leaveCategory;
                    var arr1 = [];
                    filteredData.forEach(function (value, index) {
                        var temp = value.leaveType + " <b>" + value.leaveCategory.toUpperCase() + "</b> changed from " + value.existingValue + " to " + value.newValue;
                        arr1.push(temp);
                    });
                    obj['leaveCommentsMerge'] = arr1;
                    _this.leaveHistorycopy.push(obj);
                }
                else if (filteredData.length != 0 && filteredData.length == 1) {
                    var arr = [];
                    obj['createdByDate'] = filteredData[0].createdByDate;
                    obj['createdByName'] = filteredData[0].createdByName;
                    obj['sno'] = filteredData[0].sno;
                    obj['leaveType'] = filteredData[0].leaveType;
                    obj['comments'] = filteredData[0].comments;
                    obj['leaveCategory'] = filteredData[0].leaveCategory;
                    var temp = filteredData[0].leaveType + " <b>" + filteredData[0].leaveCategory.toUpperCase() + "</b> changed from " + filteredData[0].existingValue + " to " + filteredData[0].newValue;
                    arr.push(temp);
                    obj['leaveCommentsMerge'] = arr;
                    _this.leaveHistorycopy.push(obj);
                }
            });
            _this.leaveHistorycopy.sort(function (a, b) {
                return a.sno - b.sno;
            });
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.leaveonclick = function () {
        this.selectedAll = undefined;
    };
    AdminComponent.prototype.decimalInput = function (e, val, str, leavecode) {
        if (str != undefined && str == "leavetype" && (leavecode == 'PL' || leavecode == 'PTO' || leavecode == 'APTO')) {
            var pattern = /[0-9\.\ ]/;
            if (e.keyCode === 46 && val.split('.').length === 2) {
                return false;
            }
            var inputChar = String.fromCharCode(e.charCode);
            if (e.keyCode != 8 && !pattern.test(inputChar)) {
                event.preventDefault();
            }
        }
    };
    AdminComponent.prototype.decimalValCheck = function (e, val) {
        var pattern = /[0-9\.\ ]/;
        if (e.keyCode === 46 && val.split('.').length === 2) {
            return false;
        }
        var inputChar = String.fromCharCode(e.charCode);
        if (e.keyCode != 8 && !pattern.test(inputChar)) {
            event.preventDefault();
        }
    };
    AdminComponent.prototype.getmassleaveupdate = function (leaveerror) {
        var _this = this;
        this.massupdatevalidationEvent();
        if (this.noOfDays != '' && this.noOfDays != undefined && this.leaveselected != 'Select Leave' && this.locationselected != 'Select Location') {
            var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].MASS_LEAVE_CREDIT;
            if (this.leaveselected == 2 || this.leaveselected == 4) {
                this.noOfDays = (Math.round(Number(this.noOfDays) * 100) / 100).toFixed(2);
            }
            var data = {
                // "employeeNumber": "",
                "employeeCode": this.service.employeeNumber,
                "locationId": this.locationWiseId,
                "leaveTypeId": this.locationLeaveId,
                "noOfDays": this.noOfDays
            };
            this.service.postService(url, data)
                .subscribe(function (data) {
                if (data.status == 'Success') {
                    _this.modalRef.hide();
                    _this.leaveselected = 'Select Leave';
                    _this.locationselected = 'Select Location';
                    _this.noOfDays = '';
                    _this.toastr.success(data.successMessage);
                }
                if (data.status === 'Error') {
                    _this.modalRef.hide();
                    _this.leaveselected = 'Select Leave';
                    _this.locationselected = 'Select Location';
                    _this.noOfDays = '';
                    _this.openModal(leaveerror);
                    _this.errormessage = data.errorMessage;
                }
            }, function (error) {
                console.log(error);
                if (error.status == 0) {
                    window.location.reload();
                }
            });
        }
    };
    AdminComponent.prototype.getcompoff = function (str) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].COFF_UPDATE;
        var data = {
            "employeeNumber": this.selectedEmployeeId
            // "employeeNumber": ""
        };
        this.service.postService(url, data)
            .subscribe(function (data) {
            data.LeaveCOFFDatesInfo.forEach(function (value, index) {
                value['check'] = false;
            });
            _this.leaveDates = data.LeaveCOFFDatesInfo;
            if (str == '') {
                _this.listofcompoffdates = data.LeaveCOFFDatesInfo;
                var arrtemp = [];
                if (_this.tempArr != undefined && _this.tempArr.length != 0) {
                    _this.tempArr.filter(function (elem) {
                        for (var i = 0; i < _this.listofcompoffdates.length; i++) {
                            if (elem == _this.listofcompoffdates[i].leaveCOFFDates && _this.revokeFocused) {
                                _this.listofcompoffdates[i].check = true;
                                arrtemp.push(_this.listofcompoffdates[i]);
                            }
                        }
                        return _this.listofcompoffdates;
                    });
                }
                _this.saveclick = false;
                _this.revokeDisable = arrtemp.length != 0 ? false : true;
            }
            else if (str == 'save') {
                if (_this.tempArr.length == 0) {
                    _this.listofcompoffdatesCopy.forEach(function (elem, index) {
                        if (elem.check) {
                            _this.listofcompoffdatesCopy[index].check = false;
                        }
                    });
                    _this.listofcompoffdates = _this.listofcompoffdatesCopy;
                }
                var arrtemp = [];
                if (_this.tempArr != undefined && _this.tempArr.length != 0) {
                    _this.tempArr.filter(function (elem) {
                        for (var i = 0; i < _this.listofcompoffdates.length; i++) {
                            if (elem == _this.listofcompoffdates[i].leaveCOFFDates && _this.revokeFocused) {
                                _this.listofcompoffdates[i].check = true;
                                arrtemp.push(_this.listofcompoffdates[i]);
                            }
                        }
                        return _this.listofcompoffdates;
                    });
                }
                _this.revokeDisable = _this.tempArr.length != 0 ? false : true;
            }
            var count = 0;
            //checking unchecked count values in list of dates
            _this.listofcompoffdates.forEach(function (element) {
                if (!element.check) {
                    count++;
                }
            });
            if (_this.tempObj != null && !_this.cancelCredit) {
                if (Object.keys(_this.tempObj) != undefined && Object.keys(_this.tempObj).length != 0) {
                    _this.tempObj.creditedLeave = Number(_this.tempObj.creditedLeave) + arrtemp.length;
                    _this.tempObj.balanceLeave = (Number(_this.tempObj.openLeave) + Number(_this.tempObj.creditedLeave)) - Number(_this.tempObj.usedLeave);
                }
            }
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
