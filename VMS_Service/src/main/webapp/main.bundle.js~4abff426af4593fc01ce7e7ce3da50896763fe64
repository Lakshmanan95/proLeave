    HomeComponent.prototype.reset = function (flag) {
        this.WithdrawHide = false;
        if (this.withdrawReset && this.withdrawFlag == true && this.submitBtnTxt == false && ((!this.submitBtnTxt && this.WithdrawHide) == false) || flag) {
            this.dateFrom = new Date();
            this.dateTo = new Date();
            this.reason = "";
            this.odComments = "";
            this.noOfDays = 1;
