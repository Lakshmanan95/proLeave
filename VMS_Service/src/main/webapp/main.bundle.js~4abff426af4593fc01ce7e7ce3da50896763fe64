        this.appliedDate = this.data['appliedDate'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.data['appliedDate']).format('YYYY-MM-DD').toString() : null;
        var leaveType = (this.data['employeeLeaveTypeList'] == undefined ? null : this.data['employeeLeaveTypeList']);
        var search = (this.data['searchText'] == undefined ? null : this.data['searchText']);
        var appliedon = this.isSubmit ? this.appliedDate : null;
        var approveddate = this.isSubmit ? this.approve : null;
        var fromappdate = this.isSubmit ? this.leavefrom : null;
        var toappdate = this.isSubmit ? this.leaveto : null;
        var empsearch = this.isSubmit ? search : null;
        var data = "employeeNumber=" + this.service.employeeNumber + "&approvedDate=" + approveddate + "&fromDate=" + fromappdate + "&toDate=" + toappdate + "&appliedDate=" + appliedon + "&leaveType=" + leaveType + "&leaveStatus=" + "Pending" + "&reporteeEmployeeNumber=" + "" + "&reporteeName=" + empsearch + "&numberOfDays=" + "";
        var url = __WEBPACK_IMPORTED_MODULE_3__utility_constant__["a" /* CONSTANT */].EXPORT_CSV;
        window.location.href = __WEBPACK_IMPORTED_MODULE_3__utility_constant__["a" /* CONSTANT */].EXPORT_CSV + "?" + data;
    };
    ReviewComponent.prototype.doOnSelect = function (event, pickerName) {
