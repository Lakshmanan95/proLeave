    EmployeeNamePipe.prototype.transform = function (items, userInput, str) {
        userInput = userInput.trim();
        if (items !== undefined && items !== null) {
            if (items != undefined && userInput != '' && userInput != undefined && userInput.length > 2) {
                var filtedVal = items.filter(function (val, key) {
                    if (str == undefined) {
                        if (isNaN(userInput)) {
                            if (val.employeeName.toLowerCase().indexOf(userInput.toLowerCase()) != -1) {
                                return val;
                            }
                        }
                        else if (val.employeeId) {
                            if (val.employeeId.indexOf(userInput) != -1) {
                                return val;
                            }
                        }
                    }
                    else if (str == 'auto') {
                        if (isNaN(userInput)) {
                            if (val.employeeName.toLowerCase().indexOf(userInput.toLowerCase()) != -1) {
                                return val;
                            }
                        }
                        else if (val.employeeCode) {
                            if (val.employeeCode.indexOf(userInput) != -1) {
                                return val;
                            }
                        }
                    }
                });
            }
            return filtedVal;
        }
    };
    return EmployeeNamePipe;
