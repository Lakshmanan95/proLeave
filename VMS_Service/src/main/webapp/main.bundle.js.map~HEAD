          obj['monthname'] = element.unFreezeMonth;\n            obj['year'] = element.unFreezeYear;\n            this.locationMonthcopy.push(element.unFreezeMonth);\n          }\n          if (this.locationDummyYear.indexOf(element.unFreezeYear) == -1) {\n            this.locationDummyYear.push(element.unFreezeYear);\n          }\n        });\n        this.locationObj = {};\n        this.restructinghistoryData(this.locationhistory, this.locationMonthcopy, this.locationDummyYear);\n      } else {\n        this.dummyMonthlocationObj = {};\n        this.locationDumyData = [];\n        this.dummylocationObj = {};\n        this.yearFilterObj = {};\n        this.locationDummyYear = [];\n        this.dummyMonthlocationObj = {\n          'month': '',\n          'monthdata': []\n        }\n        ///////location separation function///////\n\n        this.locationKey = [];\n        this.locationData.forEach((value, index) => {\n          var filteredData = this.locationhistory.filter(element => {\n            if (value.locationId == element.locationId) {\n              return element;\n            }\n          });\n          this.locationDummyMonth = [];\n          this.locationDummyYear = [];\n          this.locationDumyData = [];\n          var monthFilteredData = filteredData.filter(element => {\n            if (this.locationDummyMonth.indexOf(element.unFreezeMonth) == -1) {\n              this.locationDummyMonth.push(element.unFreezeMonth);\n            }\n            if (this.locationDummyYear.indexOf(element.unFreezeYear) == -1) {\n              this.locationDummyYear.push(element.unFreezeYear);\n            }\n          });\n          this.monthseparationEvent(this.locationDummyMonth, this.locationDummyYear, filteredData, value);\n        });\n      }\n      this.spinnerService.hide();\n    }, error => {\n      console.log(error);\n      if (error.status == 0) {\n        window.location.reload();\n\n      }\n    })\n    return this.renderFlag = true;\n  }\n\n  monthseparationEvent(monthlist, yearlist, data, val) {\n    this.locationDumyData = [];\n    monthlist.filter((value, key) => {\n      var filteredData = data.filter(element => {\n        if (value == element.unFreezeMonth && val.locationName == element.locationName && (element.unFreezeStatus).toLowerCase() == ('Active').toLowerCase()) {\n          return element;\n        }\n      });\n      /**year filteration */\n      yearlist.filter((value1, key) => {\n        var filteredyearData = filteredData.filter(element => {\n          if (value1 == element.unFreezeYear && val.locationName == element.locationName && (element.unFreezeStatus).toLowerCase() == ('Active').toLowerCase()) {\n            return element;\n          }\n        });\n        if (filteredyearData.length != 0) {\n          this.dummyMonthlocationObj = {\n            'month': value + '-' + value1,\n            'monthdata': filteredyearData\n          }\n          this.locationDumyData.push(this.dummyMonthlocationObj);\n        }\n      });\n\n    });\n\n    if (this.locationDumyData.length != 0) {\n      this.locationKey.push(val.locationName);\n      this.dummylocationObj[val.locationName] = this.locationDumyData;\n      this.dummylocationObjCopy[val.locationName] = yearlist;\n    }\n    this.isEmpty = Object.keys(this.dummylocationObj).length == 0 ? true : false;\n  }\n\n  /////////////////////**making mapping structure for select location history*/////////////////////\n\n  restructinghistoryData(data, month, year) {\n    var obj = {};\n    month.filter((value, key) => {\n      var filteredData = data.filter(element => {\n        if (value == element.unFreezeMonth) {\n          return element;\n        }\n      });\n      this.locationObj[value] = filteredData;\n    });\n    /**year filteration */\n    this.locationMonthcopy.filter((value, key) => {\n      var obj = {};\n      obj['monthname'] = '';\n      obj['year'] = '';\n      if (this.locationObj[value].length != 0) {\n        obj['monthname'] = value;\n        obj['year'] = this.locationObj[value][0].unFreezeYear;\n        this.locationMonth.push(obj);\n      }\n    });\n  }\n\n  DateChangeEvent() {\n    this.isvalidDate = false;\n    this.datecolor = true;\n  }\n\n  ///////////////////////// UNFREEZE PAYROLL CUTOFF PERIOD DATEPICKER VALIDATION ///////////////\n\n  doOnSelect(event, pickerName, index) {\n    this.emptyObj.comments = '';\n    if (pickerName == \"from\") {\n      this.fieldArray[index].dateto = this.fieldArray[index].datefrom;\n    }\n  }\n\n  onMonthChange(event, pickerName, index) {\n    if (event.month == 1) {\n      this.currentDateFreeze = new Date(event.year, 0, 1);\n    } else {\n      this.currentDateFreeze = new Date(event.year, event.month - 1, 1);\n    }\n    if (this.emptyObj.datefrom.getMonth() == 1) {\n      this.toPrevDisable = new Date(this.emptyObj.datefrom.getFullYear(), 0, 1);\n    } else {\n      this.toPrevDisable = new Date(this.emptyObj.datefrom.getFullYear(), this.emptyObj.datefrom.month - 1, 1);\n    }\n\n    if (this.currentDateFreeze.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {\n      $(\".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else if (this.currentDateFreeze.getTime() < this.emptyObj.datefrom.getTime() && pickerName == 'to') {\n      $(\".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else {\n      $(\".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"all\");\n      $(\".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"all\");\n    }\n\n  }\n\n  addFieldValue(index) {\n\n    if (this.fieldArray.length != 0 && (this.fieldArray[this.fieldArray.length - 1].location == 'Select Location' || this.fieldArray[this.fieldArray.length - 1].datefrom == '' || this.fieldArray[this.fieldArray.length - 1].dateto == '' || this.fieldArray[this.fieldArray.length - 1].comments == '')) {\n      return false;\n    }\n    var tempLocation = this.locationData.filter(element => {\n      if (element.locationId == this.locationtype) {\n        return element;\n      }\n    });\n    this.locationtypeTemp = tempLocation[0].locationName;\n    this.emptyObj = {};\n    this.emptyObj.location = tempLocation[0].locationName;\n    this.emptyObj.datefrom = '';\n    this.emptyObj.dateto = '';\n    this.emptyObj.locationId = tempLocation[0].locationId;\n    this.emptyObj.month = moment().format('MMMM');\n    this.emptyObj.comments = '';\n    this.fieldArray.push(this.emptyObj);\n    if (this.fieldArray) {\n      this.selectioncolor = false;\n      this.isvalidReason = false;\n      this.isvalidLocation = false;\n    } else {\n\n    }\n  }\n\n  deleteFieldValue(index) {\n    this.fieldArray.splice(index, 1);\n  }\n\n\n  resetForm() {\n    this.emptyObj.datefrom = '';\n    this.emptyObj.dateto = '';\n    this.emptyObj.comments = '';\n    this.fieldArray = [];\n  }\n\n  //////////////////////////////// UNFREEZE ACCOUNT SECTION ////////////////////////////////////\n\n  openModal(unfreezeed: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(unfreezeed, { class: 'modal-sm' });\n  }\n  openModalerror(error: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(error, { class: 'modal-sm' });\n  }\n\n  unfreezeClicked(error, unfreezeed) {\n    this.unfreeze = true;\n    this.isvalidFreeze = (this.data['searchText'] == '' || this.data['searchText'] == undefined) ? true : false;\n    if (!this.isvalidFreeze) {\n      this.openModal(unfreezeed)\n    }\n  }\n\n  employeeFreeze(error, unfreezeed) {\n\n    this.modalRef.hide();\n    const urlSave = CONSTANT.INSERT_UNFREEZE_DETAILS;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"unfreezeRecords\": [],\n      \"record\": {\n        \"unfreezeEmployeeCode\": this.selectedEmployeeId,\n        \"locationId\": 0,\n        \"fromDate\": this.newdateFrom(),\n        \"toDate\": this.newdateTo(),\n        \"flag\": \"EW\",\n        \"index\": 0\n      }\n    }\n    this.spinnerService.show();\n    this.service.postService(urlSave, data).subscribe(data => {\n      if (data.status == 'Success') {\n        this.toastr.success(data.successMessage);\n        this.selectedEmployeeId = '';\n        this.resetFreeze();\n      } if (data.status === 'Error') {\n        this.modalRef.hide();\n        this.openModal(error);\n        this.errormessage = data.errorMessage;\n      } else if (data.status === 'Failure') {\n        this.modalRef.hide();\n        this.openModal(error);\n        this.errormessage = data.validationMessage;\n      }\n      this.spinnerService.hide();\n    }, error => {\n      console.log(error);\n      if (error.status == 0) {\n        window.location.reload();\n      }\n    })\n\n  }\n  private newdateFrom(): any {\n    return moment(this.fromdatefreeze).format('YYYY-MM-DD');\n  }\n  private newdateTo(): any {\n    return moment(this.todatefreeze).format('YYYY-MM-DD');\n  }\n\n  //////////////////////////// DISPALYING EMPLOYEENAME/ID SCENARIO ///////////////////////////\n\n  onEmployeeChanged(selectedName: string) {\n    let model = selectedName.toLowerCase();\n    this.isvalidFreeze = false;\n    let temp = [];\n    this.activeEmployeeList.map(obj => {\n      if ((obj.employeeName.toLowerCase()).includes(model.toLowerCase()) || obj.employeeId.includes(model)) {\n        temp.push(obj);\n      }\n\n    });\n    this.activeEmployees = temp;\n    this.selectedColor = true;\n  }\n\n  listEmployeSelect(obj) {\n    this.data['searchText'] = obj.employeeName;\n    this.selectedEmployeeId = obj.employeeId;\n    this.isvalidFreeze = false;\n  }\n\n  getActiveEmployeeRequests() {\n    const url = CONSTANT.GET_ACTIVE_EMPLOYEE;\n\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.activeEmployeeList = data.EmployeeInfo;\n        this.activeEmployees = data.EmployeeInfo;\n        this.activeEmployeescopy = data.EmployeeInfo;\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n\n  /////////////////////////////// UNFREEZE ACCOUNT DATEPICKER VALIDATION ////////////////////////////////////////\n\n  unfreezeOnSelect(event, pickerName) {\n    if (pickerName == \"from\") {\n      this.todatefreeze = this.fromdatefreeze;\n    }\n  }\n  editLeaveDetails(obj) {\n    obj.editdetail = true;\n    this.tempopenLeave = obj.openLeave;\n    this.tempcreditedLeave = obj.creditedLeave;\n    this.tempusedLeave = obj.usedLeave;\n    this.templeaveCode = obj.leaveCode;\n  }\n  doOnMonthChange(event, pickerName) {\n    if (event.month == 1) {\n      this.currentDateFreeze = new Date(event.year, 0, 1);\n    } else {\n      this.currentDateFreeze = new Date(event.year, event.month - 1, 1);\n    }\n    if (this.fromdatefreeze.getMonth() == 1) {\n      this.toPrevDisable = new Date(this.fromdatefreeze.getFullYear(), 0, 1);\n    } else {\n      this.toPrevDisable = new Date(this.fromdatefreeze.getFullYear(), this.fromdatefreeze.month - 1, 1);\n    }\n\n    if (this.currentDateFreeze.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {\n      $(\".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else if (this.currentDateFreeze.getTime() < this.fromdatefreeze.getTime() && pickerName == 'to') {\n      $(\".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else {\n      $(\".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"all\");\n      $(\".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"all\");\n    }\n  }\n\n  resetFreeze() {\n    this.data['searchText'] = '';\n    this.fromdatefreeze = new Date();\n    this.todatefreeze = new Date();\n    this.isvalidFreeze = false;\n    this.freezecolor = true;\n  }\n\n  decline(str: any): void {\n    if (str != undefined) {\n      this.checkedValue = 'bulk-upload';\n      this.checkedValueFlag = true;\n    }\n    this.leaveCreditType = [];\n    this.spinnerService.hide();;\n    if (this.modalRef != undefined) {\n      this.modalRef.hide();\n    }\n    if (this.modalRef1 != undefined) {\n      this.modalRef1.hide();\n    }\n    this.locationselected = 'Select Location';\n    this.leaveselected = 'Select Leave';\n    this.isSave = false;\n    this.noOfDays = '';\n    this.leavereason = '';\n    this.daysDisable = true;\n    this.data['autoEmployeeName'] = '';\n    $('.creditleaveauto').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');\n  }\n\n  /////////////////////////////////////////////////// Credit Tab ////////////////////////////////////////\n\n  cancelLeaveDetails(obj) {\n    obj.openLeave = this.tempopenLeave;\n    obj.creditedLeave = this.tempcreditedLeave;\n    obj.usedLeave = this.tempusedLeave;\n    obj.editdetail = false;\n    this.revokeDisable = true;\n    this.revokeFocused = false;\n    this.tempArr = [];\n    this.getcompoff('');\n    obj.balanceLeave = (Number(this.tempopenLeave) + Number(this.tempcreditedLeave)) - Number(this.tempusedLeave);\n  }\n  deciamlCheck(data, emplist) {\n    if (data <= 10) {\n      emplist.openLeave = Number(data).toFixed(2);\n      $('#openinput').val(Number(data).toFixed(2));\n    } else if (data > 10) {\n      data = '';\n      emplist.openLeave = '';\n      $('#openinput').val('');\n    }\n  }\n  leaveExpand(obj) {\n    this.templeaveCode = '';\n    this.lists[1].checked = false;\n    this.selectChild.rework();\n    this.tempObject = obj;\n    var count = 0;\n    if (this.summaryHistory != undefined) {\n      var filteredleavedetail = this.summaryHistory.filter((value, key) => {\n        if (obj.leaveTypeId == value.leaveTypeId && moment(value.fromDate).format('YYYY') == moment().format('YYYY') && (value.leaveStatus.toLowerCase()).indexOf('approved') != -1) {\n          count += Number(value.numberDays);\n          this.currentTotal = count;\n          value.count = count;\n          return value;\n        }\n      });\n    }\n    this.showHistoryTable = $('.card-header').length != $('.cursor-point.collapsed').length && !$(event.target).hasClass('collapsed') ? false : true;\n    this.leaveonclick();\n    this.leavesummaryList = filteredleavedetail;\n    this.currentYearFilterArr = this.leavesummaryList;\n    this.currentYearFilterArr2 = [];\n    this.showSummary = true;\n  }\n\n  saveLeaveDetails(data, error, template) {\n    this.leavereason = '';\n    this.isSave = false;\n    this.editedData = data;\n    if ((data.openLeave === '' || data.openLeave === null) || (data.usedLeave === '' || data.usedLeave === null) || (data.creditedLeave === '' || data.creditedLeave === null)) {\n      data.isSubmit = true;\n    } else {\n      data.isSubmit = false;\n    }\n    if (!data.isSubmit) {\n      this.openModal(template);\n      this.getuserhistoryinfo();\n    }\n  }\n  openmassupdateModal(massupdate: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(massupdate, { class: 'modal-sm' });\n  }\n\n  openleaveChange(data, obj, str, e) {\n    const pattern = /[0-9\\.\\ ]/;\n    const inputChar = String.fromCharCode(e.charCode);\n    if (e.keyCode != 8 && !pattern.test(inputChar)) {\n      event.preventDefault();\n    }\n    obj.balanceLeave = (Number(obj.openLeave) + Number(obj.creditedLeave)) - Number(obj.usedLeave);\n  }\n\n  massUpdate(massupdate) {\n    this.openModal(massupdate)\n    this.validlocation = false;\n    this.validleave = false;\n    this.validnoOfDays = false;\n    this.emptyfields = false;\n    this.clicked = true;\n    this.checkedValue = 'bulk-upload';\n    this.checkedValueFlag = true;\n    this.locationselected = 'Select Location'\n    this.leaveselected = 'Select Leave'\n    this.noOfDays = '';\n  }\n  edit(compoffTemp, leavetype, obj) {\n    this.tempObj = obj;\n    var arrtemp = [];\n    this.initialcompOff = false;\n    if (leavetype == 'Compensatory Off') {\n      if (!this.saveclick) {\n        this.getcompoff('');\n      }\n      if (this.saveclick) {\n        this.getcompoff('save');\n      }\n      this.addedCompOff = [];\n      this.addCompOff('');\n      setTimeout(() => {\n        this.openModal(compoffTemp);\n      }, 300);\n    }\n  }\n\n  addCompOff(str: any) {\n    this.initialcompOff = str == 'add' ? true : false;\n    if (this.addedCompOff.length != 0 && this.addedCompOff[this.addedCompOff.length - 1].compoffdate == '') {\n      return false;\n    }\n    var obj = {\n      'compoffdate': ''\n    }\n    this.addedCompOff.push(obj);\n  }\n\n  /**checkbox change event */\n  revokedleaveDateChange(str, cnt) {\n    var count = 0;\n    var val = []; var arr = [];\n    var uncheckcompOff = $('.compOffCheck').not(':checked').length;\n    var checkcompOff = $('.compOffCheck:checked').length;\n    var revokeTemp = [];\n    this.revokeTempCopy = [];\n    $('.compOffCheck:checked').each(function (i) {\n      val[i] = $(this).val();\n      revokeTemp.push($(this).val());\n      $(this).prop('checked', true);\n    });\n    this.revokeTempCopy = revokeTemp;\n    this.revokeDisable = val.length != 0 ? false : true;\n    var arrtemp = [];\n    var tempFlag = false;\n    if (val != undefined && val.length != 0) {\n      val.filter(elem => {\n        for (var i = 0; i < this.listofcompoffdates.length; i++) {\n          if (elem == this.listofcompoffdates[i].leaveCOFFDates && $(event.target).val() != elem) {\n            this.listofcompoffdates[i].check = true;\n            arrtemp.push(this.listofcompoffdates[i]);\n          }\n        }\n        return this.listofcompoffdates\n      });\n\n    }\n    var arr: any[];\n    var flag = false;\n    var unrevoked = [];\n    $(\".compOffCheck:not(:checked)\").each(function (i) {\n      unrevoked = [];\n      arr[i] = $(this).val();\n      if ($(event.target)[0].innerHTML != 'Revoke' && !$(event.target)[0].checked) {\n        flag = true;\n        unrevoked.push($(event.target)[0].value)\n      }\n    });\n    if (arr.length != 0) {\n      arr.filter(el => {\n        for (var i = 0; i < this.listofcompoffdates.length; i++) {\n          if (el == this.listofcompoffdates[i].leaveCOFFDates) {\n            this.listofcompoffdates[i].check = false;\n            if (this.saveclick) {\n              this.tempArr.splice(i, 1)\n            }\n            tempFlag = true;\n          }\n        }\n      })\n    }\n    this.tempObj['creditedLeaveCopy'] = this.tempObj.creditedLeave;\n    //for count display method\n    var displayArr = [];\n    displayArr.push($(event.target)[0].value);\n    if (str != undefined && str == 'revoked') {\n      if (flag) {\n        this.tempObj.creditedLeave = this.tempObj.creditedLeave + unrevoked.length;\n      } else {\n        this.tempObj.creditedLeave = this.tempObj.creditedLeave - revokeTemp.length;\n        revokeTemp = [];\n      }\n    } else if (this.revokeFocused && $(event.target)[0].innerHTML != 'Revoke' && !$(event.target)[0].checked) {\n      this.tempObj.creditedLeave = this.tempObj.creditedLeave + unrevoked.length;\n    }\n    var arrfilter = [];\n    this.leaveDates.forEach((checkDate, index) => {\n      var index = arr.indexOf(checkDate.leaveCOFFDates);\n      if (index != -1) {\n        arr.splice(index, 1);\n      } else {\n        arrfilter.push(checkDate.leaveCOFFDates)\n      }\n    });\n\n    this.tempSavedDates = this.saveclick && arr.length != 0 ? arr : [];\n    this.tempObj.balanceLeave = (Number(this.tempObj.openLeave) + Number(this.tempObj.creditedLeave)) - Number(this.tempObj.usedLeave);\n  }\n  /**revoke button event */\n  revokeCompOffDate() {\n    var count = 0;\n    var val = []; var arr = [];\n    this.revokeFocused = true;\n    $('.compOffCheck:checked').each(function (i) {\n      val[i] = $(this).val();\n      $(this).prop('checked', true);\n    });\n\n    this.tempArr = val;\n    var count = 0;\n    this.listofcompoffdates.forEach(checkarr => {\n      if (checkarr.check == false) {\n        count++;\n      }\n    });\n    var checkFlag = false;\n    this.listofcompoffdates.forEach((checkDate, index) => {\n      var key = this.tempArr.indexOf(checkDate.leaveCOFFDates);\n      if (key != -1) {\n        if (!checkDate.check) {\n          checkFlag = true;\n        }\n      }\n    });\n    var revokeCheck = false;\n    var dummycount = 0;\n    this.listofcompoffdates.forEach((checkDate, index) => {\n      var key = this.tempArr.indexOf(checkDate.leaveCOFFDates);\n      if (key != -1) {\n        if (!checkDate.check && this.revokeFocused) {\n          revokeCheck = true;\n          dummycount++;\n        }\n      }\n    });\n    if (checkFlag) {\n      this.revokedleaveDateChange('revoked', dummycount)\n    }\n    this.revokeDisable = this.tempArr.length != 0 ? false : true;\n    this.modalRef.hide();\n  }\n  compOffChange(event, obj) {\n    obj.notSave = false;\n    obj.alreadyExist = false;\n  }\n  setOrders() {\n    this.cloneOrders = [...this.addedCompOff];\n  }\n  saveCompOffDate() {\n    var obj = {};\n    var flag = false;\n    obj['leaveCOFFDates'] = '';\n    obj['check'] = '';\n    this.tempSavedDates = [];\n    this.tempSavedDatesCopy = [];\n    this.addedCompOff.forEach((element, index) => {\n      var obj = {};\n      obj['leaveCOFFDates'] = '';\n      obj['check'] = '';\n      if (element.compoffdate != '') {\n        this.tempSavedDates.push(moment(element.compoffdate).format('YYYY-MM-DD'));\n        this.tempSavedDatesCopy.push(moment(element.compoffdate).format('YYYY-MM-DD'));\n      }\n      obj['leaveCOFFDates'] = moment(element.compoffdate).format('YYYY-MM-DD');\n      obj['check'] = false;\n    });\n    this.compOffDatesCopy = [];\n    this.addedCompOffCopy = [];\n    this.listofcompoffdates.forEach((elem, index) => {\n      this.compOffDatesCopy.push(elem.leaveCOFFDates);\n    });\n    this.tempSavedDates.forEach((elem, index) => {\n      var obj = {};\n      obj['leaveCOFFDates'] = '';\n      obj['check'] = false;\n      if (this.compOffDatesCopy.indexOf(elem) == -1) {\n        this.compOffDatesCopy.push(elem);\n        obj['leaveCOFFDates'] = elem;\n        obj['check'] = false;\n        this.listofcompoffdates.push(obj);\n        if (this.addedCompOff[index]['alreadyExist'] != undefined || this.addedCompOff[index]['alreadyExist'] == undefined) {\n          this.addedCompOff[index]['alreadyExist'] = false;\n        }\n      } else {\n        flag = true;\n        this.addedCompOff[index]['alreadyExist'] = true;\n      }\n    });\n    this.addedCompOff.forEach((elem, index) => {\n      if (elem['alreadyExist'] != undefined && !elem['alreadyExist']) {\n        this.addedCompOffCopy.push(elem);\n      }\n      if (elem.compoffdate == '') {\n        elem['notSave'] = true;\n\n      } else {\n        elem['notSave'] = false;\n      }\n      if (elem.notSave || elem.alreadyExist) {\n        flag = true;\n      }\n    });\n    var result;\n    function comparer(otherArray) {\n      return function (current) {\n        return otherArray.filter(function (other) {\n          return other.compoffdate == current.compoffdate && other.alreadyExist == current.alreadyExist\n        }).length == 0;\n      }\n    }\n    var onlyInA = this.addedCompOff.filter(comparer(this.addedCompOffCopy));\n    var onlyInB = this.addedCompOffCopy.filter(comparer(this.addedCompOff));\n    result = onlyInA.concat(onlyInB);\n    this.addedCompOff = result;\n    if (!flag) {\n      this.initialcompOff = false;\n      this.modalRef.hide();\n    } else {\n      this.initialcompOff = true;\n    }\n    var count = 0;\n    this.listofcompoffdates.forEach(checkarr => {\n      if (checkarr.check == false) {\n        count++;\n      }\n    });\n    var uncheckcompOff = $('.compOffCheck').not(':checked').length;\n    if (this.addedCompOff.length == 0) {\n      this.tempObj.creditedLeave = this.tempObj.creditedLeave + this.tempSavedDates.length;\n    } else {\n      var countsave = 0; var dataDates;\n      this.addedCompOff.forEach((elem, index) => {\n        dataDates = this.tempSavedDates.filter(ele => {\n          if (ele != moment(elem.compoffdate).format('YYYY-MM-DD')) {\n            countsave++;\n            return ele;\n          }\n        });\n      });\n      this.tempObj.creditedLeave = this.tempObj.creditedLeave + countsave;\n    }\n    this.tempObj.balanceLeave = (Number(this.tempObj.openLeave) + Number(this.tempObj.creditedLeave)) - Number(this.tempObj.usedLeave);\n    this.saveclick = true;\n    this.listofcompoffdatesCopy = this.listofcompoffdates;\n    this.revokeDisable = (this.tempArr.length != 0 || $('.compOffCheck:checked').length != 0) ? false : true;\n  }\n  cancelCompOffDate(obj, index) {\n    this.addedCompOff.splice(index, 1);\n  }\n\n  adminMonthChange(event, pickerName) {\n    if (event.month == 1) {\n      this.adminDateNav = new Date(event.year, 0, 1);\n    } else {\n      this.adminDateNav = new Date(event.year, event.month - 1, 1);\n    }\n    if (this.dateAdminFrom.getMonth() == 1) {\n      this.toPrevDisable = new Date(this.dateAdminFrom.getFullYear(), 0, 1);\n    } else {\n      this.toPrevDisable = new Date(this.dateAdminFrom.getFullYear(), this.dateAdminFrom.month - 1, 1);\n    }\n\n    if (this.adminDateNav.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {\n      $(\".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else if (this.adminDateNav.getTime() < this.dateAdminFrom.getTime() && pickerName == 'to') {\n      $(\".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"none\");\n    } else {\n      $(\".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all\").css(\"pointer-events\", \"all\");\n      $(\".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all\").css(\"pointer-events\", \"all\");\n    }\n  }\n  adminReport(adminreport) {\n    this.openModal(adminreport)\n    this.validateDate = false;\n    this.validateFromDate = false;\n    this.dateAdminFrom = '';\n    this.dateAdminTo = '';\n    this.validateToDate = false;\n  }\n  uploadselection(val) {\n    this.checkedValueFlag = val == 'bulk-upload' ? true : false;\n    this.clicked = true;\n  }\n  uploadFiles(uploaderror) {\n    this.bulkupload(uploaderror);\n    if (this.fileSelected.name != '') {\n      if (this.fileSelected.name.includes('.')) {\n        var xlsfile = this.fileSelected.name.split('.')[1];\n        if (xlsfile == 'xls' || xlsfile == 'xlsx') {\n          this.xlscheck = false;\n          this.clicked = true;\n        } else {\n          this.xlscheck = true;\n        }\n      }\n    }\n  }\n  bulkupload(uploaderror) {\n    this.spinnerService.show();\n    this.httpService.post('/vms/leaveCredit/bulkUpload?employeeNumber=' + this.service.employeeNumber, this.frmData).subscribe(\n      data => {\n        if (data['status'] == 'Success') {\n          this.modalRef.hide();\n          this.toastr.success(data['successMessage']);\n          this.fileSelected = '';\n          this.frmData = new FormData();\n          this.clicked = false;\n        } if (data['status'] === 'Error') {\n          this.modalRef.hide();\n          this.openModal(uploaderror);\n          this.errormessage = data['errorMessage'];\n          this.fileSelected = '';\n          this.clicked = false;\n          this.frmData = new FormData();\n        }\n        this.spinnerService.hide();\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n\n  uploadFiless(e) {\n    this.fileSelected = e.target.files[0];\n    this.clicked = false;\n    this.frmData.append(\"file\", this.fileSelected, this.fileSelected.name);\n  }\n  onCreditEmployeeChange(selectedName: string, empId) {\n    $('#emp-name-search').show();\n    this.isvalidFreeze = false;\n    this.cancelCredit = false;\n    let temp = [];\n    this.leavesummaryList = [];\n    this.showSummary = false;\n    this.currentYearFilterArr, this.currentYearFilterArr2 = [];\n    this.dateJoining = false;\n  }\n  leaveTypecredit(id) {\n    const url = CONSTANT.GET_LEAVE_TYPE;\n    this.locationWiseId = Number(id);\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"locationId\": id\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.leaveCreditType = data.leaveType;\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n  leavecreditchange(event) {\n    this.locationLeaveId = Number(event);\n    this.validleave = false;\n    if (this.locationselected != 'Select Location' && this.leaveselected != 'Select Leave') {\n      this.daysDisable = false;\n    }\n  }\n  leavechangeevent(event) {\n    this.leaveTypecredit(event);\n    this.validlocation = false;\n    if (this.locationselected != 'Select Location' && this.leaveselected != 'Select Leave') {\n      this.daysDisable = false;\n    }\n  }\n  nofdayschangeevent(event) {\n    this.validnoOfDays = false;\n    this.emptyfields = false;\n  }\n  massupdatevalidationEvent() {\n    if (this.noOfDays != undefined && this.noOfDays != '' && this.leaveselected != 'Select Leave' && this.locationselected != 'Select Location') {\n      this.validleave = false;\n      this.validnoOfDays = false;\n      this.emptyfields = false;\n      this.validlocation = false;\n    } else if ((this.noOfDays != undefined || this.noOfDays == undefined || this.noOfDays == '') && this.leaveselected == 'Select Leave' && this.locationselected == 'Select Location') {\n      this.validleave = true;\n      this.validnoOfDays = true;\n      this.emptyfields = true;\n      this.validlocation = true;\n    } else if ((this.noOfDays != undefined || this.noOfDays == undefined || this.noOfDays == '') && this.noOfDays == '' && this.leaveselected != 'Select Leave' && this.locationselected != 'Select Location') {\n      this.validleave = false;\n      this.emptyfields = false;\n      this.validnoOfDays = true;\n      this.validlocation = false;\n    } else if (this.noOfDays != undefined || this.noOfDays != '' && this.leaveselected == 'Select Leave' && this.locationselected != 'Select Location') {\n      this.validleave = true;\n      this.emptyfields = false;\n      this.validnoOfDays = false;\n      this.validlocation = false;\n    } else if (this.noOfDays != undefined || this.noOfDays != '' && this.leaveselected != 'Select Leave' && 