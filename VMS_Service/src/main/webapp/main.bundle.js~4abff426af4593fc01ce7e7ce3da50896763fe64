    function replacingDecimalValue() {
    }
    replacingDecimalValue.prototype.transform = function (value) {
        if (value != '' && value != null) {
            if (typeof (value) == 'string') {
                if (value.includes('.')) {
                    return value.split('.')[0];
                }
                else {
                    return value;
                }
            }
            else {
                return value.toFixed(2);
            }
