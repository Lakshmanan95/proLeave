    ReviewComponent.prototype.exportCSV = function () {
        if (this.repleavefrom == null || this.reportData['leavefrom'] == undefined && this.repleavefrom == undefined && this.repappliedDate == null && this.repapprove == null && this.reviewedOn == null && this.repleaveType == null && this.selectedEmployeeId == undefined) {
            this.repleavefrom = __WEBPACK_IMPORTED_MODULE_1_moment__().utc().startOf('month').format('YYYY-MM-DD');
        }
        else if (this.repleavefrom != null && this.repleavefrom != undefined && this.repappliedDate != null && this.repapprove != null && this.reviewedOn != null && this.repleaveType != null && this.selectedEmployeeId != undefined) {
            this.repleavefrom = this.reportData['leavefrom'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.reportData['leavefrom']).format('YYYY-MM-DD').toString() : null;
        }
        if (this.repleaveto == null || this.reportData['leaveto'] == undefined && this.repleaveto == undefined && this.repappliedDate == null && this.repapprove == null && this.reviewedOn == null && this.repleaveType == null && this.selectedEmployeeId == undefined) {
            this.repleaveto = __WEBPACK_IMPORTED_MODULE_1_moment__().utc().endOf('month').format('YYYY-MM-DD');
        }
        else if (this.repleaveto != null && this.repleaveto != undefined && this.repappliedDate != null && this.repapprove != null && this.reviewedOn != null && this.repleaveType != null && this.selectedEmployeeId != undefined) {
            this.repleaveto = this.reportData['leaveto'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.reportData['leaveto']).format('YYYY-MM-DD').toString() : null;
        }
        this.repapprove = (this.approveId == undefined ? null : this.approveId);
        this.repappliedDate = this.reportData['appliedDate'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.reportData['appliedDate']).format('YYYY-MM-DD').toString() : null;
        this.reviewedOn = this.reportData['reviewedDate'] != undefined ? __WEBPACK_IMPORTED_MODULE_1_moment__(this.reportData['reviewedDate']).format('YYYY-MM-DD').toString() : null;
        this.repleaveType = (this.leaveTypeId == undefined ? null : this.leaveTypeId);
        var format = this.selectedFlag ? 'csv' : 'pdf';
        var toggling = this.toggle ? 'DR' : 'IH';
        var rowsData = this.checkedRadioFlag ? 0 : 1;
        var empId = this.isSubmited ? this.selectedEmployeeId : undefined;
        var approve = this.isSubmited ? this.repapprove : null;
        var review = this.isSubmited ? this.reviewedOn : null;
        var appliedOn = this.isSubmited ? this.repappliedDate : null;
        var leaveType = this.isSubmited ? this.repleaveType : null;
        var url = "/vms/finance/exportLeaveBalance";
        var data = "loginEmployeeCode=" + this.service.employeeNumber + "&leaveReviewedOn=" + review + "&leaveStatusId=" + approve +
            "&leaveTypeId=" + leaveType + "&leaveAppliedOn=" + appliedOn + "&leaveToDate=" + this.repleaveto + "&leaveFromDate=" + this.repleavefrom
            + "&byRows=" + rowsData + "&fileFormat=" + format + "&employeeCode=" + empId + "&flag=" + toggling;
        window.location.href = url + "?" + data;
        if (this.checkedDropdown == 'csv') {
            this.modalRef.hide();
            this.checkedRadioFlag = true;
        }
    };
    ///////////////////////////////////////////////////////////// Download leave balance report Section ////////////////////////////////////
