s.validateDate && !this.validateFromDate && !this.validateToDate) {\n      const url = CONSTANT.ADMIN_REPORT\n      this.fromdateadmin = this.dateAdminFrom != undefined ? moment(this.dateAdminFrom).format('YYYY-MM-DD') : null;\n      this.todateadmin = this.dateAdminTo != undefined ? moment(this.dateAdminTo).format('YYYY-MM-DD') : null;\n      let data = \"employeeNumber=\" + null + \"&fromDate=\" + this.fromdateadmin + \"&toDate=\" + this.todateadmin + \"&flag=\" + \"ADMRPT\";\n      window.location.href = CONSTANT.ADMIN_REPORT + \"?\" + data;\n      this.modalRef.hide();\n    }\n\n\n  }\n\n  adminreportvalidationEvent(str) {\n    if (this.dateAdminFrom != '' && this.dateAdminTo != '') {\n      this.validateDate = false;\n      this.validateFromDate = false;\n      this.validateToDate = false;\n      ///service calll \n      if (str == 'extract') {\n\n      }\n    } else if (this.dateAdminFrom == '' && this.dateAdminTo == '') {\n      this.validateDate = true;\n    } else if (this.dateAdminFrom == '' && this.dateAdminTo != '') {\n      this.validateDate = false;\n      this.validateFromDate = true;\n      this.validateToDate = false;\n    } else if (this.dateAdminFrom != '' && this.dateAdminTo == '') {\n      this.validateDate = false;\n      this.validateFromDate = false;\n      this.validateToDate = true;\n    }\n  }\n\n  employeeLeaveBalance(empId) {\n    const url = CONSTANT.EMPLOYEE_LEAVE_BALANCE;\n\n    let data = {\n      \"employeeNumber\": empId\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.employeeLeaveListsCopy = data.EmpLeaveBalaceInfo;\n        this.employeeLeaveLists = data.EmpLeaveBalaceInfo;\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n\n  }\n\n  getuserhistoryinfo() {\n    const url = CONSTANT.USER_HISTORY_INFO;\n    let data = {\n      // \"employeeNumber\": this.service.employeeNumber,\n      \"employeeNumber\": this.selectedEmployeeId,\n      \"fromDate\": moment().subtract('year', 1).format('YYYY') + '-' + '01' + '-' + '01',\n      \"toDate\": moment().format('YYYY') + '-' + '12' + '-' + '31',\n      \"appliedDate\": null,\n      \"numberOfDays\": null,\n      \"leaveType\": this.templeaveCode,\n      \"approvedDate\": null,\n      \"leaveStatus\": null\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.summaryHistory = data;\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      });\n  }\n\n  creditcancel(data, obj) {\n    this.cancelCredit = false;\n    this.dateJoining = false;\n    this.revokeDisable = true;\n    this.saveclick = false;\n    this.templeaveCode = '';\n    this.creditIds = [];\n    this.creditObj = {};\n    this.addedCompOff = [];\n    this.tempSavedDates = [];\n    if (data != '') {\n      obj.employeeName = '';\n      this.getloggeduserinfo();\n      $('.creditleave').removeAttr('disabled');//url(/assets/img/search.png) 97% / 6% no-repeat #ffff\n      $('.creditleave').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');\n\n    }\n    obj.isSubmit = false;\n    this.getcompoff('');\n  }\n\n  getleavecreditupdate(obj, error, template) {\n    this.isSave = this.leavereason == '' ? true : false;\n    if (!this.isSave) {\n      const url = CONSTANT.INSERT_UPDATE_CREDIT\n      let data = {\n        \"loginEmployeeCode\": this.service.employeeNumber,\n        \"employeeNumber\": this.selectedEmployeeId,\n        \"comments\": this.leavereason,\n        \"leaveTypeId\": Number(this.editedData.leaveTypeId),\n        \"leaveOpen\": Number(this.editedData.openLeave),\n        \"leaveCredit\": Number(this.editedData.creditedLeave),\n        \"leaveUsed\": Number(this.editedData.usedLeave),\n        \"compoffDate\": this.tempSavedDates.length != 0 ? this.tempSavedDates.join(',') : null,\n        \"revokeCompoffDate\": this.tempArr.length != 0 ? this.tempArr.join(',') : null\n      }\n      this.spinnerService.show();\n\n      this.service.postService(url, data)\n        .subscribe(data => {\n          if (data.status == 'Success') {\n            this.modalRef.hide();\n            this.templeaveCode = '';\n            this.getcompoff('');\n            this.toastr.success(data.successMessage);\n            this.employeeLeaveBalance(this.selectedEmployeeId);\n            this.editdetail = false;\n            this.saveclick = false;\n            this.getLeaveHistory(this.selectedEmployeeId);\n            this.tempSavedDates = [];\n            this.getuserhistoryinfo();\n            this.tempArr = [];\n            this.revokeDisable = true;\n          } if (data.status === 'Error') {\n            this.modalRef.hide();\n            this.openModal(error);\n            this.errormessage = data.errorMessage;\n          }\n          this.spinnerService.hide();\n        }, error => {\n          console.log(error);\n          if (error.status == 0) {\n            window.location.reload();\n\n          }\n        });\n    }\n\n\n  }\n\n\n  getLeaveHistory(empId) {\n    const url = CONSTANT.LEAVE_CREDIT_HISTORY\n    let data = {\n      \"employeeNumber\": empId,\n      \"fromDate\": null,\n      \"toDate\": null,\n      \"flag\": \"HISRPT\"\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.leaveHistory = data.EmpLeaveHistoryInfo;\n        this.leaveHistory.forEach((history, index) => {\n          this.creditId = history.leaveCreditId;\n          if (this.creditIds.length == 0) {\n            this.creditIds.push(history.leaveCreditId);\n          } else if (this.creditIds.length != 0) {\n            if (this.creditIds.indexOf(history.leaveCreditId) == -1) {\n              this.creditIds.push(history.leaveCreditId);\n            }\n          }\n          var serial = history.sno;\n        });\n        var filteredData; this.leaveHistorycopy = [];\n        this.creditIds.forEach(element => {\n          var obj = {};\n          obj['createdByDate'] = '';\n          obj['createdByName'] = '';\n          obj['sno'] = '';\n          obj['leaveType'] = '';\n          obj['comments'] = '';\n          obj['leaveCategory'] = '';\n          obj['leaveCommentsMerge'] = [];\n          filteredData = this.leaveHistory.filter((elem, key) => {\n            if (elem.leaveCreditId == element) {\n              return elem;\n            }\n          });\n          var textMerge = '';\n          if (filteredData.length > 1) {\n            obj['createdByDate'] = filteredData[0].createdByDate;\n            obj['createdByName'] = filteredData[0].createdByName;\n            obj['sno'] = filteredData[0].sno;\n            obj['leaveType'] = filteredData[0].leaveType;\n            obj['comments'] = filteredData[0].comments;\n            obj['leaveCategory'] = filteredData[0].leaveCategory;\n            var arr1 = [];\n            filteredData.forEach((value, index) => {\n              var temp = value.leaveType + \" <b>\" + value.leaveCategory.toUpperCase() + \"</b> changed from \" + value.existingValue + \" to \" + value.newValue;\n              arr1.push(temp);\n            });\n            obj['leaveCommentsMerge'] = arr1;\n            this.leaveHistorycopy.push(obj);\n          } else if (filteredData.length != 0 && filteredData.length == 1) {\n            var arr = [];\n            obj['createdByDate'] = filteredData[0].createdByDate;\n            obj['createdByName'] = filteredData[0].createdByName;\n            obj['sno'] = filteredData[0].sno;\n            obj['leaveType'] = filteredData[0].leaveType;\n            obj['comments'] = filteredData[0].comments;\n            obj['leaveCategory'] = filteredData[0].leaveCategory;\n            var temp = filteredData[0].leaveType + \" <b>\" + filteredData[0].leaveCategory.toUpperCase() + \"</b> changed from \" + filteredData[0].existingValue + \" to \" + filteredData[0].newValue;\n            arr.push(temp);\n            obj['leaveCommentsMerge'] = arr;\n            this.leaveHistorycopy.push(obj);\n          }\n        });\n        this.leaveHistorycopy.sort((a: any, b: any) =>\n          a.sno - b.sno\n        );\n\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n\n  leaveonclick() {\n    this.selectedAll = undefined;\n  }\n\n  decimalInput(e, val, str, leavecode) {\n    if (str != undefined && str == \"leavetype\" && (leavecode == 'PL' || leavecode == 'PTO' || leavecode == 'APTO')) {\n      const pattern = /[0-9\\.\\ ]/;\n      if (e.keyCode === 46 && val.split('.').length === 2) {\n        return false;\n      }\n      const inputChar = String.fromCharCode(e.charCode);\n      if (e.keyCode != 8 && !pattern.test(inputChar)) {\n        event.preventDefault();\n      }\n    }\n  }\n  decimalValCheck(e, val) {\n    const pattern = /[0-9\\.\\ ]/;\n    if (e.keyCode === 46 && val.split('.').length === 2) {\n      return false;\n    }\n    const inputChar = String.fromCharCode(e.charCode);\n    if (e.keyCode != 8 && !pattern.test(inputChar)) {\n      event.preventDefault();\n    }\n  }\n  getmassleaveupdate(leaveerror) {\n    this.massupdatevalidationEvent();\n    if (this.noOfDays != '' && this.noOfDays != undefined && this.leaveselected != 'Select Leave' && this.locationselected != 'Select Location') {\n      const url = CONSTANT.MASS_LEAVE_CREDIT;\n      if (this.leaveselected == 2 || this.leaveselected == 4) {\n        this.noOfDays = (Math.round(Number(this.noOfDays) * 100) / 100).toFixed(2);\n      }\n      let data = {\n        // \"employeeNumber\": \"\",\n        \"employeeCode\": this.service.employeeNumber,\n        \"locationId\": this.locationWiseId,\n        \"leaveTypeId\": this.locationLeaveId,\n        \"noOfDays\": this.noOfDays\n      }\n      this.service.postService(url, data)\n        .subscribe(data => {\n          if (data.status == 'Success') {\n            this.modalRef.hide();\n            this.leaveselected = 'Select Leave'\n            this.locationselected = 'Select Location'\n            this.noOfDays = ''\n            this.toastr.success(data.successMessage);\n          } if (data.status === 'Error') {\n            this.modalRef.hide();\n            this.leaveselected = 'Select Leave'\n            this.locationselected = 'Select Location'\n            this.noOfDays = ''\n            this.openModal(leaveerror);\n            this.errormessage = data.errorMessage;\n          }\n\n        }, error => {\n          console.log(error);\n          if (error.status == 0) {\n            window.location.reload();\n\n          }\n        })\n    }\n\n  }\n\n  getcompoff(str) {\n    const url = CONSTANT.COFF_UPDATE;\n    let data = {\n      \"employeeNumber\": this.selectedEmployeeId\n      // \"employeeNumber\": \"\"\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        data.LeaveCOFFDatesInfo.forEach((value, index) => {\n          value['check'] = false;\n        });\n        this.leaveDates = data.LeaveCOFFDatesInfo;\n        if (str == '') {\n          this.listofcompoffdates = data.LeaveCOFFDatesInfo;\n          var arrtemp = [];\n          if (this.tempArr != undefined && this.tempArr.length != 0) {\n            this.tempArr.filter(elem => {\n              for (var i = 0; i < this.listofcompoffdates.length; i++) {\n                if (elem == this.listofcompoffdates[i].leaveCOFFDates && this.revokeFocused) {\n                  this.listofcompoffdates[i].check = true;\n                  arrtemp.push(this.listofcompoffdates[i]);\n                }\n              }\n              return this.listofcompoffdates\n            });\n          }\n          this.saveclick = false;\n          this.revokeDisable = arrtemp.length != 0 ? false : true;\n        } else if (str == 'save') {\n          if (this.tempArr.length == 0) {\n            this.listofcompoffdatesCopy.forEach((elem, index) => {\n              if (elem.check) {\n                this.listofcompoffdatesCopy[index].check = false;\n              }\n            });\n            this.listofcompoffdates = this.listofcompoffdatesCopy;\n          }\n          var arrtemp = [];\n          if (this.tempArr != undefined && this.tempArr.length != 0) {\n            this.tempArr.filter(elem => {\n              for (var i = 0; i < this.listofcompoffdates.length; i++) {\n                if (elem == this.listofcompoffdates[i].leaveCOFFDates && this.revokeFocused) {\n                  this.listofcompoffdates[i].check = true;\n                  arrtemp.push(this.listofcompoffdates[i]);\n                }\n              }\n              return this.listofcompoffdates\n            });\n          }\n          this.revokeDisable = this.tempArr.length != 0 ? false : true;\n        }\n        var count = 0;\n        //checking unchecked count values in list of dates\n        this.listofcompoffdates.forEach(element => {\n          if (!element.check) {\n            count++;\n          }\n        });\n        if (this.tempObj != null && !this.cancelCredit) {\n          if (Object.keys(this.tempObj) != undefined && Object.keys(this.tempObj).length != 0) {\n            this.tempObj.creditedLeave = Number(this.tempObj.creditedLeave) + arrtemp.length;\n            this.tempObj.balanceLeave = (Number(this.tempObj.openLeave) + Number(this.tempObj.creditedLeave)) - Number(this.tempObj.usedLeave);\n\n          }\n        }\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n  //////////////////////////////////// Auto Approval Functionalities//////////////////////////////////////////////\n  getAutoApprovalData() {\n    const url = CONSTANT.GET_AUTO_APPROVAL;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber\n    }\n    this.service.postService(url, data).subscribe(data => {\n 