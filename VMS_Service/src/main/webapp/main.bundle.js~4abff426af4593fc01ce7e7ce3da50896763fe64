    };
    AdminComponent.prototype.autoapprove = function (obj) {
        $('#emp-name-auto-search').hide();
        this.autoCredit = obj.employeeName != '' ? true : false;
        this.data['autoEmployeeName'] = obj.employeeName;
        this.selectedautoEmployeeId = obj.employeeId;
        this.selectAddAutoEmployeeId = obj.employeeCode;
        if (obj.employeeName != '') {
            $('.creditleaveauto').attr('disabled', 'disabled');
            $('.creditleaveauto').css('background', 'none');
        }
        else {
            $('.creditleaveauto').removeAttr('disabled'); //url(/assets/img/search.png) 97% / 6% no-repeat #ffff
            $('.creditleaveauto').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
        }
    };
    AdminComponent.prototype.autoapprovalcancel = function (data, obj) {
        this.autoCredit = false;
        if (data != '') {
            obj.autoEmployeeName = '';
            this.getAutoApprovalData();
            $('.creditleaveauto').removeAttr('disabled'); //url(/assets/img/search.png) 97% / 6% no-repeat #ffff
            $('.creditleaveauto').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
        }
    };
    AdminComponent.prototype.addAutoAprrovalRecord = function (autouploaderror) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].ADD_AUTO_APPROVAL;
        var data = {
            "loginEmployeeNumber": this.service.employeeNumber,
            "employeeNumber": this.selectAddAutoEmployeeId,
            "isActive": 1
        };
        this.service.postService(url, data).subscribe(function (data) {
            if (data.status == 'SUCCESS') {
                _this.toastr.success(data.successMessage);
                _this.getAutoApprovalTableData();
                _this.data['autoEmployeeName'] = '';
                $('.creditleaveauto').removeAttr('disabled');
                $('.creditleaveauto').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
            }
            if (data.status === 'ERROR-001') {
                $('.creditleaveauto').removeAttr('disabled');
                $('.creditleaveauto').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
                _this.openModal(autouploaderror);
                _this.errormessage = data.errorMessage;
            }
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.deleteAutoAprrovalRecord = function (obj, i) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].ADD_AUTO_APPROVAL;
        this.selectdeleteAutoEmployeeId = obj;
        var data = {
            "loginEmployeeNumber": this.service.employeeNumber,
            "employeeNumber": this.selectdeleteAutoEmployeeId,
            "isActive": 0
        };
        this.service.postService(url, data).subscribe(function (data) {
            if (data.status == 'SUCCESS') {
                _this.toastr.success(data.successMessage);
                _this.getAutoApprovalTableData();
                _this.autoapprovalDatas.splice(i, 1);
                _this.data['autoEmployeeName'] = '';
            }
            if (data.status === 'ERROR-001') {
                _this.errormessage = data.errorMessage;
            }
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    /////////////////////////////////// Finance Report Section ////////////////////////////////////
    AdminComponent.prototype.getcompanyname = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].FINANCE_REPORT_ENTITY;
        var data = {};
        this.service.getService(url, data).subscribe(function (data) {
            _this.companyName = data.companyName;
            _this.reportName = data.reportName;
            _this.nameReport = data.reportName[1];
            var arrlen = data.leaveType.length;
            var count = 0;
            data.leaveType.forEach(function (element) {
                element['checked'] = true;
                count++;
            });
            _this.overallSelect = arrlen == count ? true : false;
            _this.financeLeave = data.leaveType;
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.financeselection = function (val) {
        this.checkedFinanceFlag = val == 'entity' ? true : false;
        this.selectedColor = false;
        this.isvalidReport = false;
        this.isvalidCmp = false;
        this.isvalidEmp = false;
        this.cmpName = 'Select Entity';
        this.reporttype = 'Select Report';
        this.dateFinanceFrom = new Date();
        this.dateFinanceTo = new Date();
        this.data['fincemployeeName'] = '';
        this.overallSelect = true;
        this.financeLeave.forEach(function (element) {
            element['checked'] = true;
        });
    };
    AdminComponent.prototype.cancelCompany = function (str) {
        if (str != 'Select Entity') {
            this.cmpName = 'Select Entity';
            if (this.cmpName != '' && this.cmpName != 'Select Entity') {
                $('#selectcmp').removeClass('fin-location');
                $('#selectcmp').attr('disabled', 'disabled');
                $('#selectcmp').css('background', 'none');
            }
            else {
                $('#selectcmp').removeAttr('disabled');
                $('#selectcmp').addClass('fin-location');
                $('#selectcmp').css('background', 'url(../img/down-arrow.png) 98% / 3% no-repeat #ffff;');
            }
        }
    };
    AdminComponent.prototype.cancelReport = function (str) {
        if (str != 'Select Report') {
            this.reporttype = 'Select Report';
            if (this.reporttype != '' && this.reporttype != 'Select Report') {
                $('#selectrep').removeClass('repo-location');
                $('#selectrep').attr('disabled', 'disabled');
                $('#selectrep').css('background', 'none');
            }
            else {
                $('#selectrep').removeAttr('disabled');
                $('#selectrep').addClass('repo-location');
                $('#selectrep').css('background', 'url(../img/down-arrow.png) 98% / 3% no-repeat #ffff;');
            }
        }
    };
    AdminComponent.prototype.finaMonthChange = function (event, pickerName) {
        if (event.month == 1) {
            this.currentDateFreeze = new Date(event.year, 0, 1);
        }
        else {
            this.currentDateFreeze = new Date(event.year, event.month - 1, 1);
        }
        if (this.dateFinanceFrom.getMonth() == 1) {
            this.toPrevDisable = new Date(this.dateFinanceFrom.getFullYear(), 0, 1);
        }
        else {
            this.toPrevDisable = new Date(this.dateFinanceFrom.getFullYear(), this.dateFinanceFrom.month - 1, 1);
        }
        if (this.currentDateFreeze.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "none");
        }
        else if (this.currentDateFreeze.getTime() < this.dateFinanceFrom.getTime() && pickerName == 'to') {
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "none");
        }
        else {
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "all");
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "all");
        }
    };
    AdminComponent.prototype.finanOnSelect = function (event, pickerName) {
        if (pickerName == "from") {
            this.dateFinanceTo = this.dateFinanceFrom;
        }
    };
    AdminComponent.prototype.ExtractEntity = function (financeerror, Infotemplate) {
        var _this = this;
        this.financeValidation();
        if (this.checkedFinance == 'employee') {
            this.isvalidEmp = ((this.data['fincemployeeName'] == '' && this.reporttype != 'India Sales') || this.data['fincemployeeName'] == undefined) ? true : false;
            this.reporttype != 'Select Report';
        }
        this.extractFinace = [];
        this.extractFinaceLeave = [];
        if ((((this.cmpName != 'Select Entity') || (this.data['fincemployeeName'] != undefined && this.data['fincemployeeName'] != '') || (this.data['fincemployeeName'] == undefined || this.data['fincemployeeName'] == '')) && this.reporttype != 'Select Report') || (this.reporttype != 'Select Report' && this.checkedFinance == 'inactive')) {
            this.financeLeave.forEach(function (element) {
                if (element.checked) {
                    _this.extractFinace.push(element);
                    _this.extractFinaceLeave.push(element.leaveTypeId);
                }
            });
            if (this.extractFinace.length == 0) {
                this.errormessage = "Atleast one leave type should be selected";
                this.modalRef1 = this.modalService.show(financeerror, { class: 'modal-sm' });
            }
            var arrr = this.financeEmployeescopy.filter(function (el) {
                if (el.employeeName == _this.data['fincemployeeName'] || el.employeeId == _this.data['fincemployeeName']) {
                    return el;
                }
            });
            if (this.extractFinace.length != 0 && (((this.cmpName != 'Select Entity') || (this.data['fincemployeeName'] != undefined && this.data['fincemployeeName'] != '') || (this.checkedFinance == 'employee' && this.data['fincemployeeName'] == undefined || this.data['fincemployeeName'] == '' && this.reporttype == 'India Sales')) && this.reporttype != 'Select Report')) {
                var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].EXTRACT_ENTITY;
                this.fromdatefinan = this.dateFinanceFrom != undefined ? __WEBPACK_IMPORTED_MODULE_6_moment__(this.dateFinanceFrom).format('YYYY-MM-DD') : null;
                this.todatefinan = this.dateFinanceTo != undefined ? __WEBPACK_IMPORTED_MODULE_6_moment__(this.dateFinanceTo).format('YYYY-MM-DD') : null;
                var checkTemp = this.checkedFinance == 'employee';
                var data = void 0;
                if ((this.checkedFinance == 'entity' && this.cmpName != 'Select Entity' && this.reporttype != 'Select Report') || ((this.checkedFinance == 'employee' && this.data['fincemployeeName'] != undefined && this.data['fincemployeeName'] != '' && arrr.length != 0 && this.reporttype != 'Select Report') || (this.checkedFinance == 'employee' && this.data['fincemployeeName'] == undefined || this.data['fincemployeeName'] == '' && (arrr.length == 0) && this.reporttype == 'India Sales'))) {
                    data = ((checkTemp) ? "companyName=" : "companyName=") + ((checkTemp) ? null : (this.cmpName == 'Select Entity' ? null : this.cmpName)) + "&employeeNumber=" + ((checkTemp) ? this.selectedEmployeeId : null) + "&leaveTypeId=" + this.extractFinaceLeave.join(',') + "&reportType=" + this.reporttype + "&fromDate=" + this.fromdatefinan + "&toDate=" + this.todatefinan + "&activeStatus=" + "0";
                }
                else if (this.checkedFinance == 'inactive' && this.reporttype != 'Select Report' && this.cmpName != 'Select Entity') {
                    data = "&companyName=" + (this.cmpName == 'Select Entity' ? null : this.cmpName) + "&employeeNumber=" + ((checkTemp) ? this.selectedEmployeeId : null) + "&leaveTypeId=" + this.extractFinaceLeave.join(',') + "&reportType=" + this.reporttype + "&fromDate=" + this.fromdatefinan + "&toDate=" + this.todatefinan + "&activeStatus=" + "1";
                }
                if (data != undefined) {
                    window.location.href = url + "?" + data;
                    this.Infomessage = "Downloading... Please wait...";
                    this.modalRef1 = this.modalService.show(Infotemplate, { class: 'modal-sm' });
                    this.dateFinanceFrom = new Date();
                    this.dateFinanceTo = new Date();
                    this.cmpName = 'Select Entity';
                    this.reporttype = 'Select Report';
                    this.data['fincemployeeName'] = '';
                    this.overallSelect = true;
                    this.selectedColor = false;
                    this.financeLeave.forEach(function (element) {
                        element['checked'] = true;
                    });
