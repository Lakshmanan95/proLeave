ode.includes(model)) {\n        temp.push(obj);\n      }\n    });\n    this.reporteeListPendingRequest = temp;\n  }\n  onLeaveListSelect(selectedLeaveList: string) {\n    this.selectionColor = true;\n    return this.PendingLeaveRequests.find(userleave => userleave.leaveTypeName === selectedLeaveList);\n  }\n  ////////////////////////////////////// Leave Approve Reject //////////////////////////////////\n  processLeaveRequests(toProcess, action, reason) {\n    const url = CONSTANT.APPROVE_LEAVE_REQUEST_PROCESS;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"employeeId\": this.service.employeeId,\n      \"leaveRequestId\": toProcess,\n      \"leaveStatus\": action,\n      \"leaveReason\": reason,\n      \"contactNumber\": this.service.contactNumber,\n      \"numberOfDays\": \"\",\n      \"leaveTypeId\": \"\",\n      \"fromDate\": null,\n      \"toDate\": null\n    }\n    this.spinnerService.show();\n    this.click = true;\n    this.service.postService(url, data)\n      .subscribe(data => {\n        if (data[0].status == 'Success') {\n          this.toastr.success(data[0].successMessage);\n          this.itemsSelected = 0;\n          this.reason = '';\n          this.selectedAll = false;\n          this.click = true;\n          this.getPendingLeaveRequests();\n        }\n        else if (data[0].status == 'Error') {\n          this.toastr.error(data[0].errorMessage);\n        }\n        this.click = true;\n        this.spinnerService.hide();\n        this.searchReset();\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n\n  } //Leave Progress End\n\n  //////////////////////////// modal window for Approve & Reject /////////////////////////\n\n  singleleaverequest(leaveRequestId, numberOfDays, leaveCode, action, template, singleInfopopupTemplate, infopopupTemplate) {\n    this.leaveToProcessTest = leaveRequestId;\n    this.processAction = action;\n    var checkDaysFlag = false;\n    this.click = false;\n    if ((leaveCode == \"CL\") && (action == \"Approved\")) {\n      if (numberOfDays >= 3) {\n        checkDaysFlag = true;\n      }\n    }\n    if ((leaveCode == \"MDL\") && (action == \"Approved\")) {\n      if (numberOfDays >= 1) {\n        checkDaysFlag = true;\n      }\n    }\n    if (checkDaysFlag) {\n      this.SingleInfomessage = \"Kindly ensure to collect the medical certificate of the employee and share with HR before approving this leave!\"\n      this.modalRef1 = this.modalService.show(singleInfopopupTemplate, { class: 'modal-sm' });\n\n    } else {\n      this.openModal(template);\n    }\n  }\n\n  approveprocess(toProcess, action, template, infopopupTemplate) {\n    let selectedRow = [];\n    var checkDaysFlag = 0;\n    this.itemsSelected = 0;\n    this.click = false;\n    for (let i = 0; this.PendingLeaveRequests.length > i; i++) {\n      if (this.PendingLeaveRequests[i].selected) {\n        this.itemsSelected++;\n      }\n    }\n    toProcess.forEach((toProcessRow) => {\n      if (toProcessRow.selected && toProcessRow.selected === true) {\n        selectedRow.push(toProcessRow.leaveRequestId);\n        if (toProcessRow.leaveCode == \"CL\") {\n          if (toProcessRow.numberOfDays >= 3) {\n            checkDaysFlag++;\n          }\n        }\n        if (toProcessRow.leaveCode == \"MDL\") {\n          if (toProcessRow.numberOfDays >= 1) {\n            checkDaysFlag++;\n          }\n        } else {\n          if (checkDaysFlag > 0) {\n            checkDaysFlag++;\n          }\n        }\n      }\n    });\n    this.leaveToProcessTest = selectedRow.toString();\n    this.processAction = action;\n    if (checkDaysFlag > 1 && this.processAction == 'Approved') {\n      this.Infomessage = \"Some of the requests selected are of leave type CL/ML and are for 3 or more days. Kindly ensure to collect the medical certificate of the employee and share with HR before approving these requests!\"\n      this.modalRef1 = this.modalService.show(infopopupTemplate, { class: 'modal-sm' });\n    } else if (checkDaysFlag == 1 && this.processAction == 'Approved') {\n      this.Infomessage = \"Kindly ensure to collect the medical certificate of the employee and share with HR before approving this leave!\"\n      this.modalRef1 = this.modalService.show(infopopupTemplate, { class: 'modal-sm' });\n    } else {\n      this.openModal(template);\n    }\n  }\n\n  infohide(toProcess, action, template, infopopupTemplate) {\n    this.modalRef1.hide();\n    this.openModal(template);\n    this.click = false;\n  }\n\n  singleinfohide(toProcess, action, template, singleapprovetemplate) {\n    this.modalRef1.hide();\n    this.openModal(singleapprovetemplate);\n    this.click = false;\n  }\n\n  confirm() {\n\n    if (this.processAction == 'Approved') {\n      this.processLeaveRequests(this.leaveToProcessTest, this.processAction, '');\n      this.modalRef.hide();\n    } else if (this.ReasonChangeEvent()) {\n      this.processLeaveRequests(this.leaveToProcessTest, this.processAction, this.reason);\n      this.modalRef.hide();\n    }\n  }\n\n  decline(): void {\n    this.modalRef.hide();\n    this.isvalidReason = false;\n    this.reason = '';\n    this.radioValue = 'download-leave';\n    this.checkedRadioFlag = true;\n    this.click = false;\n  }\n\n  //////////////////////// Search /////////////////////////\n  searchClicked(g) {\n    this.isSubmit = true;\n    this.leavefrom = this.data['leavefrom'] != undefined ? moment(this.data['leavefrom']).format('YYYY-MM-DD').toString() : null;\n    this.leaveto = this.data['leaveto'] != undefined ? moment(this.data['leaveto']).format('YYYY-MM-DD').toString() : null;\n    this.approve = this.data['approve'] != undefined ? moment(this.data['approve']).format('YYYY-MM-DD').toString() : null;\n\n    this.appliedDate = this.data['appliedDate'] != undefined ? moment(this.data['appliedDate']).format('YYYY-MM-DD').toString() : null;\n    let reporteeNumber = \"\", reporteeName = \"\";\n    if (isNaN(this.data['searchText'])) {\n      reporteeName = this.data['searchText'];\n    } else {\n      reporteeNumber = this.data['searchText'];\n    }\n    let employeeLeaveList = \"\";\n    if (this.data['employeeLeaveTypeList']) {\n      employeeLeaveList = this.data['employeeLeaveTypeList']\n    }\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"reporteeEmployeeNumber\": reporteeNumber,\n      \"reporteeName\": reporteeName,\n      \"fromDate\": this.leavefrom,\n      \"toDate\": this.leaveto,\n      \"appliedDate\": this.appliedDate,\n      \"leaveType\": employeeLeaveList\n    }\n\n    if ((this.leavefrom == \"\" && this.leaveto == \"\" && (this.appliedDate != \"\" && this.data['Searchstatus'] != \"\" && this.data['employeeLeaveTypeList'] != \"\"))\n      || (this.data['employeeLeaveTypeList'] != 'undefined')) {\n      const url = CONSTANT.PENDING_REQUESTS;\n      this.spinnerService.show();\n      this.service.postService(url, data)\n        .subscribe(data => {\n          this.selectedAll = undefined;\n          this.PendingLeaveRequests = data;\n          this.spinnerService.hide();\n        }, error => {\n          console.log(error);\n          if (error.status == 0) {\n            window.location.reload();\n\n          }\n        })\n    }\n    else {\n    }\n  }\n\n  ///////////////////////// Download Excel ///////////////////////////\n\n  downloadCSV() {\n    this.leavefrom = this.data['leavefrom'] != undefined ? moment(this.data['leavefrom']).format('YYYY-MM-DD').toString() : null;\n    this.leaveto = this.data['leaveto'] != undefined ? moment(this.data['leaveto']).format('YYYY-MM-DD').toString() : null;\n    this.approve = this.data['approve'] != undefined ? moment(this.data['approve']).format('YYYY-MM-DD').toString() : null;\n\n    this.appliedDate = this.data['appliedDate'] != undefined ? moment(this.data['appliedDate']).format('YYYY-MM-DD').toString() : null;\n    let leaveType = (this.data['employeeLeaveTypeList'] == undefined ? null : this.data['employeeLeaveTypeList']);\n    let search = (this.data['searchText'] == undefined ? null : this.data['searchText']);\n    var appliedon = this.isSubmit ? this.appliedDate : null;\n    var approveddate = this.isSubmit ? this.approve : null;\n    var fromappdate = this.isSubmit ? this.leavefrom : null;\n    var toappdate = this.isSubmit ? this.leaveto : null;\n    var empsearch = this.isSubmit ? search : null;\n\n    let data = \"employeeNumber=\" + this.service.employeeNumber + \"&approvedDate=\" + approveddate + \"&fromDate=\" + fromappdate + \"&toDate=\" + toappdate + \"&appliedDate=\" + appliedon + \"&leaveType=\" + leaveType + \"&leaveStatus=\" + \"Pending\" + \"&reporteeEmployeeNumber=\" + \"\" + \"&reporteeName=\" + empsearch + \"&numberOfDays=\" + \"\";\n    const url = CONSTANT.EXPORT_CSV;\n\n    window.location.href = CONSTANT.EXPORT_CSV + \"?\" + data;\n  }\n\n  doOnSelect(event, pickerName) {\n    if (pickerName == \"fromSearch\") {\n      this.data[\"leaveto\"] = this.data[\"leavefrom\"];\n    }\n  }\n\n  searchReset() {\n\n    this.data['approve'] = undefined;\n    this.data['leavefrom'] = undefined;\n    this.data['leaveto'] = undefined;\n    this.data['searchText'] = undefined;\n\n    this.data['employeeLeaveTypeList'] = undefined;\n    this.data['appliedDate'] = undefined;\n    this.selectedAll = undefined;\n    this.selectionColor = false;\n    this.getPendingLeaveRequests();\n  }\n\n\n  ///////////////////////////// Pagination funtionalities ///////////////////////////////\n\n  pageonclick() {\n    this.selectedAll = undefined;\n    this.itemsSelected = 0;\n    for (let i = 0; this.PendingLeaveRequests.length > i; i++) {\n      this.PendingLeaveRequests[i].selected = false;\n    }\n\n  }\n\n  checkAll(ev) {\n    if ((ev.target.checked == false)) {\n      this.itemsSelected = 0;\n    }\n    let interateVAl = this.p * 15 - 15;\n    interateVAl = interateVAl > 0 ? interateVAl : interateVAl;\n    for (let i = interateVAl; i < this.p * 15; i++) {\n      if (this.PendingLeaveRequests[i] != undefined) {\n        this.PendingLeaveRequests[i].selected = this.selectedAll;\n        if (this.PendingLeaveRequests[i].selected == true) {\n          this.itemsSelected++\n        } else {\n          if (this.itemsSelected > 0)\n            this.itemsSelected--\n        }\n      }\n    }\n\n  }\n\n  itemselected(ev) {\n    if (ev.target.checked == true) {\n      this.itemsSelected++\n    } else {\n      this.itemsSelected--\n      this.selectedAll = false;\n    }\n    this.selectedAll = this.PendingLeaveRequests.every(function (item: any) {\n      return item.selected == true;\n    })\n\n  }\n\n  rowItemSelected(leave) {\n    if (leave.selected == true) {\n      leave.selected = false;\n      this.itemsSelected--;\n      this.selectedAll = false;\n    } else if (leave.selected == undefined) {\n      leave.selected = true;\n      this.itemsSelected++\n    } else {\n      leave.selected = true;\n      this.itemsSelected++\n    }\n    this.selectedAll = this.PendingLeaveRequests.every(function (item: any) {\n      return item.selected == true;\n    })\n  }\n\n\n  isAllChecked() {\n    return this.PendingLeaveRequests && this.PendingLeaveRequests.every(_ => _.selected);\n  }\n\n  openModal(template: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(template, { class: 'modal-sm' });\n  }\n\n  openModalapprove(approvetemplate: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(approvetemplate, { class: 'modal-sm' });\n  }\n\n  openModalsingle(singleapprovetemplate: TemplateRef<any>) {\n    this.modalRef = this.modalService.show(singleapprovetemplate, { class: 'modal-sm' });\n  }\n\n  changeEvent(event) {\n\n    if (event.checked) {\n      this.checkedbtn = false;\n\n    } else {\n      this.checkedbtn = true;\n    }\n  }\n\n  ReasonChangeEvent() {\n\n    if (this.reason.length == 0 || this.reason.length < 1) {\n      this.isvalidReason = true;\n      return false;\n    }\n    else {\n      this.isvalidReason = false;\n      return true;\n    }\n  }\n  listEmpNameCode: any;\n\n  listEmployeSelect(obj) {\n    this.data['searchText'] = obj.employeeName;\n  }\n\n  //////////////////////////////////////////////////Reports Tab Screen Starting/////////////////////////////////////\n\n  SearchReportsSelect(event, pickerName) {\n    this.isSearch = false;\n  }\n\n  selectAppliedOn(event, pickerName) {\n    this.isSearch = false;\n  }\n\n  ////////////////////////////////////////// For Dropdown Filter ///////////////////////////////////\n\n  reportdropdown() {\n    const url = CONSTANT.REPORT_DROP_DOWN;\n    let data = {\n      \"employeeNumber\": this.service.employeeNumber,\n      \"flag\": this.toggle ? 'DR' : 'IH'\n    }\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.reportLeaveType = data.leaveTypeInfo;\n        this.LeaveStatus = data.leaveStatusInfo;\n        this.EmployeeInfo = data.employeeInfo;\n        this.reportEmployee = data.employeeInfo;\n        this.reportEmployeeList = data.employeeInfo;\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n\n  ////////////////////////////////////////// For Filter Search ///////////////////////////////////\n\n  EmployeeSelect(obj) {\n    this.reportData['searchName'] = obj.employeeName;\n    this.selectedEmployeeId = obj.employeeCode;\n    this.isSearch = false;\n  }\n\n  LeaveTypeSelect(obj) {\n    this.leaveTypeId = obj.leaveTypeId;\n    this.reportData['reportLeaveType'] = obj.leaveType;\n    this.isSearch = false;\n  }\n\n  LeaveStatusSelect(obj) {\n    this.approveId = obj.leaveStatusId;\n    this.reportData['reportLeaveStatus'] = obj.leaveStatus;\n    this.isSearch = false;\n  }\n\n  onEmployeeChange(selectedEmp: string) {\n    this.isSearch = false;\n    let model = selectedEmp.toLowerCase();\n    let temp = [];\n    this.reportEmployee.map(obj => {\n      if ((obj.employeeName.toLowerCase()).includes(model.toLowerCase()) || obj.employeeCode.includes(model)) {\n        temp.push(obj);\n      }\n\n    });\n    this.reportEmployeeList = temp;\n  }\n\n  searchReport() {\n    this.isSubmited = true;\n    this.appliedOn = [];\n    this.repleavefrom = this.reportData['leavefrom'] != undefined ? moment(this.reportData['leavefrom']).format('YYYY-MM-DD').toString() : null;\n    this.repleaveto = this.reportData['leaveto'] != undefined ? moment(this.reportData['leaveto']).format('YYYY-MM-DD').toString() : null;\n    this.repapprove = (this.approveId == undefined ? null : this.approveId);\n\n    this.repappliedDate = this.reportData['appliedDate'] != undefined ? moment(this.reportData['appliedDate']).format('YYYY-MM-DD').toString() : null;\n    this.reviewedOn = this.reportData['reviewedDate'] != undefined ? moment(this.reportData['reviewedDate']).format('YYYY-MM-DD').toString() : null;\n    this.repleaveType = (this.leaveTypeId == undefined ? null : this.leaveTypeId);\n    this.selectedEmployeeId = this.selectedEmployeeId == undefined && this.reportData['searchName'] != undefined && this.reportData['searchName'] != '' ? '0' : this.selectedEmployeeId\n\n    let data = {\n      \"flag\": this.toggle ? 'DR' : 'IH',\n      \"loginEmployeeCode\": this.service.employeeNumber,\n      \"leaveFromDate\": this.repleavefrom,\n      \"leaveToDate\": this.repleaveto,\n      \"leaveAppliedOn\": this.repappliedDate,\n      \"leaveReviewedOn\": this.reviewedOn,\n      \"employeeCode\": this.selectedEmployeeId,\n      \"leaveTypeId\": this.repleaveType,\n      \"leaveStatusId\": this.repapprove\n    }\n    const url = CONSTANT.SEARCH_HIERACHY;\n    this.spinnerService.show();\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.ReportLeaveLoad = data.searchList;\n        this.employeeDetail = {};\n        this.modifiedHierarchy(data.searchList,'');\n        this.spinnerService.hide();\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n  modifiedHierarchy(data,str) {\n    if(str=!'' && str=='onload'){\n      this.reportData['leavefrom'] = new Date(moment().utc().startOf('month').format('YYYY-MM-DD'));\n      this.reportData['leaveto'] = new Date(moment().utc().endOf('month').format('YYYY-MM-DD'));\n    }\n    var empName, rmpEmpCode, empCode; this.employeeDetail = {};\n    var levelOneArr, levelTwoArr = [];\n    this.empCodeList = [];\n    /**Separating Level1 start*/\n    var levelOneList = data.filter((value, key) => {\n      if (this.empCodeList.length == 0) {\n        this.empCodeList.push(value.employeeCode);\n      }\n      if (this.empCodeList.length != 0) {\n        if (this.empCodeList.indexOf(value.employeeCode) == -1) {\n          this.empCodeList.push(value.employeeCode);\n        }\n      }\n      if (value.levelId == 1) {\n        return value;\n      }\n    });\n    /**Separating Level1 end*/\n    /**Separating Level2 start*/\n    var levelTwoList = data.filter((value, key) => {\n      if (value.levelId == 2) {\n        return value;\n      }\n    });\n    /**Separating Level2 end*/\n    /**Making Hierarchy for Level1 and Level2*/\n    this.empCodeList.forEach((code, index) => {\n      this.leveloneEmployeeFilterArr = levelOneList.filter((elem, key) => {\n        if (code == elem.employeeCode) {\n          empName = elem.employeeName;\n          empCode = elem.employeeCode;\n          return elem;\n        }\n      });\n      var obj = {}; levelOneArr = [];\n      obj['empCode'] = empCode;\n      obj['empName'] = empName;\n      obj['arrList'] = this.leveloneEmployeeFilterArr;\n      if (this.leveloneEmployeeFilterArr.length != 0) {\n        levelOneArr.push(obj);\n        this.employeeDetail[code] = levelOneArr\n      }\n      //leveltwostructure\n      this.leveltwoEmployeeFilterArr = levelTwoList.filter((elem1, key1) => {\n        if (code == elem1.employeeCode) {\n          empName = elem1.employeeName;\n          empCode = elem1.employeeCode;\n          rmpEmpCode = elem1.rmEmpCode;\n          return elem1;\n        }\n      });\n      var obj1 = {};     // levelTwoArr=[];\n      obj1['empCode'] = empCode;\n      obj1['empName'] = empName;\n      obj1['rmpEmpCode'] = rmpEmpCode;\n      obj1['arrList'] = this.leveltwoEmployeeFilterArr;\n      if (this.leveltwoEmployeeFilterArr.length != 0) {\n        levelTwoArr.push(obj1);\n      }\n\n    });\n    /**Checking Level2 Resource is mapping in level1 */\n    levelTwoArr.forEach((val, index1) => {\n      var arr = [];\n      if (this.employeeDetail[val.rmpEmpCode] != undefined) {\n      } else if (this.employeeDetail[val.rmpEmpCode] == undefined) {\n        arr.push(val)\n        this.employeeDetail[val.empCode] = arr;\n      }\n    });/**filtering reportees employeeId if user as sub reportee */\n    var reportee=[];\n    var subList = levelTwoArr.filter((elem1, key1) => {\n      if(this.employeeDetail[elem1.rmpEmpCode]!=undefined){\n        if (reportee.length == 0) {\n          reportee.push(elem1.rmpEmpCode);\n        }\n        if (reportee.length!=0 && reportee.indexOf(elem1.rmpEmpCode) == -1) {\n          reportee.push(elem1.rmpEmpCode)\n        }\n        return elem1;\n      }\n    });\n    /***making subreportee hierarchy */\n    var temp =[],empcodeArr=[];\n    var filteredList = reportee.filter((val, key1) => {\n      temp=[];\n      levelTwoArr.forEach((data, index) => {\n        if(val==data.rmpEmpCode){\n          data['records'] = data['arrList'];\n          temp.push(data);\n          return data;\n        }\n      });\n      if(temp.length!=0){\n        this.employeeDetail[val][0]['isReportee'] = true;\n        this.employeeDetail[val][0]['subReportees'] = temp;\n      }\n    });\n    /**Making Hierarchy for Level1 end*/\n    this.isObjectempty = Object.keys(this.employeeDetail).length == 0 ? true : false;\n  }\n\n  flagHide(val) {\n    this.toggle = val;\n    this.onLoadData();\n    this.appliedOn = [];\n    this.reportdropdown();\n    this.Reset();\n  }\n\n  /////////////////////////////////////////////////// 1Month data only for onload and reset ////////////////////////////////\n\n  onLoadData() {\n    this.ReportLeaveLoad = [];\n    this.isSubmited = false;\n    this.isSearch = true;\n    let data = {\n      \"flag\": this.toggle ? 'DR' : 'IH',\n      \"leaveFromDate\": moment().utc().startOf('month').format('YYYY-MM-DD'),\n      \"leaveToDate\": moment().utc().endOf('month').format('YYYY-MM-DD'),\n      \"loginEmployeeCode\": this.service.employeeNumber,\n    }\n    const url = CONSTANT.SEARCH_HIERACHY;\n    this.service.postService(url, data)\n      .subscribe(data => {\n        this.appliedOn = data.searchList;\n        this.modifiedHierarchy(this.appliedOn,'onload');\n      }, error => {\n        console.log(error);\n        if (error.status == 0) {\n          window.location.reload();\n\n        }\n      })\n  }\n\n  HierarchyCreation(data) {\n    this.reportData['leavefrom'] = new Date(moment().utc().startOf('month').format('YYYY-MM-DD'));\n    this.reportData['leaveto'] = new Date(moment().utc().endOf('month').format('YYYY-MM-DD'));\n    /**separating employee Ids from last 2 weeks employees list */\n    this.empCodeList = []; var keepLooping = false;\n    data.forEach((value, key) => {\n      if (this.empCodeList.length == 0) {\n        this.empCodeList.push(value.employeeCode);\n      }\n      if (this.empCodeList.length != 0) {\n        if (this.empCodeList.indexOf(value.employeeCode) == -1) {\n          this.empCodeList.push(value.employeeCode);\n        }\n      }\n    });\n\n    /**merging employees details to their paritcular employee*/\n\n    this.employeeDetail = {};\n    var empName, empCode, empReporteeName, empReporteeCode, arr = [];\n    var obj = {}, tempArr = [];\n    this.empCodeList.forEach((elem, key) => {\n      if (!keepLooping) {\n        this.filteredFirstDirectReportees = data.filter((value, index) => {\n          if (this.service.employeeNumber == value.rmEmpCode && elem == value.employeeCode) {\n            empName = value.employeeName;\n            empCode = value.employeeCode;\n            return value;\n          }\n        });\n        var obj = {}, arr = [], tempData = [];\n        var obj1 = {};\n        obj['empName'] = empName;\n        obj['empCode'] = empCode;\n        var subreporteeRecords = [];\n        var employeeRecord = [];\n        var filteredFirstDirectReportees = data.filter((value, index) => {\n          if (empCode == value.rmEmpCode) {\n            empReporteeName = value.employeeName;\n            empReporteeCode = value.employeeCode;\n            if (employeeRecord.length == 0) {\n              employeeRecord.push(value.employeeName)\n            } else if (employeeRecord.indexOf(value.employeeName) == -1) {\n              employeeRecord.push(value.employeeName)\n            }\n            obj['isReportee'] = true;\n            subreporteeRecords.push(value);\n            return value;\n          }\n        });\n        if (employeeRecord.length != 0) {\n          employeeRecord.forEach(elem => {\n            var obj1 = {};\n            obj1['empName'] = elem;\n            var listdata = filteredFirstDirectReportees.filter(element => {\n              if (elem == element.employeeName) {\n                obj1['empCode'] = element.employeeCode;\n                return element;\n              }\n            });\n            obj1['records'] = listdata;\n            tempData.push(obj1);\n          });\n        }\n        if (tempData.length != 0) {\n          obj['subReportees'] = tempData;\n        }\n\n        /**making an subreprotees record formation */\n\n        var flag = false;\n        filteredFirstDirectReportees.filter(element => {\n          if (elem == element.employeeCode) {\n            flag = true;\n          }\n        });\n        obj['arrList'] = this.filteredFirstDirectReportees;\n        if (obj['arrList'].length != 0) {\n          arr.push(obj)\n          this.employeeDetail[elem] = arr;\n        }\n        if (key == this.empCodeList.length - 1) {\n          keepLooping = true;\n        }\n      }\n    });\n    this.isObjectempty = Object.keys(this.employeeDetail).length == 0 ? true : false;\n  }\n\n  expandCollapse() {\n    var accordionId = $(event.target).attr(\"accordion-id\"),\n      numPanelOpen = $(accordionId + ' .collapse.in').length == 0 ? $(accordionId + ' .collapse.show').length : $(accordionId + ' .collapse.in').length;\n\n    $(event.target).toggleClass(\"active\");\n\n    if (numPanelOpen == 0) {\n      this.openAllPanels(accordionId);\n    } else {\n      this.closeAllPanels(accordionId);\n    }\n\n  }\n\n  openAllPanels = function (aId) {\n    $(aId + ' .panel-collapse:not(\".in\")').collapse('show');\n    $(aId + ' .accordian-body:not(\".in\")').collapse('show');\n  }\n\n  closeAllPanels = function (aId) {\n    $(aId + ' .panel-collapse.in').collapse('hide');\n    $(aId + ' .panel-collapse.show').collapse('hide');\n    $(aId + ' .accordian-body:not(\".in\")').collapse('hide');\n    $(aId + ' .accordian-body:not(\".show\")').collapse('hide');\n\n  }\n  ///////////////////////////////////////////////////////////// Download PDF & CSV Validation Section ////////////////////////////////////\n\n  radioselection(val) {\n    this.checkedRadioFlag = val == 'download-leave' ? true : false;\n  }\n\n  dropdownselection(val) {\n    this.selectedFlag = val == 'csv' ? true : false;\n  }\n\n  export(reptemplate) {\n    if (this.checkedDropdown == 'csv') {\n      this.openModal(reptemplate);\n      this.radioValue = 'download-leave';\n      this.checkedRadioFlag = true;\n    } else {\n      this.exportCSV();\n    }\n  }\n\n  exportCSV() {\n    if (this.repleavefrom == null || this.reportData['leavefrom'] == undefined && this.repleavefrom == undefined && this.repappliedDate == null && this.repapprove == null && this.reviewedOn == n