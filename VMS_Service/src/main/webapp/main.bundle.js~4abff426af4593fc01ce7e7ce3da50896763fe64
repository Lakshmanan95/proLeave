            _this.financeEmployeescopy = data.EmployeeInfo;
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.onEmployeeChange = function (selectedName) {
