    /////////////////////////////////////////////////// 1Month data only for onload and reset ////////////////////////////////
    ReviewComponent.prototype.onLoadData = function () {
        var _this = this;
        this.ReportLeaveLoad = [];
        this.isSubmited = false;
        this.isSearch = true;
        var data = {
            "flag": this.toggle ? 'DR' : 'IH',
            "leaveFromDate": __WEBPACK_IMPORTED_MODULE_1_moment__().utc().startOf('month').format('YYYY-MM-DD'),
            "leaveToDate": __WEBPACK_IMPORTED_MODULE_1_moment__().utc().endOf('month').format('YYYY-MM-DD'),
            "loginEmployeeCode": this.service.employeeNumber,
        };
        var url = __WEBPACK_IMPORTED_MODULE_3__utility_constant__["a" /* CONSTANT */].SEARCH_HIERACHY;
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.appliedOn = data.searchList;
            _this.modifiedHierarchy(_this.appliedOn, 'onload');
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    ReviewComponent.prototype.HierarchyCreation = function (data) {
        var _this = this;
        this.reportData['leavefrom'] = new Date(__WEBPACK_IMPORTED_MODULE_1_moment__().utc().startOf('month').format('YYYY-MM-DD'));
        this.reportData['leaveto'] = new Date(__WEBPACK_IMPORTED_MODULE_1_moment__().utc().endOf('month').format('YYYY-MM-DD'));
        /**separating employee Ids from last 2 weeks employees list */
        this.empCodeList = [];
        var keepLooping = false;
        data.forEach(function (value, key) {
            if (_this.empCodeList.length == 0) {
                _this.empCodeList.push(value.employeeCode);
            }
            if (_this.empCodeList.length != 0) {
                if (_this.empCodeList.indexOf(value.employeeCode) == -1) {
                    _this.empCodeList.push(value.employeeCode);
                }
            }
        });
        /**merging employees details to their paritcular employee*/
        this.employeeDetail = {};
        var empName, empCode, empReporteeName, empReporteeCode, arr = [];
        var obj = {}, tempArr = [];
        this.empCodeList.forEach(function (elem, key) {
            if (!keepLooping) {
                _this.filteredFirstDirectReportees = data.filter(function (value, index) {
                    if (_this.service.employeeNumber == value.rmEmpCode && elem == value.employeeCode) {
                        empName = value.employeeName;
                        empCode = value.employeeCode;
                        return value;
                    }
                });
                var obj = {}, arr = [], tempData = [];
                var obj1 = {};
                obj['empName'] = empName;
                obj['empCode'] = empCode;
                var subreporteeRecords = [];
                var employeeRecord = [];
                var filteredFirstDirectReportees = data.filter(function (value, index) {
                    if (empCode == value.rmEmpCode) {
                        empReporteeName = value.employeeName;
                        empReporteeCode = value.employeeCode;
                        if (employeeRecord.length == 0) {
                            employeeRecord.push(value.employeeName);
                        }
                        else if (employeeRecord.indexOf(value.employeeName) == -1) {
                            employeeRecord.push(value.employeeName);
                        }
                        obj['isReportee'] = true;
                        subreporteeRecords.push(value);
                        return value;
                    }
                });
                if (employeeRecord.length != 0) {
                    employeeRecord.forEach(function (elem) {
                        var obj1 = {};
                        obj1['empName'] = elem;
                        var listdata = filteredFirstDirectReportees.filter(function (element) {
                            if (elem == element.employeeName) {
                                obj1['empCode'] = element.employeeCode;
                                return element;
                            }
                        });
                        obj1['records'] = listdata;
                        tempData.push(obj1);
                    });
                }
                if (tempData.length != 0) {
                    obj['subReportees'] = tempData;
                }
                /**making an subreprotees record formation */
                var flag = false;
                filteredFirstDirectReportees.filter(function (element) {
                    if (elem == element.employeeCode) {
                        flag = true;
                    }
                });
                obj['arrList'] = _this.filteredFirstDirectReportees;
                if (obj['arrList'].length != 0) {
                    arr.push(obj);
                    _this.employeeDetail[elem] = arr;
                }
                if (key == _this.empCodeList.length - 1) {
                    keepLooping = true;
                }
            }
        });
        this.isObjectempty = Object.keys(this.employeeDetail).length == 0 ? true : false;
    };
    ReviewComponent.prototype.expandCollapse = function () {
        var accordionId = $(event.target).attr("accordion-id"), numPanelOpen = $(accordionId + ' .collapse.in').length == 0 ? $(accordionId + ' .collapse.show').length : $(accordionId + ' .collapse.in').length;
        $(event.target).toggleClass("active");
        if (numPanelOpen == 0) {
            this.openAllPanels(accordionId);
        }
        else {
            this.closeAllPanels(accordionId);
        }
    };
