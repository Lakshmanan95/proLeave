    //////////////////////////// modal window for Approve & Reject /////////////////////////
    ReviewComponent.prototype.singleleaverequest = function (leaveRequestId, numberOfDays, leaveCode, action, template, singleInfopopupTemplate, infopopupTemplate) {
        this.leaveToProcessTest = leaveRequestId;
        this.processAction = action;
        var checkDaysFlag = false;
        this.click = false;
        if ((leaveCode == "CL") && (action == "Approved")) {
            if (numberOfDays >= 3) {
                checkDaysFlag = true;
            }
        }
        if ((leaveCode == "MDL") && (action == "Approved")) {
            if (numberOfDays >= 1) {
                checkDaysFlag = true;
            }
        }
        if (checkDaysFlag) {
            this.SingleInfomessage = "Kindly ensure to collect the medical certificate of the employee and share with HR before approving this leave!";
            this.modalRef1 = this.modalService.show(singleInfopopupTemplate, { class: 'modal-sm' });
        }
        else {
            this.openModal(template);
        }
    };
    ReviewComponent.prototype.approveprocess = function (toProcess, action, template, infopopupTemplate) {
        var selectedRow = [];
        var checkDaysFlag = 0;
        this.itemsSelected = 0;
        this.click = false;
        for (var i = 0; this.PendingLeaveRequests.length > i; i++) {
            if (this.PendingLeaveRequests[i].selected) {
                this.itemsSelected++;
            }
        }
        toProcess.forEach(function (toProcessRow) {
            if (toProcessRow.selected && toProcessRow.selected === true) {
                selectedRow.push(toProcessRow.leaveRequestId);
                if (toProcessRow.leaveCode == "CL") {
                    if (toProcessRow.numberOfDays >= 3) {
                        checkDaysFlag++;
                    }
                }
                if (toProcessRow.leaveCode == "MDL") {
                    if (toProcessRow.numberOfDays >= 1) {
                        checkDaysFlag++;
                    }
                }
                else {
                    if (checkDaysFlag > 0) {
                        checkDaysFlag++;
                    }
                }
            }
        });
        this.leaveToProcessTest = selectedRow.toString();
        this.processAction = action;
        if (checkDaysFlag > 1 && this.processAction == 'Approved') {
            this.Infomessage = "Some of the requests selected are of leave type CL/ML and are for 3 or more days. Kindly ensure to collect the medical certificate of the employee and share with HR before approving these requests!";
            this.modalRef1 = this.modalService.show(infopopupTemplate, { class: 'modal-sm' });
        }
        else if (checkDaysFlag == 1 && this.processAction == 'Approved') {
            this.Infomessage = "Kindly ensure to collect the medical certificate of the employee and share with HR before approving this leave!";
            this.modalRef1 = this.modalService.show(infopopupTemplate, { class: 'modal-sm' });
        }
        else {
            this.openModal(template);
        }
    };
    ReviewComponent.prototype.infohide = function (toProcess, action, template, infopopupTemplate) {
        this.modalRef1.hide();
        this.openModal(template);
        this.click = false;
    };
    ReviewComponent.prototype.singleinfohide = function (toProcess, action, template, singleapprovetemplate) {
        this.modalRef1.hide();
        this.openModal(singleapprovetemplate);
        this.click = false;
    };
    ReviewComponent.prototype.confirm = function () {
        if (this.processAction == 'Approved') {
            this.processLeaveRequests(this.leaveToProcessTest, this.processAction, '');
            this.modalRef.hide();
        }
        else if (this.ReasonChangeEvent()) {
            this.processLeaveRequests(this.leaveToProcessTest, this.processAction, this.reason);
            this.modalRef.hide();
        }
    };
    ReviewComponent.prototype.decline = function () {
        this.modalRef.hide();
        this.isvalidReason = false;
        this.reason = '';
        this.radioValue = 'download-leave';
        this.checkedRadioFlag = true;
        this.click = false;
    };
