        this.emptyfields = false;
    };
    AdminComponent.prototype.massupdatevalidationEvent = function () {
        if (this.noOfDays != undefined && this.noOfDays != '' && this.leaveselected != 'Select Leave' && this.locationselected != 'Select Location') {
            this.validleave = false;
            this.validnoOfDays = false;
            this.emptyfields = false;
            this.validlocation = false;
        }
        else if ((this.noOfDays != undefined || this.noOfDays == undefined || this.noOfDays == '') && this.leaveselected == 'Select Leave' && this.locationselected == 'Select Location') {
            this.validleave = true;
            this.validnoOfDays = true;
            this.emptyfields = true;
            this.validlocation = true;
        }
        else if ((this.noOfDays != undefined || this.noOfDays == undefined || this.noOfDays == '') && this.noOfDays == '' && this.leaveselected != 'Select Leave' && this.locationselected != 'Select Location') {
            this.validleave = false;
            this.emptyfields = false;
            this.validnoOfDays = true;
            this.validlocation = false;
        }
        else if (this.noOfDays != undefined || this.noOfDays != '' && this.leaveselected == 'Select Leave' && this.locationselected != 'Select Location') {
            this.validleave = true;
            this.emptyfields = false;
            this.validnoOfDays = false;
            this.validlocation = false;
        }
        else if (this.noOfDays != undefined || this.noOfDays != '' && this.leaveselected != 'Select Leave' && this.locationselected == 'Select Location') {
            this.validleave = false;
            this.emptyfields = false;
            this.validnoOfDays = false;
            this.validlocation = true;
        }
        else if ((this.noOfDays != undefined || this.noOfDays == undefined || this.noOfDays == '') || this.leaveselected == 'Select Leave' || this.locationselected == 'Select Location') {
            this.emptyfields = true;
        }
    };
    /**onchange of employeename */
    AdminComponent.prototype.leaveCreditEmployee = function (obj, empId) {
        $('#emp-name-search').hide();
        this.cancelCredit = obj.employeeName != '' ? true : false;
        this.showSummary = false;
        this.templeaveCode = obj.leaveCode;
        this.employeeLeaveBalance(obj.employeeId);
        this.getLeaveHistory(obj.employeeId);
        this.leavesummaryList = [];
        this.currentYearFilterArr, this.currentYearFilterArr2 = [];
        this.doj = obj.doj;
        this.empid = obj.employeeId;
        if (obj.employeeName != '') {
            $('.creditleave').attr('disabled', 'disabled');
            $('.creditleave').css('background', 'none');
        }
        else {
            $('.creditleave').removeAttr('disabled'); //url(/assets/img/search.png) 97% / 6% no-repeat #ffff
            $('.creditleave').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
        }
        this.data['employeeName'] = obj.employeeName;
        this.selectedEmployeeId = obj.employeeId;
        this.isvalidFreeze = false;
        this.dateJoining = true;
        this.creditIds = [];
        this.creditObj = {};
        this.getuserhistoryinfo();
    };
    AdminComponent.prototype.adminOnSelect = function (event, pickerName) {
        if (pickerName == "from") {
            this.validateFromDate = this.dateAdminFrom != '' ? false : true;
            this, this.dateAdminTo = this.dateAdminFrom;
            this.validateDate = false;
        }
        else if (pickerName == "to") {
            this.validateDate = false;
            this.validateToDate = this.dateAdminTo != '' ? false : true;
        }
    };
    AdminComponent.prototype.ExtractDetail = function () {
        this.adminreportvalidationEvent('extract');
        if (!this.validateDate && !this.validateFromDate && !this.validateToDate) {
            var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].ADMIN_REPORT;
            this.fromdateadmin = this.dateAdminFrom != undefined ? __WEBPACK_IMPORTED_MODULE_6_moment__(this.dateAdminFrom).format('YYYY-MM-DD') : null;
            this.todateadmin = this.dateAdminTo != undefined ? __WEBPACK_IMPORTED_MODULE_6_moment__(this.dateAdminTo).format('YYYY-MM-DD') : null;
            var data = "employeeNumber=" + null + "&fromDate=" + this.fromdateadmin + "&toDate=" + this.todateadmin + "&flag=" + "ADMRPT";
            window.location.href = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].ADMIN_REPORT + "?" + data;
            this.modalRef.hide();
        }
    };
    AdminComponent.prototype.adminreportvalidationEvent = function (str) {
        if (this.dateAdminFrom != '' && this.dateAdminTo != '') {
            this.validateDate = false;
            this.validateFromDate = false;
            this.validateToDate = false;
            ///service calll 
            if (str == 'extract') {
            }
        }
        else if (this.dateAdminFrom == '' && this.dateAdminTo == '') {
            this.validateDate = true;
        }
        else if (this.dateAdminFrom == '' && this.dateAdminTo != '') {
            this.validateDate = false;
            this.validateFromDate = true;
            this.validateToDate = false;
        }
        else if (this.dateAdminFrom != '' && this.dateAdminTo == '') {
            this.validateDate = false;
            this.validateFromDate = false;
            this.validateToDate = true;
        }
    };
    AdminComponent.prototype.employeeLeaveBalance = function (empId) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].EMPLOYEE_LEAVE_BALANCE;
