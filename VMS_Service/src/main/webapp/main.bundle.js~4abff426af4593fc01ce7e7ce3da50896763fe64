    AdminComponent.prototype.companynameevent = function (cmpName) {
        this.isvalidCmp = false;
    };
    AdminComponent.prototype.reportnameevent = function (reporttype) {
        this.maxDateToDisable = new Date(this.today.getFullYear(), 11, 31);
        this.dateFinanceFrom = new Date();
        this.dateFinanceTo = new Date();
        this.isvalidReport = false;
        this.selectedColor = false;
        this.overallSelect = true;
        this.financeLeave.forEach(function (element) {
            element['checked'] = true;
        });
        if (reporttype == "Leave Balance") {
            this.dateFinanceFrom = new Date();
            this.dateFinanceTo = new Date();
        }
        else if (reporttype == "India Sales") {
            this.cmpName = 'Select Entity';
            this.data['fincemployeeName'] = '';
            this.dateFinanceFrom = new Date();
            this.dateFinanceTo = new Date();
            this.selectedColor = true;
            this.overallSelect = true;
            this.financeLeave.forEach(function (element) {
                element['checked'] = true;
            });
        }
        else if (reporttype == "Comp Off") {
            this.overallSelect = false;
            this.financeLeave.forEach(function (element) {
                if (element.leaveType == "Compensatory Off") {
                    element['checked'] = true;
                }
                else
                    element['checked'] = false;
            });
        }
        if (reporttype == "As of Leave Balance") {
            this.dateFinanceTo = new Date(new Date().setDate(new Date().getDate() - 1));
            this.dateFinanceFrom = new Date(new Date().setDate(new Date().getDate() - 1));
            this.maxDateToDisable = new Date(new Date().setDate(new Date().getDate() - 1));
        }
    };
    AdminComponent.prototype.financeValidation = function () {
        this.isvalidCmp = false;
        this.isvalidReport = false;
        if ((this.checkedFinance == 'entity') || (this.checkedFinance == 'employee' || (this.checkedFinance == 'inactive'))) {
            if ((this.cmpName == 'Select Entity' && this.reporttype != 'India Sales') && this.reporttype == 'Select Report') {
                this.isvalidCmp = true;
                this.isvalidReport = true;
            }
            else if ((this.cmpName == 'Select Entity' && this.reporttype != 'India Sales') && this.reporttype != 'Select Report') {
                this.isvalidCmp = true;
                this.isvalidReport = false;
            }
            else if (this.cmpName != 'Select Entity' && this.reporttype == 'Select Report') {
                this.isvalidCmp = false;
                this.isvalidReport = true;
            }
        }
    };
    AdminComponent.prototype.checkall = function (event, obj) {
        if (obj.checked) {
            obj.checked = false;
            ;
        }
        else {
            obj.checked = true;
        }
        var arrlen = this.financeLeave.length;
        var count = 0;
        this.financeLeave.forEach(function (element) {
            if (element.checked) {
                count++;
            }
        });
        this.overallSelect = arrlen == count ? true : false;
    };
    /**select all change event*/
    AdminComponent.prototype.chooseAll = function (event) {
        var _this = this;
        this.overallSelect = this.overallSelect ? false : true;
        this.financeLeave.forEach(function (element) {
            element['checked'] = _this.overallSelect ? true : false;
        });
    };
    AdminComponent.prototype.reset = function () {
        this.dateFinanceFrom = new Date();
        this.dateFinanceTo = new Date();
        this.overallSelect = true;
        this.selectedColor = false;
        this.financeLeave.forEach(function (element) {
            element['checked'] = true;
        });
