var HomeComponent = (function () {
    function HomeComponent(service, router, spinnerService, modalService) {
        this.service = service;
        this.router = router;
        this.spinnerService = spinnerService;
        this.modalService = modalService;
        this.data = {};
        this.workingdays = [];
        this.reportingManagerCountFlag = false;
        this.disableDates = [];
        this.disableDatesForWeekend = [];
        this.enabledDates = [];
        this.enabledDatesForWeekend = [];
        this.approvedDates = [];
        this.holidaysApproved = [];
        this.leaveTypeLists = [];
        this.defaultLeaveTypeLists = [];
        this.continuousLeaveTypeLists = [];
        this.continuousLeaveTypeListsWithWeekend = [];
        this.defaultLeaveWithoutBalance = [];
        this.leaveBalanceWidget = [];
        this.LeaveTypeNewDropDown = [];
        this.CopyLeaveTypeNewDropDown = [];
        this.legendsLeaveData = [];
        this.legendcolor = [];
        this.checkTimeOffHistory = [];
        this.clicked = false;
        this.reportingManagerName = this.service.reportingManagerName;
        this.today = new Date();
        this.dateFrom = new Date();
        this.dateTo = new Date();
        this.noOfDays = 1;
        this.LeaveTypeNew = [{ "type": "Loss Of Pay", "typeid": 5 }, { "type": "On Duty", "typeid": 6 }];
        this.legendsLeaveData = ["Holiday", "OD", "LOP", "COFF"];
        this.continuousLeaveTypeListsWithWeekend = ["LOP", "ML", "FML", "MML", "MCL", "PRL"];
        this.holidaysArray = [];
        this.onDutyOptions = 'undefined';
        this.onDutyOptionsArr = [];
        this.isvalid = false;
        this.reason = '';
        this.odComments = '';
        this.leavetype = 'undefined';
        this.submitBtnTxt = true;
        this.onDutyFlag = false;
        this.hideIcon = true;
        this.isvalidLeavetype = false;
        this.isvalidReason = false;
        this.isvalidonDutyOptions = false;
        this.WithdrawHide = true;
        this.withdrawFlag = false;
        this.LeaveList = [];
        this.Searchstatus = [{ "Searchstatus": "Pending" }, { "Searchstatus": "Cancelled" }, { "Searchstatus": "Approved" }, { "Searchstatus": "Rejected" }];
        this.withdrawReset = true;
        this.percentage;
        this.message = 'Are you sure you want to submit the leave request?';
        this.historyNotifyEnable = true;
        this.toDateEditable = true;
        this.maxDateYearEnd = new Date(this.today.getFullYear(), 10, 30);
        this.minDateYearEnd = new Date(this.today.getFullYear(), 1, 1);
        this.maxDateToDisable = new Date(this.today.getFullYear(), 11, 31);
        this.minDateToDisable = new Date(this.today.getFullYear(), 0, 1);
    }
    HomeComponent.prototype.ngOnInit = function () {
        this.getloggeduserinfo();
        this.FilterForm = new __WEBPACK_IMPORTED_MODULE_5__angular_forms__["FormGroup"]({
            days: new __WEBPACK_IMPORTED_MODULE_5__angular_forms__["FormControl"]()
        });
        var btn = document.getElementById("LB");
        var ele = document.getElementsByClassName('container-fluid')[0];
        btn.style.display = 'none';
        ele.addEventListener('scroll', function () {
            if (ele.scrollTop > 20) {
                btn.style.display = "block";
            }
            else {
                btn.style.display = "none";
            }
        });
    };
    /////////////////// User logged informatioan ///////////////////////
    HomeComponent.prototype.getloggeduserinfo = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_1__utility_constant__["a" /* CONSTANT */].LOGGED_USER_INFO;
        var data = {
            // "employeeNumber": "102861"
            "employeeNumber": ""
        };
        this.LeaveTypeNewDropDown = [];
        this.CopyLeaveTypeNewDropDown = [];
        this.onDutyOptionsArr = [];
        this.legendcolor = [];
        this.leaveBalanceWidget = [];
        this.spinnerService.show();
        this.service.postService(url, data)
            .subscribe(function (data) {
            var setColor;
            var checkFlag = false;
            data.LeaveTypeList.forEach(function (element) {
                _this.leaveTypeLists.push(element.leaveCode);
                if (element.isDefault == "Y") {
                    _this.defaultLeaveTypeLists.push(element.leaveCode);
                }
                if (element.isContinuous == "Y") {
                    _this.continuousLeaveTypeLists.push(element.leaveCode);
                }
                checkFlag = true;
            });
            if (checkFlag) {
                _this.LeaveTypeNewDropDown.push({ "type": "Loss Of Pay", "leaveCode": "LOP", "typeid": 5 }, { "type": "On Duty", "typeid": 6, "leaveCode": "OD" });
            }
            data.LeaveBalanceInfo.forEach(function (element, index) {
                if (_this.leaveTypeLists.indexOf(element.leaveCode) > -1) {
                    if (_this.continuousLeaveTypeLists.indexOf(element.leaveCode) > -1) {
                        if (parseFloat(element.approvedAndPendingBalance) == 0 && element.balanceLeave > 0) {
                            _this.LeaveTypeNewDropDown.push({ "type": element.leaveTypeName, "leaveCode": element.leaveCode, "typeid": element.leaveTypeId, "balanceLeave": element.balanceLeave, "location": element.locationCode });
                        }
                    }
                    else if (element.balanceLeave > 0 || element.leaveCode == "OD" || element.leaveCode == "LOP") {
                        _this.LeaveTypeNewDropDown.push({ "type": element.leaveTypeName, "leaveCode": element.leaveCode, "typeid": element.leaveTypeId, "balanceLeave": element.balanceLeave, "location": element.locationCode });
                    }
                    if (_this.defaultLeaveTypeLists.indexOf(element.leaveCode) > -1) {
                        _this.leaveBalanceWidget.push(element);
                        _this.defaultLeaveWithoutBalance.push(element.leaveCode);
                    }
                }
                if (_this.defaultLeaveTypeLists.indexOf(element.leaveCode) > -1 && _this.legendsLeaveData.indexOf(element.leaveCode) < 0) {
                    _this.legendsLeaveData.push(element.leaveCode);
                }
                var perDay = 100 / (element.openLeave + Number(element.creditedLeave));
                // Setting color in legends
                if (element.leaveCode !== 'PTL' && element.leaveCode !== 'BL' && element.leaveCode !== 'ML') {
                    element.percentage = perDay * Number(element.usedLeave);
                }
                if (element.leaveCode == 'ML') {
                    _this.currentLeaveBalance = element.balanceLeave;
                }
            });
            _this.defaultLeaveTypeLists.forEach(function (element) {
                if (_this.defaultLeaveWithoutBalance.indexOf(element) < 0) {
                    _this.leaveBalanceWidget.push({ "leaveCode": element, "leaveTypeName": element, "openLeave": "0", "creditedLeave": "0.00", "usedLeave": "0.00", "balanceLeave": "0.00", "pendingLeave": "0.00" });
                }
            });
            if (data.LeaveBalanceInfo.length == 0) {
                data.LeaveTypeList.forEach(function (element) {
                    if (element.isDefault == "Y") {
                        _this.leaveBalanceWidget.push({ "leaveCode": element.leaveCode, "leaveTypeName": element.leaveType, "openLeave": "0", "creditedLeave": "0.00", "usedLeave": "0.00", "balanceLeave": "0.00", "pendingLeave": "0.00" });
                    }
                });
            }
            _this.legendsLeaveData.forEach(function (element) {
                setColor = data.Legends[0].hasOwnProperty(element) != undefined ? data.Legends[0][element] : '';
                _this.legendcolor.push({ "leaveCode": element, "color": setColor });
            });
            data.OutOfOfficeOptions.forEach(function (element, index) {
                _this.onDutyOptionsArr.push({ "type": element.odOptionName, "typeid": element.odOptionId, "code": element.odOptionCode, "reason": element.leaveReason });
            });
            data.AdHocWorkingDays.forEach(function (element, index) {
                if (_this.workingdays != undefined && _this.workingdays.indexOf(element.leaveDate) == -1) {
                    _this.workingdays.push(element.leaveDate);
                }
            });
            _this.userData = _this.leaveBalanceWidget;
            _this.percentage = 100;
            _this.loggedUsserInfo = data;
            _this.holidays = data.HolidayList;
            _this.LeaveList = data.LeaveTypeList;
            _this.service.employeeNumber = data.EmpReportManagerInfo[0].employeeNumber;
            _this.service.employeeType = data.EmpReportManagerInfo[0].employeeType;
            if (_this.service.employeeType == 'C') {
                _this.hideIcon = false;
            }
            _this.service.sendReporteeInfo({ employeeName: data.EmpReportManagerInfo[0].employeeName, locationName: data.EmpReportManagerInfo[0].locationName, role: data.EmpReportManagerInfo[0].role, leftPanelMenu: "Home", hide: _this.hideIcon });
            _this.service.employeeId = data.EmpReportManagerInfo[0].employeeId;
            _this.service.contactNumber = data.EmpReportManagerInfo[0].contactNumber;
            _this.service.locationCode = data.EmpReportManagerInfo[0].locationCode;
            _this.reportingManagerCount = data.EmpReportManagerInfo[0].reportingManagerName.split(',');
            _this.location = data.EmpReportManagerInfo[0].locationName;
            _this.name = data.EmpReportManagerInfo[0].employeeName;
            _this.reportingManagerCountLength = _this.reportingManagerCount.length;
            if (_this.reportingManagerCount.length > 1) {
                _this.reportingManagerCountFlag = true;
            }
            _this.reportingManagerName = data.EmpReportManagerInfo[0].reportingManagerName.replace(/,/g, '|');
            if (_this.service.employeeType == 'C') {
                _this.router.navigate(['/consultant']);
            }
            //Holiday List
            for (var i = 0; i < data.HolidayList.length; i++) {
                _this.holidaysArray.push(data.HolidayList[i].holidayDate);
            }
            _this.disabledDaysArr();
            _this.CopyLeaveTypeNewDropDown = _this.CopyLeaveTypeNewDropDown.concat(_this.LeaveTypeNewDropDown);
            _this.spinnerService.hide();
            _this.getuserhistoryinfo();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    /////////////////// Showing leave Information //////////////////
    HomeComponent.prototype.getleaverequestinfo = function (post, rejectInfoTemplate, Infotemplate) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_1__utility_constant__["a" /* CONSTANT */].LEAVE_REQUEST_FETCH;
        this.submitBtnTxt = true;
        this.withdrawFlag = true;
        this.currentLeave = post.leaveRequestId;
        (post.leaveStatus == 'Pending') ? this.WithdrawHide = true : this.WithdrawHide = false;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "leaveRequestId": post.leaveRequestId
        };
        this.spinnerService.show();
        this.service.postService(url, data)
            .subscribe(function (data) {
            if (post.leaveStatus == "Rejected") {
                _this.fetchdateFrom = new Date(data[0].fromDate);
                _this.fetchdateTo = new Date(data[0].toDate);
                _this.rejectedreson = data[0].rejectedReason;
                _this.leavetypename = data[0].leaveTypeName;
                _this.noOfDays = data[0].numberOfDays;
                _this.modalRef3 = _this.modalService.show(rejectInfoTemplate, { class: 'modal-sm' });
                _this.leavetype = data[0].leaveTypecode;
                _this.dateFrom = new Date(data[0].fromDate);
                _this.dateTo = new Date(data[0].toDate);
                if (data[0].leaveReason == '') {
                    _this.fetchreason = data[0].odOptionName;
                }
                else {
                    _this.fetchreason = data[0].leaveReason;
                }
                _this.odComments = data[0].leaveReason;
                _this.reason = data[0].leaveReason;
                (data[0].odOptionCode == '') ? _this.onDutyFlag = false : _this.onDutyFlag = true;
                _this.onDutyOptions = data[0].odOptionCode;
                _this.submitBtnTxt = false;
            }
            else {
                if (_this.LeaveTypeNewDropDown.find(function (x) { return x.leaveCode == data[0].leaveTypecode; }) == undefined) {
                    _this.LeaveTypeNewDropDown.push({ "type": data[0].leaveTypeName, "leaveCode": data[0].leaveTypecode, "typeid": data[0].leaveTypeId });
                }
                _this.leavetype = data[0].leaveTypecode;
                _this.leavetypename = data[0].leaveTypeName;
                _this.leaveTypeChangeEvent();
                (data[0].odOptionCode == '') ? _this.onDutyFlag = false : _this.onDutyFlag = true;
                _this.onDutyOptions = data[0].odOptionCode;
                _this.OndutyDropDown();
                var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
                var fromDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(data[0].fromDate).format('YYYY/MM/DD').toString());
                var toDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(data[0].toDate).format('YYYY/MM/DD').toString());
                var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));
                var numberOfDays = diffDays + 1;
                _this.dateFrom = fromDate;
                _this.dateTo = toDate;
                if (data[0].odOptionCode == 'OTH') {
                    _this.odComments = data[0].leaveReason;
                    _this.reason = "";
                }
                else {
                    _this.reason = data[0].leaveReason;
                    _this.odComments = "";
                }
                _this.noOfDays = data[0].numberOfDays;
                _this.submitBtnTxt = false;
            }
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    HomeComponent.prototype.rejecthide = function (post, rejectInfoTemplate) {
        this.modalRef3.hide();
    };
    HomeComponent.prototype.onRightClick = function () {
        return false;
    };
    ////////////////// Getting User history information service ///////////////////// 
    HomeComponent.prototype.getuserhistoryinfo = function () {
        var url = __WEBPACK_IMPORTED_MODULE_1__utility_constant__["a" /* CONSTANT */].USER_HISTORY_INFO;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            // "employeeNumber": "102861",
            "appliedDate": "",
            "fromDate": "",
            "toDate": "",
            "numberOfDays": "",
            "leaveType": "",
            "approvedDate": "",
            "leaveStatus": ""
        };
        this.fetchTimeOffHistory(url, data);
    };
    HomeComponent.prototype.fetchTimeOffHistory = function (url, data) {
        var _this = this;
        this.checkTimeOffHistory = [];
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.historyLeaveListRequest = data;
            for (var i = 0; i < data.length; i++) {
                if (data[i].leaveStatus.toLowerCase() == "approved") {
                    var noOfDates = [];
                    var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
                    var fromDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(data[i].fromDate).format('YYYY/MM/DD').toString());
                    var toDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(data[i].toDate).format('YYYY/MM/DD').toString());
                    var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));
                    var numberOfDays = diffDays + 1;
                    for (var j = 0; j < numberOfDays; j++) {
                        _this.approvedDates.push(__WEBPACK_IMPORTED_MODULE_3_moment__(data[i].fromDate).add(j, 'days').format('YYYY-MM-DD').toString());
                        var leaveData = {
                            leaveType: data[i].leaveTypeId,
                            leaveTypeCode: data[i].leaveCode,
                            leaveDate: __WEBPACK_IMPORTED_MODULE_3_moment__(data[i].fromDate).add(j, 'days').format('YYYY-MM-DD').toString()
                        };
                        _this.holidaysApproved.push(leaveData);
                    }
                    if (_this.continuousLeaveTypeLists.indexOf(data[i].leaveCode) > -1) {
                        if (_this.today.getFullYear() == new Date(data[i].toDate).getFullYear()) {
                            _this.checkTimeOffHistory.push(data[i].leaveCode);
                        }
                    }
                }
                else if (data[i].leaveStatus.toLowerCase().includes("pending")) {
                    if (_this.continuousLeaveTypeLists.indexOf(data[i].leaveCode) > -1) {
                        if (_this.today.getFullYear() == new Date(data[i].toDate).getFullYear()) {
                            _this.checkTimeOffHistory.push(data[i].leaveCode);
                        }
                    }
                }
                ////////// splitting the dual approval and dispalying single approval name in UI////////////
                _this.reportingManagerCoun = data[i].approvedStatus;
                data[i].notificationApprovedStatus = data[i].approvedStatus;
                _this.reportingManagerCoun = data[i].approvedStatus.split(",", 1);
                if (data[i].approvedStatus.split(",").length < 2) {
                    _this.reportingManagerCoun = data[i].approvedStatus;
                    data[i].notificationShow = false;
                }
                else {
                    if (data[i].notificationApprovedStatus.toLowerCase().includes("approved")) {
                        data[i].notificationShow = false;
                    }
                    else {
                        data[i].notificationShow = true;
                    }
                }
                data[i].approvedStatus = _this.reportingManagerCoun;
            }
            _this.userhistoryInfo = data.sort(function (a, b) {
                return new Date(a.appliedOnDate).getTime() - new Date(b.appliedOnDate).getTime();
            });
            //////////////////// hover dual approval/////////////////////
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    ////////////////////////// filter histroy ///////////////////////
    HomeComponent.prototype.searchHistroy = function (g) {
        this.leavefrom = this.data['leavefrom'] != undefined ? __WEBPACK_IMPORTED_MODULE_3_moment__(this.data['leavefrom']).format('YYYY-MM-DD').toString() : null;
        this.leaveto = this.data['leaveto'] != undefined ? __WEBPACK_IMPORTED_MODULE_3_moment__(this.data['leaveto']).format('YYYY-MM-DD').toString() : null;
        this.approve = this.data['approve'] != undefined ? __WEBPACK_IMPORTED_MODULE_3_moment__(this.data['approve']).format('YYYY-MM-DD').toString() : null;
        this.appliedDate = this.data['appliedDate'] != undefined ? __WEBPACK_IMPORTED_MODULE_3_moment__(this.data['appliedDate']).format('YYYY-MM-DD').toString() : null;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "appliedDate": this.appliedDate,
            "fromDate": this.leavefrom,
            "toDate": this.leaveto,
            "numberOfDays": this.data['days'],
            "leaveType": this.data['employeeLeaveTypeList'],
            "approvedDate": this.approve,
            "leaveStatus": this.data['Searchstatus']
        };
        if ((this.leavefrom == "" && this.leaveto == "" && (this.approve != "" && this.appliedDate != "" && this.data['days'] != "" && this.data['Searchstatus'] != "" && this.data['employeeLeaveTypeList'] != ""))
            || (this.data['employeeLeaveTypeList'] != 'undefined')) {
            var url = __WEBPACK_IMPORTED_MODULE_1__utility_constant__["a" /* CONSTANT */].USER_HISTORY_INFO;
            this.fetchTimeOffHistory(url, data);
        }
    };
    HomeComponent.prototype.searchReset = function () {
        this.data['approve'] = undefined;
        this.data['leavefrom'] = undefined;
        this.data['leaveto'] = undefined;
        this.data['days'] = "";
        this.data['employeeLeaveTypeList'] = undefined;
        this.data['appliedDate'] = undefined;
        this.data['Searchstatus'] = undefined;
        this.getuserhistoryinfo();
    };
    ////////////////// Displaying color in calander ////////////////////////
    HomeComponent.prototype.setAlignmentStyle = function (date) {
        var month = date.month + 1;
        var tempDate = date.year + '-' + ((month < 10) ? '0' + month : '' + month) + '-' + ((date.day < 10) ? '0' + date.day : '' + date.day);
        var dateMoment = __WEBPACK_IMPORTED_MODULE_3_moment__(date).format('YYYY-MM-DD').toString();
        var test1 = __WEBPACK_IMPORTED_MODULE_3_moment__(dateMoment).utc();
        var newDate = new Date(test1.toString());
        if (this.holidaysArray && this.holidaysArray.indexOf(tempDate) != -1) {
            return "#D07CED";
        }
        for (var i = 0; i < this.holidaysApproved.length; i++) {
            if (!((newDate.getDay() == 6 || newDate.getDay() == 0 || this.disableDatesForWeekend.indexOf(dateMoment) > -1) && this.enabledDatesForWeekend.indexOf(dateMoment) < 0) || this.continuousLeaveTypeListsWithWeekend.indexOf(this.holidaysApproved[i].leaveTypeCode) > -1) {
                if (this.holidaysApproved[i].leaveDate == tempDate) {
                    if (this.holidaysApproved[i].leaveType == '1') {
                        return "#DB9696";
                    }
                    if (this.holidaysApproved[i].leaveType == '2') {
                        return "#F5C66E";
                    }
                    if (this.holidaysApproved[i].leaveType == '3') {
                        return "#49D1E3";
                    }
                    if (this.holidaysApproved[i].leaveType == '4') {
                        return "#F5C66E";
                    }
                    if (this.holidaysApproved[i].leaveType == '5') {
                        return "#DF485B";
                    }
                    if (this.holidaysApproved[i].leaveType == '6') {
                        return "#7ED321";
                    }
                    if (this.holidaysApproved[i].leaveType == '14') {
                        return "#DB9696";
                    }
                    if (this.holidaysApproved[i].leaveType == '15') {
                        return "#F5C66E";
                    }
                    if (this.holidaysApproved[i].leaveType == '22') {
                        return "#DB9696";
                    }
                }
            }
        }
    };
    //////////////////////////// Counting the days////////////////////////
    HomeComponent.prototype.doOnSelect = function (event, pickerName) {
        if (pickerName == "from") {
            this.dateTo = this.dateFrom;
            if (this.leavetype == "ML") {
                this.dateFilterUpdate(this.leavetype);
                return true;
            }
        }
        else if (pickerName == "fromSearch") {
            this.data["leaveto"] = this.data["leavefrom"];
        }
        this.dateFilterUpdate(this.leavetype);
    };
    //////////////////////////////// Year & Month Freeze in Date Picker///////////////////////////////////
    HomeComponent.prototype.doOnMonthChange = function (event, pickerName) {
        if (event.month == 1) {
            this.currentDate = new Date(event.year, 0, 1);
        }
        else {
            this.currentDate = new Date(event.year, event.month - 1, 1);
        }
        if (this.dateFrom.getMonth() == 1) {
            this.toPrevDiable = new Date(this.dateFrom.getFullYear(), 0, 1);
        }
        else {
            this.toPrevDiable = new Date(this.dateFrom.getFullYear(), this.dateFrom.month - 1, 1);
        }
        if (this.currentDate.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {
            $(".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "none");
        }
        else if (this.currentDate.getTime() < this.dateFrom.getTime() && pickerName == 'to') {
            $(".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "none");
        }
        else {
            $(".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "all");
            $(".toDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "all");
        }
        if (this.currentDate.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'from') {
            $(".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "none");
        }
        else if (this.currentDate.getTime() < this.minDateYearEnd.getTime() && pickerName == 'from') {
            $(".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "none");
        }
        else {
            $(".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "all");
            $(".fromDate_div .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "all");
        }
    };
    ///////////////////////////////////// For Top Scroll From Leave History ////////////////////////////////////////
    HomeComponent.prototype.scroll = function (el) {
        el.scrollIntoView(true);
        var scrolledY = window.scrollY;
        if (scrolledY) {
            window.scroll(0, scrolledY - 60);
        }
    };
    HomeComponent.prototype.scrollToBottom = function (id) {
        document.getElementsByClassName('container-fluid')[0].scrollTo(0, document.getElementsByClassName('container-fluid')[0].scrollHeight);
    };
    HomeComponent.prototype.scrollToTop = function (id) {
        document.getElementsByClassName('container-fluid')[0].scrollTo(0, 0);
    };
    HomeComponent.prototype.calculateNumberOfDays = function () {
        var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
        this.noOfDays = 0;
        var fromDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(this.dateFrom).format('YYYY/MM/DD').toString());
        var toDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(this.dateTo).format('YYYY/MM/DD').toString());
        var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));
        var numberOfDays = diffDays + 1;
        for (var i = 0; i < numberOfDays; i++) {
            var inrDate = new Date(this.dateFrom);
            inrDate.setDate(inrDate.getDate() + i);
            var checkIncrementedDate = __WEBPACK_IMPORTED_MODULE_3_moment__(inrDate).format('YYYY-MM-DD');
            if (this.holidaysArray.indexOf(checkIncrementedDate) < 0 && (!(inrDate.getDay() == 0 || inrDate.getDay() == 6))) {
                this.noOfDays++;
            }
            if (this.workingdays.indexOf(checkIncrementedDate) > -1) {
                this.noOfDays++;
            }
        }
    };
    //////// disaling dates in datepicker based on working, holidays and approved dates/////////////
    HomeComponent.prototype.disabledDaysArr = function () {
        var _this = this;
        this.workingdays.forEach(function (element) {
            _this.enabledDates.push(new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(element).format('YYYY/MM/DD').toString()));
            _this.enabledDatesForWeekend.push(element);
        });
        this.holidaysArray.forEach(function (element) {
            _this.disableDates.push(new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(element).format('YYYY/MM/DD').toString()));
            _this.disableDatesForWeekend.push(element);
        });
        this.approvedDates.forEach(function (element) {
        });
    };
    ////////////////////////// service call for submit /////////////////////////////
    HomeComponent.prototype.leaveTypeChangeEvent = function () {
        if (this.leavetype == 'undefined') {
            this.isvalidLeavetype = true;
        }
        else if (this.leavetype == 'OD') {
            this.isvalidLeavetype = false;
            this.isvalidonDutyOptions = false;
            this.isvalidReason = false;
            this.onDutyFlag = true;
            this.isvalidLeavetype = false;
            this.reason = "";
        }
        else {
            this.isvalidonDutyOptions = false;
            this.isvalidReason = false;
            this.onDutyFlag = false;
            this.isvalidLeavetype = false;
        }
        this.selectedColor = true;
        this.dateFilterUpdate(this.leavetype);
        return this.isvalidLeavetype;
    };
    HomeComponent.prototype.ReasonChangeEvent = function () {
        if (this.leavetype !== 'OD' && (this.reason.length == 0 || this.reason.length < 1)) {
            this.isvalidReason = true;
        }
        else if (this.leavetype == 'OD' && (this.odComments.length == 0 || this.odComments.length < 1)) {
            this.isvalidReason = true;
        }
        else {
            this.isvalidReason = false;
        }
        return this.isvalidReason;
    };
    HomeComponent.prototype.OndutyDropDown = function () {
        if (this.onDutyOptions == 'undefined') {
            this.isvalidonDutyOptions = true;
            this.odComments = "";
        }
        else if (this.onDutyOptions == "OTH") {
            this.isvalidLeavetype = false;
            this.isvalidReason = false;
            this.isvalidonDutyOptions = false;
        }
        else {
            this.odComments = "";
            this.isvalidonDutyOptions = false;
        }
        this.selectedodcolor = true;
        return this.isvalidonDutyOptions;
    };
    /////////////////////////// Maternity Leave validations ////////////////////////
    HomeComponent.prototype.dateFilterUpdate = function (leaveType) {
        if ((leaveType == "ML" || leaveType == "MML" || leaveType == "MCL") && this.service.locationCode) {
            this.toDateEditable = false;
            this.dateTo = new Date((new Date(this.dateFrom)).getTime() + (60 * 60 * 24 * 1000 * (parseFloat(this.currentLeaveBalance) - 1)));
            var fromDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(this.dateFrom).format('YYYY/MM/DD').toString());
            var toDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(this.dateTo).format('YYYY/MM/DD').toString());
            var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
            var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));
            this.noOfDays = diffDays + 1;
        }
        else if (this.continuousLeaveTypeListsWithWeekend.indexOf(leaveType) > -1) {
            this.toDateEditable = true;
            var fromDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(this.dateFrom).format('YYYY/MM/DD').toString());
            var toDate = new Date(__WEBPACK_IMPORTED_MODULE_3_moment__(this.dateTo).format('YYYY/MM/DD').toString());
            var oneDay = 24 * 60 * 60 * 1000; // hours*minutes*seconds*milliseconds
            var diffDays = Math.round(Math.abs((fromDate.getTime() - toDate.getTime()) && (toDate.getTime() - fromDate.getTime()) / (oneDay)));
            this.noOfDays = diffDays + 1;
        }
        else {
            this.toDateEditable = true;
            this.calculateNumberOfDays();
        }
    };
    ////////////////////////// fetching leave and dispalying in leave request form ///////////////////
    HomeComponent.prototype.fetchid = function (d, x) {
        var _this = this;
        if (d == 'true') {
            return this.LeaveTypeNewDropDown.find(function (x) { return x.leaveCode == _this.leavetype; }).typeid;
        }
        else if (d == 'OD') {
            return this.onDutyOptionsArr.find(function (x) { return x.code == _this.onDutyOptions; }).typeid;
        }
        else {
            return this.LeaveList.find(function (x) { return x.leaveCode == _this.leavetype; }).leaveTypeId;
        }
    };
    HomeComponent.prototype.submitClicked = function (template, success, error, Infotemplate, LeaveHistoryTemplate) {
        var _this = this;
        this.leaveTypeChangeEvent();
        this.ReasonChangeEvent();
        this.OndutyDropDown();
