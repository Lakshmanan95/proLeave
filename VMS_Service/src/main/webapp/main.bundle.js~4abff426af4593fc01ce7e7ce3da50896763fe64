            .subscribe(function (data) {
            _this.PendingLeaveRequests = data;
            _this.reporteeListPendingRequest = data;
            _this.reporteeLeaveListRequest = data;
            _this.service.pendingCount = data.length;
            _this.isSubmit = false;
            _this.PendingLeaveRequests = data.sort(function (a, b) {
                return new Date(a.submittedDate).getTime() - new Date(b.submittedDate).getTime();
            });
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
