        this.reporttype = 'Select Report';
        this.locationselected = 'Select Location';
        this.leaveselected = 'Select Leave';
        this.isvalidFreeze = false;
        this.validateFromDate = false;
        this.validateToDate = false;
        this.validateDate = false;
        this.maxDateYearEnd = new Date(this.today.getFullYear(), 10, 30);
        this.minDateYearEnd = new Date(this.today.getFullYear(), 1, 1);
        this.maxDateToDisable = new Date(this.today.getFullYear(), 11, 31);
        this.minDateToDisable = new Date(this.today.getFullYear(), 0, 1);
        this.dummylocationObj = [];
        this.currentYearFilterArr = [];
        this.currentYearFilterArr2 = [];
        this.showSummary = false;
        this.initialcompOff = false;
        this.hidecompofinput = true;
        this.checkedValue = 'bulk-upload';
        this.checkedFinance = 'entity';
        this.checkedValueFlag = this.checkedValue == 'bulk-upload' ? true : false;
        this.checkedFinanceFlag = this.checkedFinance == 'entity' ? true : false;
        this.dummyMonthlocationObj = {};
        this.dummylocationObjCopy = {};
        this.validCheck = false;
        this.monthNameList = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'Octber', 'November', 'December'];
        this.locationDumyData = [];
        this.modifiedLocationArr = [];
        this.monthLoop = [];
        this.filteredMonth = [];
        this.checkedList = [];
        this.revokeTempCopy = [];
        document.addEventListener('click', function () { return _this.onClick(event); });
    }
    AdminComponent.prototype.ngOnInit = function () {
        var currentyear = __WEBPACK_IMPORTED_MODULE_6_moment__().format('YYYY');
        var pastyear = __WEBPACK_IMPORTED_MODULE_6_moment__().subtract('year', 1).format('YYYY');
        this.yearArr.push(currentyear);
        this.yearArr.push(pastyear);
        this.getloggeduserinfo();
        this.freezedMonthList();
        this.addCompOff('');
        this.getAutoApprovalData();
        this.checkedFinance = 'entity';
        this.radioList = ['Entity', 'Employee', 'Inactive Enployee'];
        this.currentYear = __WEBPACK_IMPORTED_MODULE_6_moment__().format('YYYY');
        this.overallSelect = true;
        this.pastYear = __WEBPACK_IMPORTED_MODULE_6_moment__().subtract('year', 1).format('YYYY');
        this.lists =
            [
                { "name": this.currentYear, "checked": true },
                { "name": this.pastYear, "checked": false }
            ];
        this.checkedList.push(this.lists[0].name);
    };
    AdminComponent.prototype.onClick = function (event) {
        if (!$(event.target).hasClass('creditleave')) {
            $('#emp-name-search').hide();
            event.stopPropagation();
        }
        if (!$(event.target).hasClass('creditleavefin')) {
            $('#fin-name-search').hide();
            event.stopPropagation();
        }
    };
    AdminComponent.prototype.getCheckedValue = function (checkedList) {
        if (checkedList.length == 2) {
            delete checkedList[1];
        }
        return checkedList;
    };
    AdminComponent.prototype.shareCheckedList = function (filteredYearList) {
        var _this = this;
        this.scopeSelectedYear = filteredYearList;
        this.currentYearFilterArr, this.currentYearFilterArr2 = [];
        var count = 0;
        var currentYearFilter = this.summaryHistory.filter(function (value, key) {
            if (_this.tempObject.leaveTypeId == value.leaveTypeId && __WEBPACK_IMPORTED_MODULE_6_moment__(value.fromDate).format('YYYY') == filteredYearList[0] && (value.leaveStatus.toLowerCase()).indexOf('approved') != -1) {
                count += Number(value.numberDays);
                _this.currentTotal = count;
                value.count = count;
                return value;
            }
        });
        this.currentYearFilterArr = currentYearFilter;
        var count1 = 0;
        this.secondyearTable = filteredYearList.length == 2 ? true : false;
        if (filteredYearList.length == 2) {
            var currentYearFilter2 = this.summaryHistory.filter(function (value, key) {
                if (_this.tempObject.leaveTypeId == value.leaveTypeId && __WEBPACK_IMPORTED_MODULE_6_moment__(value.fromDate).format('YYYY') == filteredYearList[1] && (value.leaveStatus.toLowerCase()).indexOf('approved') != -1) {
                    count1 += Number(value.numberDays);
                    _this.pastTotal = count1;
                    return value;
                }
            });
            this.currentYearFilterArr2 = currentYearFilter2;
        }
    };
    AdminComponent.prototype.shareIndividualCheckedList = function (item) {
    };
    AdminComponent.prototype.freezedMonthList = function () {
        var _this = this;
        this.monthLoop = [];
        var currentMonth = __WEBPACK_IMPORTED_MODULE_6_moment__(__WEBPACK_IMPORTED_MODULE_6_moment__().subtract('month', 1)).format('MMMM');
        var tempFlag = false;
        this.monthNameList.filter(function (elem) {
            if (!tempFlag) {
                if (currentMonth == elem) {
                    tempFlag = true;
                }
                else {
                    _this.monthLoop.push(elem);
                }
            }
        });
    };
    AdminComponent.prototype.getloggeduserinfo = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].LOGGED_USER_INFO;
        var data = {
            // "employeeNumber": "102861"
            "employeeNumber": ""
        };
        this.spinnerService.show();
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.service.employeeNumber = data.EmpReportManagerInfo[0].employeeNumber;
            _this.service.employeeId = data.EmpReportManagerInfo[0].employeeId;
            _this.service.role = data.EmpReportManagerInfo[0].role;
            _this.service.hideRole = data.EmpReportManagerInfo[0].role.split(',');
            if (_this.service.hideRole.includes('Super Admin')) {
                _this.hideButton = true;
            }
            else {
                _this.hideButton = false;
            }
            _this.service.employeeType = data.EmpReportManagerInfo[0].employeeType;
            if (_this.service.employeeType == 'C') {
                _this.hideIcon = false;
            }
            _this.service.sendReporteeInfo({ employeeName: data.EmpReportManagerInfo[0].employeeName, locationName: data.EmpReportManagerInfo[0].locationName, role: data.EmpReportManagerInfo[0].role, leftPanelMenu: "Admin", hide: _this.hideIcon });
            if ((_this.service.role === 'Employee') || (_this.service.role === 'Reporting Manager')) {
                _this.router.navigate(['/home']);
            }
            _this.getActiveEmployeeRequests();
            _this.getFinanceEmployeeReport();
            _this.getEmployeeAdminLocation();
            _this.getAutoApprovalData();
            _this.getAutoApprovalTableData();
            _this.getcompanyname();
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    //////////////////////////////////////// UNFREEZE PAYROLL CUTOFF PERIOD SECTION ////////////////////////////////
    AdminComponent.prototype.getEmployeeAdminLocation = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].EMP_ADMIN_LOCATION_INFO;
        var data = {
            // "employeeNumber": "102861"
            "employeeNumber": ""
        };
        this.service.postService(url, data).subscribe(function (data) {
            _this.locationData = data.EmpAdminLocationInfo;
            _this.gethistory(0);
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.ReasonChangeEvent = function () {
        var _this = this;
        this.fieldArray.forEach(function (value, index) {
            value.location = _this.locationtype;
            value.datefromempty = value.datefrom == '' ? true : false;
            value.datetoempty = value.dateto == '' ? true : false;
            value.commentempty = value.comments == '' ? true : false;
            var tempLocation = _this.locationData.filter(function (element) {
                if (element.locationId == _this.locationtype) {
                    return element;
                }
            });
            _this.locationtypeTemp = tempLocation[0].locationName;
            var obj = {};
            if (!value.datefromempty && !value.datetoempty && !value.commentempty) {
                obj['comments'] = value.comments;
                obj['datefrom'] = value.datefrom;
                obj['dateto'] = value.dateto;
                obj['month'] = value.month;
                obj['location'] = _this.locationtype;
                obj['locationId'] = tempLocation[0].locationId;
                _this.validCheck = false;
            }
            else {
                _this.validCheck = true;
            }
        });
    };
    AdminComponent.prototype.saveServiceArrayFormation = function () {
        var _this = this;
        this.modifiedLocationArr = [];
        this.fieldArray.filter(function (elem, key) {
            var obj = {};
            obj['unfreezeEmployeeCode'] = '';
            obj['locationId'] = elem.locationId;
            obj['fromDate'] = __WEBPACK_IMPORTED_MODULE_6_moment__(elem.datefrom).format('YYYY-MM-DD');
            obj['toDate'] = __WEBPACK_IMPORTED_MODULE_6_moment__(elem.dateto).format('YYYY-MM-DD');
            obj['comments'] = elem.comments;
            obj['flag'] = 'LW';
            obj['index'] = key;
            _this.modifiedLocationArr.push(obj);
        });
    };
    AdminComponent.prototype.openModalunfreerror = function (unfreezeerror) {
        this.modalRef = this.modalService.show(unfreezeerror, { class: 'modal-sm' });
    };
    AdminComponent.prototype.saveClicked = function (template, error) {
        this.ReasonChangeEvent();
        if (!this.validCheck) {
            this.openModal(template);
        }
    };
    AdminComponent.prototype.locationSave = function (template, success, unfreezeerror) {
        var _this = this;
        this.saveServiceArrayFormation();
        this.DateChangeEvent();
        var urlSave = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].INSERT_UNFREEZE_DETAILS;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "unfreezeRecords": this.modifiedLocationArr,
            "record": null
        };
        this.service.postService(urlSave, data).subscribe(function (data) {
            if (data.status == 'Success') {
                _this.modifiedLocationArr = [];
                _this.modalRef.hide();
                _this.toastr.success(data.successMessage);
                _this.gethistory(_this.locationtype);
                _this.resetForm();
            }
            if (data.status === 'Error') {
                _this.modalRef.hide();
                _this.openModal(unfreezeerror);
                _this.errormessage = data.errorMessage;
            }
            else if (data.status === 'Failure') {
                var a = _this.fieldArray.splice(data.duplicateDateIndex[0], 1);
                _this.fieldArray = a;
                _this.modalRef.hide();
                _this.openModal(unfreezeerror);
                _this.gethistory(_this.locationtype);
                _this.errormessage = '';
                _this.errorvalidationmessage = data.validationMessage;
                _this.duplicateDates = data.duplicateDates;
            }
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    ////////////////////////////////////// CANCEL THE COUNTRY TO LOAD MAIN HISTORY /////////////////////////////
    AdminComponent.prototype.cancelSelectedCountry = function (str) {
        if (str != 'Select Location') {
            this.locationtype = 'Select Location';
            if (this.locationtype != 'Select Location') {
                $('#selectloc').attr('disabled', 'disabled');
                $('#selectloc').addClass('locationcrossdisable');
            }
            else {
                $('#selectloc').removeAttr('disabled');
                $('#selectloc').removeClass('locationcrossdisable');
            }
            this.locationDumyData = [];
            this.dummylocationObj = {};
            this.fieldArray = [];
            this.dummyMonthlocationObj = {
                'month': '',
                'monthdata': []
            };
            this.gethistory(0);
        }
    };
    /////////////////////////// ON SELECT DROPDOWN ICON AND CROSS ICON DISPLAY ///////////////////////////
    AdminComponent.prototype.locationTypeChangeEvent = function (event, id) {
        this.dummylocationObj = [];
        this.locationDumyData = [];
        this.dummylocationObj = {};
        this.monthLoop = [];
        this.dummyMonthlocationObj = {
            'month': '',
            'monthdata': []
        };
        if (this.locationtype != 'Select Location') {
            $('#location').removeClass('ad-location');
            $('#location').addClass('close-location');
            $('#selectloc').attr('disabled', 'disabled');
            $('#selectloc').addClass('locationcrossdisable');
        }
        else {
            $('#location').removeClass('close-location');
            $('#location').addClass('ad-location');
            $('#selectloc').removeAttr('disabled');
            $('#selectloc').removeClass('locationcrossdisable');
        }
        this.gethistory(event);
        this.freezedMonthList();
    };
    /////////////////////////////////////// Load History ////////////////////////////
    AdminComponent.prototype.gethistory = function (id) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].GET_LEAVE_UNFREEZED_DETAILS;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "locationId": this.locationtype == 'Select Location' ? '0' : id
        };
        this.spinnerService.show();
        this.service.postService(url, data).subscribe(function (data) {
            _this.locationDummyYear = [];
            _this.locationhistory = data.LeaveUnfreezeInfo;
            if (_this.locationtype != 'Select Location' && id) {
                var history = {};
                _this.locationMonth = [];
                _this.locationMonthcopy = [];
                _this.locationhistory.forEach(function (element, index) {
                    var obj = {};
                    obj['monthname'] = '';
                    obj['year'] = '';
                    if (_this.locationMonthcopy.indexOf(element.unFreezeMonth) == -1) {
                        obj['monthname'] = element.unFreezeMonth;
                        obj['year'] = element.unFreezeYear;
                        _this.locationMonthcopy.push(element.unFreezeMonth);
                    }
                    if (_this.locationDummyYear.indexOf(element.unFreezeYear) == -1) {
                        _this.locationDummyYear.push(element.unFreezeYear);
                    }
                });
                _this.locationObj = {};
                _this.restructinghistoryData(_this.locationhistory, _this.locationMonthcopy, _this.locationDummyYear);
            }
            else {
                _this.dummyMonthlocationObj = {};
                _this.locationDumyData = [];
                _this.dummylocationObj = {};
                _this.yearFilterObj = {};
                _this.locationDummyYear = [];
                _this.dummyMonthlocationObj = {
                    'month': '',
                    'monthdata': []
                };
                ///////location separation function///////
                _this.locationKey = [];
                _this.locationData.forEach(function (value, index) {
                    var filteredData = _this.locationhistory.filter(function (element) {
                        if (value.locationId == element.locationId) {
                            return element;
                        }
                    });
                    _this.locationDummyMonth = [];
                    _this.locationDummyYear = [];
                    _this.locationDumyData = [];
                    var monthFilteredData = filteredData.filter(function (element) {
                        if (_this.locationDummyMonth.indexOf(element.unFreezeMonth) == -1) {
                            _this.locationDummyMonth.push(element.unFreezeMonth);
                        }
                        if (_this.locationDummyYear.indexOf(element.unFreezeYear) == -1) {
                            _this.locationDummyYear.push(element.unFreezeYear);
                        }
                    });
                    _this.monthseparationEvent(_this.locationDummyMonth, _this.locationDummyYear, filteredData, value);
                });
            }
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
        return this.renderFlag = true;
    };
    AdminComponent.prototype.monthseparationEvent = function (monthlist, yearlist, data, val) {
        var _this = this;
        this.locationDumyData = [];
        monthlist.filter(function (value, key) {
            var filteredData = data.filter(function (element) {
                if (value == element.unFreezeMonth && val.locationName == element.locationName && (element.unFreezeStatus).toLowerCase() == ('Active').toLowerCase()) {
                    return element;
                }
            });
            /**year filteration */
            yearlist.filter(function (value1, key) {
                var filteredyearData = filteredData.filter(function (element) {
                    if (value1 == element.unFreezeYear && val.locationName == element.locationName && (element.unFreezeStatus).toLowerCase() == ('Active').toLowerCase()) {
                        return element;
                    }
                });
                if (filteredyearData.length != 0) {
                    _this.dummyMonthlocationObj = {
                        'month': value + '-' + value1,
                        'monthdata': filteredyearData
                    };
                    _this.locationDumyData.push(_this.dummyMonthlocationObj);
                }
            });
        });
        if (this.locationDumyData.length != 0) {
            this.locationKey.push(val.locationName);
            this.dummylocationObj[val.locationName] = this.locationDumyData;
            this.dummylocationObjCopy[val.locationName] = yearlist;
        }
        this.isEmpty = Object.keys(this.dummylocationObj).length == 0 ? true : false;
    };
    /////////////////////**making mapping structure for select location history*/////////////////////
    AdminComponent.prototype.restructinghistoryData = function (data, month, year) {
        var _this = this;
        var obj = {};
        month.filter(function (value, key) {
            var filteredData = data.filter(function (element) {
                if (value == element.unFreezeMonth) {
                    return element;
                }
            });
            _this.locationObj[value] = filteredData;
        });
        /**year filteration */
        this.locationMonthcopy.filter(function (value, key) {
            var obj = {};
            obj['monthname'] = '';
            obj['year'] = '';
            if (_this.locationObj[value].length != 0) {
                obj['monthname'] = value;
                obj['year'] = _this.locationObj[value][0].unFreezeYear;
                _this.locationMonth.push(obj);
            }
        });
    };
    AdminComponent.prototype.DateChangeEvent = function () {
        this.isvalidDate = false;
        this.datecolor = true;
    };
    ///////////////////////// UNFREEZE PAYROLL CUTOFF PERIOD DATEPICKER VALIDATION ///////////////
    AdminComponent.prototype.doOnSelect = function (event, pickerName, index) {
        this.emptyObj.comments = '';
        if (pickerName == "from") {
            this.fieldArray[index].dateto = this.fieldArray[index].datefrom;
        }
    };
    AdminComponent.prototype.onMonthChange = function (event, pickerName, index) {
        if (event.month == 1) {
            this.currentDateFreeze = new Date(event.year, 0, 1);
        }
        else {
            this.currentDateFreeze = new Date(event.year, event.month - 1, 1);
        }
        if (this.emptyObj.datefrom.getMonth() == 1) {
            this.toPrevDisable = new Date(this.emptyObj.datefrom.getFullYear(), 0, 1);
        }
        else {
            this.toPrevDisable = new Date(this.emptyObj.datefrom.getFullYear(), this.emptyObj.datefrom.month - 1, 1);
        }
        if (this.currentDateFreeze.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {
            $(".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "none");
        }
        else if (this.currentDateFreeze.getTime() < this.emptyObj.datefrom.getTime() && pickerName == 'to') {
            $(".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "none");
        }
        else {
            $(".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "all");
            $(".ad-date-pick .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "all");
        }
    };
    AdminComponent.prototype.addFieldValue = function (index) {
        var _this = this;
        if (this.fieldArray.length != 0 && (this.fieldArray[this.fieldArray.length - 1].location == 'Select Location' || this.fieldArray[this.fieldArray.length - 1].datefrom == '' || this.fieldArray[this.fieldArray.length - 1].dateto == '' || this.fieldArray[this.fieldArray.length - 1].comments == '')) {
            return false;
        }
        var tempLocation = this.locationData.filter(function (element) {
            if (element.locationId == _this.locationtype) {
                return element;
            }
        });
        this.locationtypeTemp = tempLocation[0].locationName;
        this.emptyObj = {};
        this.emptyObj.location = tempLocation[0].locationName;
        this.emptyObj.datefrom = '';
        this.emptyObj.dateto = '';
        this.emptyObj.locationId = tempLocation[0].locationId;
        this.emptyObj.month = __WEBPACK_IMPORTED_MODULE_6_moment__().format('MMMM');
        this.emptyObj.comments = '';
        this.fieldArray.push(this.emptyObj);
        if (this.fieldArray) {
            this.selectioncolor = false;
            this.isvalidReason = false;
            this.isvalidLocation = false;
        }
        else {
        }
    };
    AdminComponent.prototype.deleteFieldValue = function (index) {
        this.fieldArray.splice(index, 1);
    };
    AdminComponent.prototype.resetForm = function () {
        this.emptyObj.datefrom = '';
        this.emptyObj.dateto = '';
        this.emptyObj.comments = '';
        this.fieldArray = [];
    };
    //////////////////////////////// UNFREEZE ACCOUNT SECTION ////////////////////////////////////
    AdminComponent.prototype.openModal = function (unfreezeed) {
        this.modalRef = this.modalService.show(unfreezeed, { class: 'modal-sm' });
    };
    AdminComponent.prototype.openModalerror = function (error) {
        this.modalRef = this.modalService.show(error, { class: 'modal-sm' });
    };
    AdminComponent.prototype.unfreezeClicked = function (error, unfreezeed) {
        this.unfreeze = true;
        this.isvalidFreeze = (this.data['searchText'] == '' || this.data['searchText'] == undefined) ? true : false;
        if (!this.isvalidFreeze) {
            this.openModal(unfreezeed);
        }
    };
    AdminComponent.prototype.employeeFreeze = function (error, unfreezeed) {
        var _this = this;
        this.modalRef.hide();
        var urlSave = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].INSERT_UNFREEZE_DETAILS;
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "unfreezeRecords": [],
            "record": {
                "unfreezeEmployeeCode": this.selectedEmployeeId,
                "locationId": 0,
                "fromDate": this.newdateFrom(),
                "toDate": this.newdateTo(),
                "flag": "EW",
                "index": 0
            }
        };
        this.spinnerService.show();
        this.service.postService(urlSave, data).subscribe(function (data) {
            if (data.status == 'Success') {
                _this.toastr.success(data.successMessage);
                _this.selectedEmployeeId = '';
                _this.resetFreeze();
            }
            if (data.status === 'Error') {
                _this.modalRef.hide();
                _this.openModal(error);
                _this.errormessage = data.errorMessage;
            }
            else if (data.status === 'Failure') {
                _this.modalRef.hide();
                _this.openModal(error);
                _this.errormessage = data.validationMessage;
            }
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.newdateFrom = function () {
        return __WEBPACK_IMPORTED_MODULE_6_moment__(this.fromdatefreeze).format('YYYY-MM-DD');
    };
    AdminComponent.prototype.newdateTo = function () {
        return __WEBPACK_IMPORTED_MODULE_6_moment__(this.todatefreeze).format('YYYY-MM-DD');
    };
    //////////////////////////// DISPALYING EMPLOYEENAME/ID SCENARIO ///////////////////////////
    AdminComponent.prototype.onEmployeeChanged = function (selectedName) {
        var model = selectedName.toLowerCase();
        this.isvalidFreeze = false;
        var temp = [];
        this.activeEmployeeList.map(function (obj) {
            if ((obj.employeeName.toLowerCase()).includes(model.toLowerCase()) || obj.employeeId.includes(model)) {
                temp.push(obj);
            }
        });
        this.activeEmployees = temp;
        this.selectedColor = true;
    };
    AdminComponent.prototype.listEmployeSelect = function (obj) {
        this.data['searchText'] = obj.employeeName;
        this.selectedEmployeeId = obj.employeeId;
        this.isvalidFreeze = false;
    };
    AdminComponent.prototype.getActiveEmployeeRequests = function () {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].GET_ACTIVE_EMPLOYEE;
        var data = {
            "employeeNumber": this.service.employeeNumber
        };
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.activeEmployeeList = data.EmployeeInfo;
            _this.activeEmployees = data.EmployeeInfo;
            _this.activeEmployeescopy = data.EmployeeInfo;
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    /////////////////////////////// UNFREEZE ACCOUNT DATEPICKER VALIDATION ////////////////////////////////////////
    AdminComponent.prototype.unfreezeOnSelect = function (event, pickerName) {
        if (pickerName == "from") {
            this.todatefreeze = this.fromdatefreeze;
        }
    };
    AdminComponent.prototype.editLeaveDetails = function (obj) {
        obj.editdetail = true;
        this.tempopenLeave = obj.openLeave;
        this.tempcreditedLeave = obj.creditedLeave;
        this.tempusedLeave = obj.usedLeave;
        this.templeaveCode = obj.leaveCode;
    };
    AdminComponent.prototype.doOnMonthChange = function (event, pickerName) {
        if (event.month == 1) {
            this.currentDateFreeze = new Date(event.year, 0, 1);
        }
        else {
            this.currentDateFreeze = new Date(event.year, event.month - 1, 1);
        }
        if (this.fromdatefreeze.getMonth() == 1) {
            this.toPrevDisable = new Date(this.fromdatefreeze.getFullYear(), 0, 1);
        }
        else {
            this.toPrevDisable = new Date(this.fromdatefreeze.getFullYear(), this.fromdatefreeze.month - 1, 1);
        }
        if (this.currentDateFreeze.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "none");
        }
        else if (this.currentDateFreeze.getTime() < this.fromdatefreeze.getTime() && pickerName == 'to') {
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "none");
        }
        else {
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "all");
            $(".col-sm-6 .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "all");
        }
    };
    AdminComponent.prototype.resetFreeze = function () {
        this.data['searchText'] = '';
        this.fromdatefreeze = new Date();
        this.todatefreeze = new Date();
        this.isvalidFreeze = false;
        this.freezecolor = true;
    };
    AdminComponent.prototype.decline = function (str) {
        if (str != undefined) {
            this.checkedValue = 'bulk-upload';
            this.checkedValueFlag = true;
        }
        this.leaveCreditType = [];
        this.spinnerService.hide();
        ;
        if (this.modalRef != undefined) {
            this.modalRef.hide();
        }
        if (this.modalRef1 != undefined) {
            this.modalRef1.hide();
        }
        this.locationselected = 'Select Location';
        this.leaveselected = 'Select Leave';
        this.isSave = false;
        this.noOfDays = '';
        this.leavereason = '';
        this.daysDisable = true;
        this.data['autoEmployeeName'] = '';
        $('.creditleaveauto').css('background', 'url(/vms/assets/img/search.png) 97% / 6% no-repeat #ffff');
    };
    /////////////////////////////////////////////////// Credit Tab ////////////////////////////////////////
    AdminComponent.prototype.cancelLeaveDetails = function (obj) {
        obj.openLeave = this.tempopenLeave;
        obj.creditedLeave = this.tempcreditedLeave;
        obj.usedLeave = this.tempusedLeave;
        obj.editdetail = false;
        this.revokeDisable = true;
        this.revokeFocused = false;
        this.tempArr = [];
        this.getcompoff('');
        obj.balanceLeave = (Number(this.tempopenLeave) + Number(this.tempcreditedLeave)) - Number(this.tempusedLeave);
    };
    AdminComponent.prototype.deciamlCheck = function (data, emplist) {
        if (data <= 10) {
            emplist.openLeave = Number(data).toFixed(2);
            $('#openinput').val(Number(data).toFixed(2));
        }
        else if (data > 10) {
            data = '';
            emplist.openLeave = '';
            $('#openinput').val('');
        }
    };
    AdminComponent.prototype.leaveExpand = function (obj) {
        var _this = this;
        this.templeaveCode = '';
        this.lists[1].checked = false;
        this.selectChild.rework();
        this.tempObject = obj;
        var count = 0;
        if (this.summaryHistory != undefined) {
            var filteredleavedetail = this.summaryHistory.filter(function (value, key) {
                if (obj.leaveTypeId == value.leaveTypeId && __WEBPACK_IMPORTED_MODULE_6_moment__(value.fromDate).format('YYYY') == __WEBPACK_IMPORTED_MODULE_6_moment__().format('YYYY') && (value.leaveStatus.toLowerCase()).indexOf('approved') != -1) {
                    count += Number(value.numberDays);
                    _this.currentTotal = count;
                    value.count = count;
                    return value;
                }
            });
        }
        this.showHistoryTable = $('.card-header').length != $('.cursor-point.collapsed').length && !$(event.target).hasClass('collapsed') ? false : true;
        this.leaveonclick();
        this.leavesummaryList = filteredleavedetail;
        this.currentYearFilterArr = this.leavesummaryList;
        this.currentYearFilterArr2 = [];
        this.showSummary = true;
    };
    AdminComponent.prototype.saveLeaveDetails = function (data, error, template) {
        this.leavereason = '';
        this.isSave = false;
        this.editedData = data;
        if ((data.openLeave === '' || data.openLeave === null) || (data.usedLeave === '' || data.usedLeave === null) || (data.creditedLeave === '' || data.creditedLeave === null)) {
            data.isSubmit = true;
        }
        else {
            data.isSubmit = false;
        }
        if (!data.isSubmit) {
            this.openModal(template);
            this.getuserhistoryinfo();
        }
    };
    AdminComponent.prototype.openmassupdateModal = function (massupdate) {
        this.modalRef = this.modalService.show(massupdate, { class: 'modal-sm' });
    };
    AdminComponent.prototype.openleaveChange = function (data, obj, str, e) {
        var pattern = /[0-9\.\ ]/;
        var inputChar = String.fromCharCode(e.charCode);
        if (e.keyCode != 8 && !pattern.test(inputChar)) {
            event.preventDefault();
        }
        obj.balanceLeave = (Number(obj.openLeave) + Number(obj.creditedLeave)) - Number(obj.usedLeave);
    };
    AdminComponent.prototype.massUpdate = function (massupdate) {
        this.openModal(massupdate);
        this.validlocation = false;
        this.validleave = false;
        this.validnoOfDays = false;
        this.emptyfields = false;
        this.clicked = true;
        this.checkedValue = 'bulk-upload';
        this.checkedValueFlag = true;
        this.locationselected = 'Select Location';
        this.leaveselected = 'Select Leave';
        this.noOfDays = '';
    };
    AdminComponent.prototype.edit = function (compoffTemp, leavetype, obj) {
        var _this = this;
        this.tempObj = obj;
        var arrtemp = [];
        this.initialcompOff = false;
        if (leavetype == 'Compensatory Off') {
            if (!this.saveclick) {
                this.getcompoff('');
            }
            if (this.saveclick) {
                this.getcompoff('save');
            }
            this.addedCompOff = [];
            this.addCompOff('');
            setTimeout(function () {
                _this.openModal(compoffTemp);
            }, 300);
        }
    };
    AdminComponent.prototype.addCompOff = function (str) {
        this.initialcompOff = str == 'add' ? true : false;
        if (this.addedCompOff.length != 0 && this.addedCompOff[this.addedCompOff.length - 1].compoffdate == '') {
            return false;
        }
        var obj = {
            'compoffdate': ''
        };
        this.addedCompOff.push(obj);
    };
    /**checkbox change event */
    AdminComponent.prototype.revokedleaveDateChange = function (str, cnt) {
        var _this = this;
        var count = 0;
        var val = [];
        var arr = [];
        var uncheckcompOff = $('.compOffCheck').not(':checked').length;
        var checkcompOff = $('.compOffCheck:checked').length;
        var revokeTemp = [];
        this.revokeTempCopy = [];
        $('.compOffCheck:checked').each(function (i) {
            val[i] = $(this).val();
            revokeTemp.push($(this).val());
            $(this).prop('checked', true);
        });
        this.revokeTempCopy = revokeTemp;
        this.revokeDisable = val.length != 0 ? false : true;
        var arrtemp = [];
        var tempFlag = false;
        if (val != undefined && val.length != 0) {
            val.filter(function (elem) {
                for (var i = 0; i < _this.listofcompoffdates.length; i++) {
                    if (elem == _this.listofcompoffdates[i].leaveCOFFDates && $(event.target).val() != elem) {
                        _this.listofcompoffdates[i].check = true;
                        arrtemp.push(_this.listofcompoffdates[i]);
                    }
                }
                return _this.listofcompoffdates;
            });
        }
        var arr;
        var flag = false;
        var unrevoked = [];
        $(".compOffCheck:not(:checked)").each(function (i) {
            unrevoked = [];
            arr[i] = $(this).val();
            if ($(event.target)[0].innerHTML != 'Revoke' && !$(event.target)[0].checked) {
                flag = true;
                unrevoked.push($(event.target)[0].value);
            }
        });
        if (arr.length != 0) {
            arr.filter(function (el) {
                for (var i = 0; i < _this.listofcompoffdates.length; i++) {
                    if (el == _this.listofcompoffdates[i].leaveCOFFDates) {
                        _this.listofcompoffdates[i].check = false;
                        if (_this.saveclick) {
                            _this.tempArr.splice(i, 1);
                        }
                        tempFlag = true;
                    }
                }
            });
        }
        this.tempObj['creditedLeaveCopy'] = this.tempObj.creditedLeave;
        //for count display method
        var displayArr = [];
        displayArr.push($(event.target)[0].value);
        if (str != undefined && str == 'revoked') {
            if (flag) {
                this.tempObj.creditedLeave = this.tempObj.creditedLeave + unrevoked.length;
            }
            else {
                this.tempObj.creditedLeave = this.tempObj.creditedLeave - revokeTemp.length;
                revokeTemp = [];
            }
        }
        else if (this.revokeFocused && $(event.target)[0].innerHTML != 'Revoke' && !$(event.target)[0].checked) {
            this.tempObj.creditedLeave = this.tempObj.creditedLeave + unrevoked.length;
        }
        var arrfilter = [];
        this.leaveDates.forEach(function (checkDate, index) {
            var index = arr.indexOf(checkDate.leaveCOFFDates);
            if (index != -1) {
                arr.splice(index, 1);
            }
            else {
                arrfilter.push(checkDate.leaveCOFFDates);
            }
        });
        this.tempSavedDates = this.saveclick && arr.length != 0 ? arr : [];
        this.tempObj.balanceLeave = (Number(this.tempObj.openLeave) + Number(this.tempObj.creditedLeave)) - Number(this.tempObj.usedLeave);
    };
    /**revoke button event */
    AdminComponent.prototype.revokeCompOffDate = function () {
        var _this = this;
        var count = 0;
        var val = [];
        var arr = [];
        this.revokeFocused = true;
        $('.compOffCheck:checked').each(function (i) {
            val[i] = $(this).val();
            $(this).prop('checked', true);
        });
        this.tempArr = val;
        var count = 0;
        this.listofcompoffdates.forEach(function (checkarr) {
            if (checkarr.check == false) {
                count++;
            }
        });
        var checkFlag = false;
        this.listofcompoffdates.forEach(function (checkDate, index) {
            var key = _this.tempArr.indexOf(checkDate.leaveCOFFDates);
            if (key != -1) {
                if (!checkDate.check) {
                    checkFlag = true;
                }
            }
        });
        var revokeCheck = false;
        var dummycount = 0;
        this.listofcompoffdates.forEach(function (checkDate, index) {
            var key = _this.tempArr.indexOf(checkDate.leaveCOFFDates);
            if (key != -1) {
                if (!checkDate.check && _this.revokeFocused) {
                    revokeCheck = true;
                    dummycount++;
                }
            }
        });
        if (checkFlag) {
            this.revokedleaveDateChange('revoked', dummycount);
        }
        this.revokeDisable = this.tempArr.length != 0 ? false : true;
        this.modalRef.hide();
    };
    AdminComponent.prototype.compOffChange = function (event, obj) {
        obj.notSave = false;
        obj.alreadyExist = false;
    };
    AdminComponent.prototype.setOrders = function () {
        this.cloneOrders = this.addedCompOff.slice();
    };
    AdminComponent.prototype.saveCompOffDate = function () {
        var _this = this;
        var obj = {};
        var flag = false;
        obj['leaveCOFFDates'] = '';
        obj['check'] = '';
        this.tempSavedDates = [];
        this.tempSavedDatesCopy = [];
        this.addedCompOff.forEach(function (element, index) {
            var obj = {};
            obj['leaveCOFFDates'] = '';
            obj['check'] = '';
            if (element.compoffdate != '') {
                _this.tempSavedDates.push(__WEBPACK_IMPORTED_MODULE_6_moment__(element.compoffdate).format('YYYY-MM-DD'));
                _this.tempSavedDatesCopy.push(__WEBPACK_IMPORTED_MODULE_6_moment__(element.compoffdate).format('YYYY-MM-DD'));
            }
            obj['leaveCOFFDates'] = __WEBPACK_IMPORTED_MODULE_6_moment__(element.compoffdate).format('YYYY-MM-DD');
            obj['check'] = false;
        });
        this.compOffDatesCopy = [];
        this.addedCompOffCopy = [];
        this.listofcompoffdates.forEach(function (elem, index) {
            _this.compOffDatesCopy.push(elem.leaveCOFFDates);
        });
        this.tempSavedDates.forEach(function (elem, index) {
            var obj = {};
            obj['leaveCOFFDates'] = '';
            obj['check'] = false;
            if (_this.compOffDatesCopy.indexOf(elem) == -1) {
                _this.compOffDatesCopy.push(elem);
                obj['leaveCOFFDates'] = elem;
                obj['check'] = false;
                _this.listofcompoffdates.push(obj);
                if (_this.addedCompOff[index]['alreadyExist'] != undefined || _this.addedCompOff[index]['alreadyExist'] == undefined) {
                    _this.addedCompOff[index]['alreadyExist'] = false;
                }
            }
            else {
                flag = true;
                _this.addedCompOff[index]['alreadyExist'] = true;
            }
        });
        this.addedCompOff.forEach(function (elem, index) {
            if (elem['alreadyExist'] != undefined && !elem['alreadyExist']) {
                _this.addedCompOffCopy.push(elem);
            }
            if (elem.compoffdate == '') {
                elem['notSave'] = true;
            }
            else {
                elem['notSave'] = false;
            }
            if (elem.notSave || elem.alreadyExist) {
                flag = true;
            }
        });
        var result;
        function comparer(otherArray) {
            return function (current) {
                return otherArray.filter(function (other) {
                    return other.compoffdate == current.compoffdate && other.alreadyExist == current.alreadyExist;
                }).length == 0;
            };
        }
        var onlyInA = this.addedCompOff.filter(comparer(this.addedCompOffCopy));
        var onlyInB = this.addedCompOffCopy.filter(comparer(this.addedCompOff));
        result = onlyInA.concat(onlyInB);
        this.addedCompOff = result;
        if (!flag) {
            this.initialcompOff = false;
            this.modalRef.hide();
        }
        else {
            this.initialcompOff = true;
        }
        var count = 0;
        this.listofcompoffdates.forEach(function (checkarr) {
            if (checkarr.check == false) {
                count++;
            }
        });
        var uncheckcompOff = $('.compOffCheck').not(':checked').length;
        if (this.addedCompOff.length == 0) {
            this.tempObj.creditedLeave = this.tempObj.creditedLeave + this.tempSavedDates.length;
        }
        else {
            var countsave = 0;
            var dataDates;
            this.addedCompOff.forEach(function (elem, index) {
                dataDates = _this.tempSavedDates.filter(function (ele) {
                    if (ele != __WEBPACK_IMPORTED_MODULE_6_moment__(elem.compoffdate).format('YYYY-MM-DD')) {
                        countsave++;
                        return ele;
                    }
                });
            });
            this.tempObj.creditedLeave = this.tempObj.creditedLeave + countsave;
        }
        this.tempObj.balanceLeave = (Number(this.tempObj.openLeave) + Number(this.tempObj.creditedLeave)) - Number(this.tempObj.usedLeave);
        this.saveclick = true;
        this.listofcompoffdatesCopy = this.listofcompoffdates;
        this.revokeDisable = (this.tempArr.length != 0 || $('.compOffCheck:checked').length != 0) ? false : true;
    };
    AdminComponent.prototype.cancelCompOffDate = function (obj, index) {
        this.addedCompOff.splice(index, 1);
    };
    AdminComponent.prototype.adminMonthChange = function (event, pickerName) {
        if (event.month == 1) {
            this.adminDateNav = new Date(event.year, 0, 1);
        }
        else {
            this.adminDateNav = new Date(event.year, event.month - 1, 1);
        }
        if (this.dateAdminFrom.getMonth() == 1) {
            this.toPrevDisable = new Date(this.dateAdminFrom.getFullYear(), 0, 1);
        }
        else {
            this.toPrevDisable = new Date(this.dateAdminFrom.getFullYear(), this.dateAdminFrom.month - 1, 1);
        }
        if (this.adminDateNav.getTime() > this.maxDateYearEnd.getTime() && pickerName == 'to') {
            $(".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "none");
        }
        else if (this.adminDateNav.getTime() < this.dateAdminFrom.getTime() && pickerName == 'to') {
            $(".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "none");
        }
        else {
            $(".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-prev.ui-corner-all").css("pointer-events", "all");
            $(".admin-calender .ui-datepicker.ui-widget.ui-widget-content.ui-helper-clearfix.ui-corner-all.ui-shadow.ng-trigger.ng-trigger-overlayState a.ui-datepicker-next.ui-corner-all").css("pointer-events", "all");
        }
    };
    AdminComponent.prototype.adminReport = function (adminreport) {
        this.openModal(adminreport);
        this.validateDate = false;
        this.validateFromDate = false;
        this.dateAdminFrom = '';
        this.dateAdminTo = '';
        this.validateToDate = false;
    };
    AdminComponent.prototype.uploadselection = function (val) {
        this.checkedValueFlag = val == 'bulk-upload' ? true : false;
        this.clicked = true;
    };
    AdminComponent.prototype.uploadFiles = function (uploaderror) {
        this.bulkupload(uploaderror);
        if (this.fileSelected.name != '') {
            if (this.fileSelected.name.includes('.')) {
                var xlsfile = this.fileSelected.name.split('.')[1];
                if (xlsfile == 'xls' || xlsfile == 'xlsx') {
                    this.xlscheck = false;
                    this.clicked = true;
                }
                else {
                    this.xlscheck = true;
                }
            }
        }
    };
    AdminComponent.prototype.bulkupload = function (uploaderror) {
        var _this = this;
        this.spinnerService.show();
        this.httpService.post('/vms/leaveCredit/bulkUpload?employeeNumber=' + this.service.employeeNumber, this.frmData).subscribe(function (data) {
            if (data['status'] == 'Success') {
                _this.modalRef.hide();
                _this.toastr.success(data['successMessage']);
                _this.fileSelected = '';
                _this.frmData = new FormData();
                _this.clicked = false;
            }
            if (data['status'] === 'Error') {
                _this.modalRef.hide();
                _this.openModal(uploaderror);
                _this.errormessage = data['errorMessage'];
                _this.fileSelected = '';
                _this.clicked = false;
                _this.frmData = new FormData();
            }
            _this.spinnerService.hide();
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.uploadFiless = function (e) {
        this.fileSelected = e.target.files[0];
        this.clicked = false;
        this.frmData.append("file", this.fileSelected, this.fileSelected.name);
    };
    AdminComponent.prototype.onCreditEmployeeChange = function (selectedName, empId) {
        $('#emp-name-search').show();
        this.isvalidFreeze = false;
        this.cancelCredit = false;
        var temp = [];
        this.leavesummaryList = [];
        this.showSummary = false;
        this.currentYearFilterArr, this.currentYearFilterArr2 = [];
        this.dateJoining = false;
    };
    AdminComponent.prototype.leaveTypecredit = function (id) {
        var _this = this;
        var url = __WEBPACK_IMPORTED_MODULE_2__utility_constant__["a" /* CONSTANT */].GET_LEAVE_TYPE;
        this.locationWiseId = Number(id);
        var data = {
            "employeeNumber": this.service.employeeNumber,
            "locationId": id
        };
        this.service.postService(url, data)
            .subscribe(function (data) {
            _this.leaveCreditType = data.leaveType;
        }, function (error) {
            console.log(error);
            if (error.status == 0) {
                window.location.reload();
            }
        });
    };
    AdminComponent.prototype.leavecreditchange = function (event) {
        this.locationLeaveId = Number(event);
        this.validleave = false;
        if (this.locationselected != 'Select Location' && this.leaveselected != 'Select Leave') {
            this.daysDisable = false;
        }
    };
    AdminComponent.prototype.leavechangeevent = function (event) {
        this.leaveTypecredit(event);
        this.validlocation = false;
        if (this.locationselected != 'Select Location' && this.leaveselected != 'Select Leave') {
            this.daysDisable = false;
        }
    };
    AdminComponent.prototype.nofdayschangeevent = function (event) {
